@CAN_DEFECATION, CHECK_TARGET
#FUNCTION
#DIM CHECK_TARGET
#DIM CONST BORDER_OF_DEFECATE = 70
#DIM CONST BORDER_OF_FECES = 70

SIF BASE:CHECK_TARGET:便意 / (MAXBASE:CHECK_TARGET:便意 / 100) >= BORDER_OF_DEFECATE
	RETURNF 1

SIF BASE:CHECK_TARGET:便量 / (MAXBASE:CHECK_TARGET:便量 / 100) >= BORDER_OF_FECES
	RETURNF 1

RETURNF 0

