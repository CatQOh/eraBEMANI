			DELCHARA ASSI
			ASSI = LASSI
		ENDIF
		IF 対象生成
			DELCHARA TARGET
			TARGET = LTARGET
		ENDIF
		CSTR:PLAYER:ドラゴン = %LDRAGON%
		RETURN
	CASE -7
		ページ数--
	CASE -9
		ページ数++
	CASEELSE
		SIF TRAINNAME:LRESULT == ""
			CONTINUE
		コマンド実行条件 = 
		CALLFORMF USERCOM_ABLE_%TRAINNAME:LRESULT%
		DRAWLINE
		PRINTFORM □%TRAINNAME:LRESULT%
		LLANG = %LANGUAGE%
		LANGUAGE = ENG
		PRINTFORML (%I18N(TRAINNAME:LRESULT, "TRAIN")%)
		LANGUAGE = %LLANG%
		PRINTFORML 　%TRAIN_COMMENT(TRAINNAME:LRESULT)%
		PRINTL 
		PRINTFORML □実行条件

		SPLIT コマンド実行条件, "/", VAR_実行条件
		IF RESULT == 0
			PRINTL 　無条件で実行可能
		ELSE
			FOR LCOUNT, 0, RESULT-1
				PRINTFORML 　%VAR_実行条件:LCOUNT%
			NEXT
		ENDIF
		DRAWLINE
		WAIT
ENDSELECT

