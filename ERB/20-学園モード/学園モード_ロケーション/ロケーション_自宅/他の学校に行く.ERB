@他の学校に行く
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC LRESULT

FOR LCOUNT, 0, VARSIZE("学校名")
	IF 侵入可能:LCOUNT || 小中高(学校名:LCOUNT) == "大"
		SIF 学校名:LCOUNT != CSTR:PLAYER:在校
			PRINTFORML [{LCOUNT, 2}] - %学校名:LCOUNT%
	ENDIF
NEXT

DRAWLINE
PRINTPLAIN 　他の学校に行く　
PRINTL [10000] 戻る

DO
	BINPUT
	LRESULT = RESULT
	SELECTCASE LRESULT
		CASE 0 TO VARSIZE("学校名")
			CALL 学校へ行く, LRESULT
			RETURN 0
		CASE 10000
			RETURN 0
	ENDSELECT
LOOP 1
