@STATUS, ロストキャラ表示
#DIM DYNAMIC LCOUNT
#DIMS DYNAMIC 相性表示用
#DIM DYNAMIC 表示数
#DIM DYNAMIC 前のキャラ = -1
#DIM DYNAMIC 次のキャラ = -1
#DIM DYNAMIC ロストキャラ表示
#DIM DYNAMIC 相性補正後
#DIM DYNAMIC 種族嫌悪例外処理
#DIM マニュアル表示
DRAWLINE

CALL 基本情報

IF TARGET != MASTER
	DRAWLINE
	PRINTFORML □ %I18N("珠")% 
	CALL SHOW_INFO_JUEL, TARGET
ENDIF
CALL SHOW_INFO, TARGET

PRINTPLAINFORM □ %I18N("相性")% 
PRINTFORML [100] \@ OPTION:相性非表示 ? 開く # 閉じる \@
IF !OPTION:相性非表示
	SIF TALENT:服従
		PRINTFORML 　%I18N("全キャラ")%×120\% 
	SIF TALENT:隷属
		PRINTFORML 　%I18N("全キャラ")%×150\% 

	SIF TALENT:犬嫌い
		PRINTFORML 　%I18N("素質:|0|との相性", , I18N("犬"))%-30\%
	SIF TALENT:猫嫌い
		PRINTFORML 　%I18N("素質:|0|との相性", , I18N("猫"))%-30\%
	SIF CSTR:種族嫌悪 != ""
		PRINTFORML 　種族:%CSTR:種族嫌悪%との相性-30\%

	表示数 = 0
	FOR LCOUNT, 0, 20000
		;CSVがあるか、子供の場合に参照
		IF EXISTCSV(LCOUNT) || LCOUNT > 10000
			;子供・クローンの人数を突っ切ったら終了
			SIF LCOUNT > 10000 && CHILDNAME:(LCOUNT-10000) == ""
				BREAK
			;子供・クローンが何らかの理由で消えてたら相性も消す
			[SKIPSTART]
			IF LCOUNT > 10000 && !GETCHARA(LCOUNT)
				RELATION:LCOUNT = 100
				CONTINUE
			ENDIF
			[SKIPEND]
			相性表示用 = 
			相性補正後 = 0
			IF RELATION:LCOUNT || TALENT:犬嫌い || TALENT:猫嫌い || CSTR:種族嫌悪 != ""
				IF TALENT:隷属
					相性補正後 = 150
				ELSEIF TALENT:服従
					相性補正後 = 120
				ELSE
					相性補正後 = RELATION:LCOUNT == 0 ? 100 # RELATION:LCOUNT
				ENDIF
				IF TALENT:犬嫌い
					IF GETCHARA(LCOUNT) >= 0 && TALENT:GETCHARA(LCOUNT):犬 && !種族嫌悪例外処理(CHARANAMEF(TARGET), CHARANAMEF(LCOUNT))
						相性補正後 = MAX(相性補正後-30, 1)
					ELSEIF EXISTCSV(LCOUNT) && CSVTALENT(LCOUNT, GETNUM(TALENT, "犬")) && !種族嫌悪例外処理(CHARANAMEF(TARGET), CSVNAMEF(LCOUNT))
						相性補正後 = MAX(相性補正後-30, 1)
					ENDIF
				ENDIF
				IF TALENT:猫嫌い
					IF GETCHARA(LCOUNT) >= 0 && TALENT:GETCHARA(LCOUNT):猫 && !種族嫌悪例外処理(CHARANAMEF(TARGET), CHARANAMEF(LCOUNT))
						相性補正後 = MAX(相性補正後-30, 1)
					ELSEIF EXISTCSV(LCOUNT) && CSVTALENT(LCOUNT, GETNUM(TALENT, "猫")) && !種族嫌悪例外処理(CHARANAMEF(TARGET), CSVNAMEF(LCOUNT))
						相性補正後 = MAX(相性補正後-30, 1)
					ENDIF
				ENDIF
				IF CSTR:種族嫌悪 != ""
					IF GETCHARA(LCOUNT) >= 0 && ISHATERACE(TARGET, GETCHARA(LCOUNT)) && !種族嫌悪例外処理(CHARANAMEF(TARGET), CHARANAMEF(LCOUNT))
						相性補正後 = MAX(相性補正後-30, 1)
						SIF ORIGIN && CSTR:GETCHARA(LCOUNT):種族 == "人間" && 相性補正後 == 70
							CONTINUE
					ELSEIF EXISTCSV(LCOUNT) && 特定種族嫌悪(TARGET, CSVCSTR(LCOUNT, GETNUM(CSTR, "種族"))) && !種族嫌悪例外処理(CHARANAMEF(TARGET), CSVNAMEF(LCOUNT))
						相性補正後 = MAX(相性補正後-30, 1)
						;人間嫌い70%を全部表示するとえらいこっちゃなので除外
						SIF CSVCSTR(LCOUNT, GETNUM(CSTR, "種族")) == "人間" && 相性補正後 == 70
							CONTINUE
					ENDIF
				ENDIF
				SIF (相性補正後 == 100) || (TALENT:服従 && 相性補正後 == 120) || (TALENT:隷属 && 相性補正後 == 150)
					CONTINUE
				IF 相性補正後 > 100
					SETCOLORBYNAME coral
				ELSE
					SETCOLORBYNAME lightskyblue
				ENDIF
				IF NOF(MASTER) == LCOUNT
					相性表示用 = 　%NAMEDISP(MASTER)%×{相性補正後}\%
				ELSE
					相性表示用 = 　%CSVNAMEDISP(LCOUNT)%×{相性補正後}\%
				ENDIF
			ENDIF
			IF 相性表示用 != ""
				PRINTFORMLC %相性表示用%
				表示数++
			ENDIF
			RESETCOLOR
			IF 表示数 == 4
				CALL NEWLINE
				表示数 = 0
			ENDIF
		ENDIF
	NEXT
ENDIF
CALL NEWLINE

DRAWLINE
IF ORIGIN || マニュアル表示
	PRINTFORML □%I18N("キャラマニュアル")%
	FOR LCOUNT, 0, 100
		SIF CSTR:LCOUNT == ""
			BREAK
		CALL MANUALPRINT, @"　%CSTR:LCOUNT%"
		CALL NEWLINE
	NEXT
	PRINTFORML 　登場作品：「%CSTR:登場作品%」\@ CSTR:登場作品２ != "" ? 「%CSTR:登場作品２%」 #\@\@ CSTR:登場作品３ != "" ? 「%CSTR:登場作品３%」 #\@
ENDIF
IF CFLAG:キャラロスト
	CALL CHECKDRAWLINE
	PRINTFORML □%I18N("キャラロストの原因")%
	CALL PASELIL, @"　%CSTR:ロスト詳細%"
ENDIF


CALL CHECKDRAWLINE
PRINTPLAIN 　BASIC　

FOR LCOUNT, TARGET+1, CHARANUM
	IF ロストキャラ表示
		IF CFLAG:LCOUNT:キャラロスト
			次のキャラ = LCOUNT
			BREAK
		ENDIF
	ELSE
		IF !CFLAG:LCOUNT:キャラロスト
			次のキャラ = LCOUNT
			BREAK
		ENDIF
	ENDIF
NEXT
FOR LCOUNT, TARGET-1, -1, -1
	IF ロストキャラ表示
		IF CFLAG:LCOUNT:キャラロスト
			前のキャラ = LCOUNT
			BREAK
		ENDIF
	ELSE
		IF !CFLAG:LCOUNT:キャラロスト
			前のキャラ = LCOUNT
			BREAK
		ENDIF
	ENDIF
NEXT

PRINTFORM [0] %I18N("戻る")% 
SIF MOD:バスト関連ステータス表示 || TALENT:MASTER:孔雀の瞳 || (ASSI > 0 && TALENT:ASSI:孔雀の瞳)
	PRINTFORM [4] %I18N("性経験履歴")% 
PRINTFORM [5] %I18N("その他情報")% 
CALL PRINTSPACE, 前のキャラ >= 0, @"[6] %I18N("前のキャラ")% "
CALL PRINTSPACE, 次のキャラ >= 0, @"[7] %I18N("次のキャラ")% "
;PRINTPLAINFORM %SPACESLENS("　BASIC　")%

{
IF !ロストキャラ表示 && (調教可能(TARGET) || 助手可能(TARGET))
 && (!SHOP_配属キャラ選択不可 || (SHOP_配属キャラ選択不可 && CSTR:配属 == ""))
 && !CHECK_CHILD_CARE(TARGET) && !CFLAG:使用不可 && !CFLAG:監禁
}
	CALL PRINTSPACE, SHOW_TARGET != TARGET && 調教可能(TARGET), @"[8] %I18N("対象に指定")% "
	CALL PRINTSPACE, NOSAMES(TARGET, MASTER, ASSI) && 助手可能(TARGET), @"[9] %I18N("助手に指定")% "
	PRINTFORM [10] %I18N("アイテム使用")%
ENDIF
PRINTL 
PRINTPLAINFORM %SPACESLENS("　BASIC　")%

IF !ロストキャラ表示
	IF ISSLIME(TARGET)
		PRINTFORM [11] %I18N("体色変更"), 10, LEFT% 
	ELSE
		PRINTFORM [11] %I18N("タグ色変更"), 10, LEFT% 
	ENDIF
ENDIF

PRINTFORM [12] %I18N("キャラマニュアル表示/非表示")% 

SIF MOD:バスト関連ステータス表示
	PRINTFORM [97] %I18N("バストサイズをメモする")% 
PRINTFORM [98] %I18N("メモを書く")%
PRINTL 

BINPUT
SELECTCASE RESULT
	CASE 0
		RETURN 0
	CASE 4
		JUMP STATUS_SEX_EXP
	CASE 5
		;以下フラグリセットのためにJUMPで処理
		JUMP STATUS_ETC, ロストキャラ表示
	CASE 6
		TARGET = 前のキャラ
		JUMP STATUS, ロストキャラ表示
	CASE 7
		TARGET = 次のキャラ
		JUMP STATUS, ロストキャラ表示
	CASE 8
		CALL ASSIGN_TARGET, TARGET
		RETURN 1
	CASE 9
		CALL ASSIGN_ASSI, TARGET
		RETURN 0
	CASE 10
		CALL アイテム使用_逆引き, TARGET
		JUMP STATUS, ロストキャラ表示
	CASE 11
		IF ISSLIME(TARGET)
			PRINTFORML □%CALLNAME:TARGET%の体色を設定してください
		ELSE
			PRINTFORML □%CALLNAME:TARGET%のタグ色を設定してください
		ENDIF
		VARLINE = LINECOUNT
		CALL RGBBAR, CFLAG:タグ色/0x10000, CFLAG:タグ色%0x10000/0x100, CFLAG:タグ色%0x100, , TARGET
		SIF RESULT
			CFLAG:タグ色 = RESULT
		JUMP STATUS, ロストキャラ表示
	CASE 12
		INVERTBIT マニュアル表示, 0
		JUMP STATUS, ロストキャラ表示
	;50 称号の固定
	CASE 50
		IF 固定称号 == ""
			CALL 称号表示
			固定称号 = %HTML_TOPLAINTEXT(HTML_POPPRINTINGSTR())%
			PRINTFORMW 称号を「%固定称号%」で固定しました
		ELSE
			固定称号 = 
			PRINTW 称号の固定を解除しました
		ENDIF
		JUMP STATUS, ロストキャラ表示
	;97 バストサイズ詳細
	CASE 97
		CALL CHARA_BUST_SETTING, TARGET
		JUMP STATUS, ロストキャラ表示
	;98 メモ
	CASE 98
		PRINTL メモの内容を入力してください
		INPUTS
		メモ:TARGET:0 = %RESULTS%
		JUMP STATUS, ロストキャラ表示
	;99に服装変更のボタンがある
	CASE 99
		CALL CHARA_CLOTHES_SETTING
		JUMP STATUS, ロストキャラ表示
	CASE 100
		INVERTBIT OPTION:相性非表示, 0
		JUMP STATUS, ロストキャラ表示
ENDSELECT

@基本情報
#DIMS DYNAMIC 表示用変数

PRINTFORM □ %I18N("基本情報")% 
IF CFLAG:タグ色
	IF ISSLIME(TARGET)
		PRINTFORM %I18N("体色")%:
	ELSE
		PRINTFORM %I18N("タグ色")%:
	ENDIF
	SETCOLOR CFLAG:タグ色
	PRINT ■
	RESETCOLOR
ENDIF
PRINTL 
PRINTFORM 　%I18N("名前")%:%NAMEDISP(TARGET)% %I18N("性別")%:%性別表示(TARGET)% 

PRINTFORM %I18N("種族")%:
IF NOF(TARGET) == MASTER || NOF(TARGET) >= 10000 || CSTR:種族 == CSVCSTR(NOF(TARGET), GETNUM(CSTR, "種族"))
	IF TARGET == MASTER && CSTR:種族 != 主人初期種族 && 主人初期種族 != ""
		PRINTFORM %I18N("元|0|の|1|", , 主人初期種族, CSTR:種族)% 
	ELSE
