			曲一覧:LCOUNT = %SUBSTRING(曲一覧:LCOUNT, 1)%
		SIF 曲一覧:LCOUNT == ""
			BREAK
		;括弧2個分で+4
		表示文字数 += STRLENS(曲一覧:LCOUNT)+4
		IF 表示文字数 >= 表示可能文字数
			表示用行数++
			;括弧+全角スペース=6
			表示文字数 = STRLENS(曲一覧:LCOUNT)+6
		ENDIF
		表示用テキスト:表示用行数 += @"『%曲一覧:LCOUNT%』"
	NEXT
ENDIF
FOR LCOUNT, 0, 表示用行数+1
	IF カラム
		CALL MCOLUMNPRINTL, @"　%表示用テキスト:LCOUNT%", "", 1
	ELSE
		PRINTFORML 　%表示用テキスト:LCOUNT%
	ENDIF
NEXT
