;;;NOで渡す
@担当曲表示, 対象
#DIM DYNAMIC 対象
#DIMS DYNAMIC 曲一覧, 1000
;行頭に全角スペースを入れるため
#DIM DYNAMIC 表示文字数 = 2
#DIM CONST 表示可能文字数 = STRLENS(GETLINESTR("-"))
#DIM DYNAMIC LCOUNT

IF !EXISTCSV(対象) || CSVCSTR(対象, GETNUM(CSTR, "担当曲")) == ""
	PRINTL 　[無し]
	RETURN
ENDIF

SPLIT CSVCSTR(対象, GETNUM(CSTR, "担当曲")), "|", 曲一覧


PRINT 　
FOR LCOUNT, 0, 1000
	;行頭に改行相当のスペースが入ってるので一文字消す
	SIF CHARATU(曲一覧:LCOUNT, 0) == " "
		曲一覧:LCOUNT = %SUBSTRING(曲一覧:LCOUNT, 1)%
	SIF 曲一覧:LCOUNT == ""
		BREAK
	;括弧2個分で+4
	表示文字数 += STRLENS(曲一覧:LCOUNT)+4
	IF 表示文字数 >= 表示可能文字数
		PRINTL 
		PRINT 　
		;括弧+全角スペース=6
		表示文字数 = STRLENS(曲一覧:LCOUNT)+6
	ENDIF
	PRINTFORM 『%曲一覧:LCOUNT%』
NEXT
CALL NEWLINE
RETURN

	
