@売却額補正計算
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC 倍率

倍率 = 100
;素質による売却額補正値計算
IF TALENT:処女
	倍率 += 100
	PRINTL 処女 +100%
ENDIF
IF TALENT:再生処女
	倍率 += 50
	PRINTL 再生処女 +50%
ENDIF

;素質による増減
;陥落素質
IF TALENT:恋慕
	PRINTL 恋慕 -30%
	倍率 -= 30
ENDIF
IF TALENT:淫乱
	PRINTL 淫乱 +30%
	倍率 += 30
ENDIF
IF TALENT:服従
	PRINTL 服従 +100%
	倍率 += 100
ENDIF
IF TALENT:親愛
	PRINTL 親愛 -50%
	倍率 -= 50
ENDIF
IF TALENT:娼婦
	PRINTL 娼婦 +60%
	倍率 += 60
ENDIF
IF TALENT:隷属
	PRINTL 隷属 +200%
	倍率 += 200
ENDIF

IF SEX(TARGET) == 1
	PRINTL 男 +30%
	倍率 += 30
ENDIF

;その他素質
FOR LCOUNT, 0 ,VARSIZE("TALENT")
	SIF !TALENT:LCOUNT
		CONTINUE
	SELECTCASE TALENTNAME:LCOUNT
		;バストサイズ
		CASE "バストサイズ"
			SELECTCASE TALENT:バストサイズ
				;絶壁
				CASE 1
					PRINTL 絶壁 -20%
					倍率 -= 20
				;貧乳
				CASE 2
					PRINTL 貧乳 -10%
					倍率 -= 10
				;巨乳
				CASE 4
					PRINTL 巨乳 +20%
					倍率 += 20
				;豊乳
				CASE 5
					PRINTL 豊乳 +25%
					倍率 += 25
				;爆乳
				CASE IS >= 6
					PRINTL 爆乳 +30%
					倍率 += 30
			ENDSELECT
		CASE "体格"
			SELECTCASE TALENT:体格
				CASE 1
					PRINTL 幼児 +20%
					倍率 += 20
				CASE 2
					PRINTL 小柄 +10%
					倍率 += 10
			ENDSELECT
		;長いのは見づらいから数行に分ける
		;[素直]及び[解放]は[抵抗][抑圧]消滅時に付いたものならマイナス補正無効
		CASE "素直"
			IF !(NOF(TARGET) < 10000 && CSVTALENT(NOF(TARGET), GETNUM(TALENT, "抵抗")))
				PRINTL 素直 -20%
				倍率 -= 20
			ENDIF
		CASE "解放"
