;奴隷売却の処理
@CHARA_SELL, 金額取得
#DIM DYNAMIC LTARGET
#DIM DYNAMIC LASSI
#DIM DYNAMIC PRICE
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC 金額取得
#DIMS DYNAMIC 追加情報
#DIM DYNAMIC 倍率
#DIM DYNAMIC 口上補正あり

;金額取得の引数を渡されたらそのキャラの金額を取得する
IF 金額取得
	SKIPDISP 1
	LTARGET = TARGET
	TARGET = 金額取得
	GOTO 計算
ENDIF

DRAWLINE
PRINTL □誰を売りますか？
FOR PAGING, 0, CHARANUM
	追加情報 = 
	IF ASSI == PAGING
		追加情報 = (助手)
	;ELSEIF ISASSI:PAGING
	;	追加情報 = (元助手)
	ELSEIF CFLAG:PAGING:助手可能
		追加情報 = (助手可)
	ENDIF
	CALL PAGING, 売却可能(PAGING), @"%追加情報%", "SELL SLAVE"
NEXT

SELECTCASE RESULT
	CASE -1
		RETURN 0
	CASE 0 TO CHARANUM
		;売却処理
		LTARGET = TARGET
		LASSI = ASSI
		TARGET = RESULT
ENDSELECT

SIF TARGET == MASTER
	RESTART

$計算
CALL 売却額計算
PRICE = RESULT

SETCOLOR 100, 150, 250
PRINTFORML 能力価格 = {PRICE}
RESETCOLOR

PRINTL 

CALL 売却額補正計算
倍率 = RESULT

PRINTFORML 素質倍率 = {倍率}\%
;補正値はどんなに低くても最低30%
IF 倍率 < 30
	PRINTL 最低保証 30%
	倍率 = 30
ENDIF 
;補正値の適用
PRICE *= 倍率
PRICE /= 100

SETCOLOR 100, 150, 250
PRINTFORML 能力価格×素質補正 = {PRICE}
RESETCOLOR

PRINTL 
倍率 = 100
IF TALENT:崩壊
	PRINTL 崩壊 -20%
	倍率 -= 20
ENDIF
IF TALENT:妊娠
	PRINTL 妊娠 -30%
	倍率 -= 30
ENDIF
IF TALENT:狂気
	PRINTL 狂気 -30%
	倍率 -= 30
ENDIF
IF TALENT:即落ち
	PRINTL 即落ち -80%
	倍率 -= 80
ENDIF

IF CSTR:眷属主 != "" && CSTR:種族 != "ヴァンパイア"
	PRINTL 眷属化 -20%
	倍率 -= 20
ENDIF

IF CFLAG:眷属化した人数 && CSTR:種族 != "ヴァンパイア"
	PRINTFORML 眷属主 -{20*CFLAG:眷属化した人数}\%
	倍率 -= 20*CFLAG:眷属化した人数
ENDIF

IF CFLAG:生殖力強化 == 1
	PRINTL 不妊治療中 -10%
	倍率 -= 10
ENDIF

IF CFLAG:卵管結紮 == 1
	PRINTL 不妊手術済み -10%
	倍率 -= 10
ENDIF
FOR LCOUNT, GETNUM(CSTR, "右腕の印"), GETNUM(CSTR, "胴体の印")+1
	IF CSTR:LCOUNT != ""
		SELECTCASE CSTRNAME:LCOUNT
			CASE "右腕の印", "左腕の印", "右足の印", "左足の印"
				PRINTFORML %CSTRNAME:LCOUNT% -10\%
				倍率 -= 10
			CASE "背中の印"
				PRINTFORML %CSTRNAME:LCOUNT% -15\%
				倍率 -= 15
			CASE "胴体の印"
				PRINTFORML %CSTRNAME:LCOUNT% -20\%
				倍率 -= 20
		ENDSELECT
	ENDIF
NEXT

IF CFLAG:人工物 && CSTR:種族 != "機械"
	PRINTL 人体錬成によって作られた -50%
	倍率 -= 50
ENDIF

SIF 倍率 != 100
	PRINTFORML 特殊素質補正 = {倍率}\%

IF 倍率 < 10
	PRINTFORML 最低保証10\%
	倍率 = 10
ENDIF

PRICE *= 倍率
PRICE /= 100

IF 倍率 != 100
	SETCOLOR 100, 150, 250
	PRINTFORML 価格×特殊素質補正 = {PRICE}
	RESETCOLOR
ENDIF

;難易度処理
SELECTCASE FLAG:難易度
	CASE -1
		PRINTL 難易度:BEGINNER ×120%
		TIMES PRICE , 1.20
	CASE 1
		PRINTL 難易度:NORMAL ×100%
		TIMES PRICE , 1.00
	CASE 2
		PRINTL 難易度:HYPER ×80%
		TIMES PRICE , 0.80
	CASE 3
		PRINTL 難易度:ANOTHER ×60%
		TIMES PRICE , 0.60
	CASE 4
		PRINTL 難易度:LEGGENDARIA ×200%
		TIMES PRICE , 2.00
ENDSELECT
SETCOLOR 100, 150, 250
PRINTFORML 価格×難易度補正 = {PRICE}
RESETCOLOR
;価格×種族補正
SELECTCASE CSTR:種族
	;人間,人間(？),獣
	CASE "人間", "人間(？)", "獣"
		倍率 = 100
	;宇宙人,獣人,人魚,ヴァンパイア
	CASE "宇宙人", "獣人", "人魚", "ヴァンパイア"
		倍率 = 120
	;改造人間,機械,精霊,妖精
	CASE "改造人間", "機械", "精霊", "妖精"
		倍率 = 110
	;天使,鳳凰
	CASE "天使", "鳳凰"
		倍率 = 140
	;悪魔,魔術師
	CASE "悪魔", "魔術師"
		倍率 = 130
	;神
	CASE "神"
		倍率 = 150
	;ゾンビ
	CASE "ゾンビ"
		倍率 = 95
	;スライム
	CASE "スライム"
		倍率 = 90
	;表記ゆれがあった場合
	CASEELSE
		倍率 = 100
ENDSELECT

IF 倍率 != 100
	PRINTL 
	SETCOLOR 100, 150, 250
	PRINTFORML 価格×種族(%CSTR:種族%)補正:{倍率}\% = {PRICE*倍率/100}
	RESETCOLOR
	PRICE *= 倍率
	PRICE /= 100
ENDIF
PRINTL 

SETCOLOR 100, 150, 250
CALL PASELIL, @"価格:{PRICE}＄＋元の価格:{CFLAG:価格}＄ = {PRICE+CFLAG:価格}＄"
RESETCOLOR
PRICE += CFLAG:価格

;元助手
;IF ISASSI:TARGET == 1
;	PRINTFORML 助手経験あり -50\%
;	TIMES PRICE , 0.50
;ENDIF

IF 金額取得
	TARGET = LTARGET
	SKIPDISP 0
	RETURN PRICE
ENDIF

TRYCALLFORM 売却額補正_%CSVNAMERPS(NO:TARGET)%
IF RESULT
	PRICE = RESULT
	口上補正あり = 1
ENDIF

;売却確認
PRINTFORM %NAME:TARGET%は
SETCOLOR 50, 150, 250
CALL PASELI, @"{PRICE}＄"
RESETCOLOR
PRINTFORML で売れそうです\@ 口上補正あり ? (口上補正) #\@
PRINTL 売却しますか？
PRINTL [0] はい
PRINTL [1] いいえ
CFLAG:売却寸止め = 1
BINPUT

SELECTCASE RESULT
	CASE 0
		CFLAG:売却額 = PRICE
		;売却時のイベント口上
		CALL KOJO_MESSAGE_EVENT, "売却時"
		IF RESULT == 1
			TARGET = LTARGET
			RESTART
		ENDIF
		SELECTCASE 月齢
			CASE 2300 TO 2599
				SIF FALLTYPE(TARGET) == 1
					CALL 実績解除, "下弦の月に願うは君への罰"
		ENDSELECT
		IF NOF(TARGET) != 0
			SELECTCASE CHARANAMEF(TARGET)
				;実績「Σかリアンを素数価格で売却する」
				CASE "Σ", "リアン"
					SIF ISPRIME(PRICE)
						CALL 実績解除, "I only Divide Me"
				;実績「緋浮美を140,000パセリ以上で売却する」
				CASE "梅桐 緋浮美"
					SIF PRICE >= 140000
						CALL 実績解除, "GOLD RUSH"
				;実績:オトベアを売り飛ばす
				CASE "オトベア"
					CALL 実績解除, "お前よりくまモンの方がいい！"
			ENDSELECT
		ENDIF
		バンめし = 0
		IF !CSVTALENT(NOF(MASTER), GETNUM(TALENT, "子供好き"))
			IF TALENT:体格 <= 2
				FLAG:小児性愛スコア += 3
				IF FLAG:小児性愛スコア >= 10 && !TALENT:MASTER:子供好き
					PRINTFORMW 度重なる児童人身売買で、%CALLNAME:MASTER%は子供に対して異常な性的嗜好を持つようになった
					PRINTFORMW %CALLNAME:MASTER%は[子供好き]を得た
					TALENT:MASTER:子供好き = 1
				ENDIF
				CALL SETMIN(FLAG:小児性愛スコア, 20)
			ELSE
				FLAG:小児性愛スコア -= 2
				IF FLAG:小児性愛スコア < 10 && TALENT:MASTER:子供好き
					PRINTFORMW %CALLNAME:MASTER%はしばらく児童の人身売買から離れていたせいか、子供に対する執着が薄れたようだ
					PRINTFORMW %CALLNAME:MASTER%は[子供好き]を失った
					TALENT:MASTER:子供好き = 0
				ENDIF
				CALL SETMAX(FLAG:小児性愛スコア, -20)
			ENDIF
		ENDIF
		CALL PAYMENT, PRICE, @"%NAME:TARGET%売却"
		CALL プレイログ記録, @"%NAME:TARGET%売却"
		CSTR:ロスト要因 = 売却
		CSTR:ロスト詳細 = {PRICE}＄の価格で売却された
		CALL CLEAR_CHARA, TARGET
	CASE 1
		TARGET = LTARGET
		RESTART
ENDSELECT

@CLEAR_CHARA, ARG, ARG:1, 死, 加工
#DIM DYNAMIC LTARGET
#DIM DYNAMIC LASSI
#DIM DYNAMIC 死
#DIM DYNAMIC 加工

;アイドルユニットに属していた場合、そのユニットは解散
SIF CFLAG:ARG:ユニット番号
	CALL BREAKUP, CFLAG:ARG:ユニット番号
;監禁室を外す
SIF CFLAG:ARG:監禁
	監禁室使用状況:(CFLAG:ARG:監禁) = 

CFLAG:ARG:居住 = 0
IF !ARG:1
	CALL 配属解除, ARG, 1
	CALL CLEAR_FLAG, ARG
ENDIF

IF ARG:1
	DELCHARA ARG
ELSE
	CFLAG:ARG:キャラロスト = 1
ENDIF
IF ARG:1
	LTARGET = TARGET
	LASSI = ASSI
	SIF ARG == LTARGET
		TARGET = -1
	SIF ARG == LASSI
		ASSI = -1
	SIF ARG < LTARGET
		TARGET--
	SIF ARG < LASSI
		ASSI--
	SIF !引き継ぎ処理中
		CALLF メモリ解放, "キャラ消去"
ELSE
	SIF TARGET == ARG
		TARGET = -1
	SIF ASSI == ARG
		ASSI = -1
ENDIF

IF 死
	SIF GROUPMATCH(TFLAG:捕食相手, 父親取得(ARG), 母親取得(ARG))
		RETURN
	IF CHARANAMEF(ARG) == "ハート" && TRANCERACE:ARG:0 == ""
		;（主人死亡が面倒なので）主人マッドラットでハートが死んだ時何も起きない不具合が潜在的にあるはず 後で
		;マッドラットが先に死んだ際の処理をどうするかはアイデアがない
		IF !CCB:GETNO("マッドラット")
			PRINTFORML ハートの死により、その心臓の持ち主もどこかで死亡したようだ
			CCB:GETNO("マッドラット") = 1
		ELSEIF GETCHARANAME("マッドラット") > 0
			PRINTFORML マッドラットは心臓が停止し、死亡しました
			CSTR:GETCHARANAME("マッドラット"):ロスト要因 = 死亡
			CSTR:GETCHARANAME("マッドラット"):ロスト詳細 = 心臓が停止した
			CALL CLEAR_CHARA, GETCHARANAME("マッドラット"), 0, 1
		ENDIF
	ENDIF
	IF (GETCHARA(父親取得(ARG)) > 0 && !加工 && CSTR:GETCHARA(父親取得(ARG)):配属 != "博物館") || (GETCHARA(母親取得(ARG)) > 0 && !加工 && CSTR:GETCHARA(母親取得(ARG)):配属 != "博物館")
		;博物館で加工した場合はそっちで処理済みなので計算しない
		SIF GETCHARA(父親取得(ARG)) > 0 && !加工 && CSTR:GETCHARA(父親取得(ARG)):配属 != "博物館"
			PRINTFORM %CALLNAME:GETCHARA(父親取得(ARG))%
		SIF GETCHARA(父親取得(ARG)) > 0 && !加工 && CSTR:GETCHARA(父親取得(ARG)):配属 != "博物館" && GETCHARA(母親取得(ARG)) > 0 && !加工 && CSTR:GETCHARA(母親取得(ARG)):配属 != "博物館"
			PRINT と
		SIF GETCHARA(母親取得(ARG)) > 0 && !加工 && CSTR:GETCHARA(母親取得(ARG)):配属 != "博物館"
			PRINTFORM %CALLNAME:GETCHARA(母親取得(ARG))%
		PRINTFORML に%CALLNAME:ARG%の死を伝えますか？
		PRINTL [0] - はい
		PRINTL [1] - いいえ
		BINPUT
		IF RESULT == 0
			死亡キャラ = ARG
			SIF GETCHARA(父親取得(ARG)) > 0 && !加工 && CSTR:GETCHARA(父親取得(ARG)):配属 != "博物館"
				CALL 崩壊処理, GETCHARA(父親取得(ARG)), "子供の死"
			SIF GETCHARA(母親取得(ARG)) > 0 && !加工 && CSTR:GETCHARA(母親取得(ARG)):配属 != "博物館"
				CALL 崩壊処理, GETCHARA(母親取得(ARG)), "子供の死"
		ENDIF
	ENDIF
ENDIF

