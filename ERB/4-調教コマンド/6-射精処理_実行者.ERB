;---------------------------------------------------------
;射精処理
;---------------------------------------------------------
@SAMEN_SHOOT, 射精先, 射精量, 射精したキャラ = -1, 射精されるキャラ = -1
#DIM DYNAMIC 射精先
#DIM DYNAMIC 射精量
#DIM DYNAMIC 射精したキャラ
#DIM DYNAMIC 射精されるキャラ
#DIM DYNAMIC 計算用射精量
#DIM DYNAMIC 計算用精液量
#DIM DYNAMIC 増加量
#DIM DYNAMIC 魔力回復量, 2

計算用射精量 = LIMIT(射精量, 1, 10)
SIF 射精したキャラ == -1
	射精したキャラ = PLAYER
SIF 射精されるキャラ == -1
	射精されるキャラ = TARGET
SIF 射精量 <= 0
	RETURN 0

;射精先変数内訳
;1 = 膣内射精
;2 = アナル射精
;3 = 手淫
;4 = フェラ
;5 = パイズリ
;6 = その他奉仕
;7 = 助手を犯すで射精
;8 = クンニされて射精
;---以下は調教対象の射精なので管轄外---
;;9 = 助手を犯させるで射精
;;10 = 逆レイプで射精
;--------------------------------------
;11 = 3P助手射精
;12 = 臍中出し
;13 = 乳管中出し

;コマンドごとの分岐
SELECTCASE 射精先
;通常セックス系
	CASE 1
		;主人による奴隷への中出し
		IF 射精したキャラ == MASTER
			IF !TEQUIP:MASTER:コンドーム
				SETBIT TFLAG:中出し, 0
				PRINTFORML 膣内射精経験+{計算用射精量}
				EXP:膣射経験 += 計算用射精量
				;計算用精液量は子宮が受け入れる精液の量
				計算用精液量 = 1000*計算用射精量
				;排卵日の前7日と後14日は子宮が降りてきて精液を受け入れやすくなる
				IF MOD:排卵周期
					SELECTCASE 排卵からの経過時間:TARGET:0
						CASE 1 TO 140
							TIMES 計算用精液量, 1.50
						CASE 141
							TIMES 計算用精液量, 1.45
						CASE 142
							TIMES 計算用精液量, 1.40
						CASE 143
							TIMES 計算用精液量, 1.35
						CASE 144
							TIMES 計算用精液量, 1.30
						CASE 145
							TIMES 計算用精液量, 1.25
						CASE 146
							TIMES 計算用精液量, 1.20
						CASE 147
							TIMES 計算用精液量, 1.15
						CASE 148
							TIMES 計算用精液量, 1.10
						CASE 149
							TIMES 計算用精液量, 1.05
						CASE 150 TO 210
							TIMES 計算用精液量, 1.00
						CASE 211
							TIMES 計算用精液量, 1.05
						CASE 212
							TIMES 計算用精液量, 1.10
						CASE 213
							TIMES 計算用精液量, 1.15
						CASE 214
							TIMES 計算用精液量, 1.20
						CASE 215
							TIMES 計算用精液量, 1.25
						CASE 216
							TIMES 計算用精液量, 1.30
						CASE 217
							TIMES 計算用精液量, 1.35
						CASE 218
							TIMES 計算用精液量, 1.40
						CASE 219
							TIMES 計算用精液量, 1.45
						CASE 220 TO 280
							TIMES 計算用精液量, 1.50
					ENDSELECT
				ENDIF
				;快Ｖの増加量が大きいほど、膣奥が収縮して精液を吸い上げてくる
				計算用精液量 *= (100000+UP:快Ｖ)
				計算用精液量 /= 100000
				PREG:主人膣内射精 += 計算用精液量
				CFLAG:主人からの膣射経験++
				TFLAG:主人から奴隷への膣内射精回数++
				中出し回数:MASTER:(NOF(TARGET))++
				中出され回数:(NOF(MASTER))++
				;妊娠していないのであれば中出し時の恐怖判定等
				SIF !TALENT:妊娠
					CALL SAMEN_SHOOT_SOURCE, 射精したキャラ
				;ビデオ撮影中はボーナスカウント
				SIF TEQUIP:ビデオカメラ
					VIDEOBONUS:主人中出し++
				;獣人、獣による催淫
				SELECTCASE CSTR:射精したキャラ:種族
					CASE "獣人", "獣"
						CFLAG:射精されるキャラ:残数 += ((CFLAG:射精されるキャラ:媚薬%5)+計算用射精量)/5
						CFLAG:射精されるキャラ:媚薬 += 計算用射精量
				ENDSELECT
				;出産トラウマがあればストレスが溜まる サキュバスで無効
				SIF CFLAG:出産トラウマ && !TALENT:サキュバス
					CFLAG:ストレス値 += CFLAG:出産トラウマ
				SIF CFLAG:ストレス値 > 999
					CFLAG:ストレス値 = 999
			ENDIF
		;助手による奴隷への中出し
		ELSEIF 射精したキャラ == ASSI
			IF !TEQUIP:ASSI:コンドーム
				SETBIT TFLAG:中出し, 0
				PRINTFORML 膣内射精経験+{計算用射精量}
				EXP:TARGET:膣射経験 += 計算用射精量
				;計算用精液量は子宮が受け入れる精液の量
				計算用精液量 = 1000*計算用射精量
				;排卵日の前7日と後14日は子宮が降りてきて精液を受け入れやすくなる
				IF MOD:排卵周期
					SELECTCASE 排卵からの経過時間:TARGET:0
						CASE 1 TO 140
							TIMES 計算用精液量, 1.50
						CASE 141
							TIMES 計算用精液量, 1.45
						CASE 142
							TIMES 計算用精液量, 1.40
						CASE 143
							TIMES 計算用精液量, 1.35
						CASE 144
							TIMES 計算用精液量, 1.30
						CASE 145
							TIMES 計算用精液量, 1.25
						CASE 146
							TIMES 計算用精液量, 1.20
						CASE 147
							TIMES 計算用精液量, 1.15
