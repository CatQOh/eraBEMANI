;ビデオの重複使用を処理するために、予め値段の加算を行っておく
@ビデオポイント, コマンド名
#FUNCTION
#DIMS DYNAMIC コマンド名
#DIM DYNAMIC ポイント

SELECTCASE コマンド名
	;愛撫系
	CASE "愛撫", "アナル愛撫", "胸愛撫", "首絞め愛撫"
		ポイント += 30
	CASE "クンニ", "指挿入れ", "アナル舐め", "胸揉み", "乳首責め", "乳首舐め", "臍愛撫"
		ポイント += 60
	CASE "フェラする", "射精寸止め", "お掃除フェラする"
		ポイント += 90
	CASE "秘貝開帳", "秘貝開帳見せつけ", "自慰見せつけ"
		ポイント += 150
	CASE "胸自慰", "足扱きする", "下着扱きする", "手淫する", "素股する"
		ポイント += 120
	CASE "自慰", "パイズリする", "パイズリフェラする"
		ポイント += 200
	;コミュニケーション系
	CASE "キスする", "吸血する", "吸血させる", "キスしてもらう", "ふたなり化"
		ポイント += 100
	CASE "何もしない", "プレゼント", "まったりする", "家事", "機械の勉強", "スキンシップ", "尻尾を撫でる"
		ポイント += 10
	CASE "会話", "歌の練習", "料理を作る", "お菓子を作る", "楽器の練習", "ハッキング", "排卵マッサージ", "魔法詠唱", "プログラミング演習", "魔法の練習"
		ポイント += 50
	CASE "罵倒", "ビデオ鑑賞", "お酒を飲ませる", "情欲を煽る", "ジュースを飲ませる", "バストサイズ変更"
		ポイント += 20
	;道具系
	CASE "ローター", "貞操帯"
		ポイント += 90
	CASE "電気按摩", "クリキャップ", "ニプルキャップ", "搾乳器"
		ポイント += 120
	CASE "オナホール", "バイブ", "アナルバイブ", "アナルビーズ", "電動エネマグラ", "搾精器"
		ポイント += 150
	CASE "ニプルキャップ"
		ポイント += 120
	CASE "乳首ローター", "ペニスバンド"
		ポイント += 60
	CASE "搾精コンドーム", "コンドーム", "ミッドナイトスペシャル"
		ポイント += 30
	;セックス系
	CASE "正常位", "対面座位", "背面座位", "正常位アナル", "対面座位アナル", "背面座位アナル", "正常位させる", "後背位させる", "正常位アナルさせる", "後背位アナルさせる"
		ポイント += 200
	CASE "後背位", "後背位アナル", "下着ずらし挿入", "下着ずらし挿入アナル"
		ポイント += 250
	CASE "騎乗位", "騎乗位アナル", "スライム姦", "スライム姦させる", "臍挿入", "乳管挿入"
		ポイント += 300
	CASE "逆レイプ", "逆レイプキス"
		ポイント += 150
	;奉仕系
	CASE "手淫", "泡踊り", "足扱き", "兜合わせ", "下着扱き"
		ポイント += 150
	CASE "フェラチオ"
		ポイント += 180
	CASE "パイズリ", "オナホ扱き", "バイブ被挿入", "アナルバイブ被挿入"
		ポイント += 200
	CASE "素股", "ローター愛撫させる", "電気按摩愛撫させる", "アナル愛撫させる"
		ポイント += 100
	CASE "足を舐めさせる"
		ポイント += 60
	CASE "縦パイズリ", "パイズリフェラ"
		ポイント += 250
	CASE "母乳飲み"
		ポイント += 90
	;SM系
	CASE "恐怖を煽る"
		ポイント += 30
	CASE "スパンキング", "乳スパンキング", "アイマスク", "ボールギャグ", "ビンタ", "異物挿入解除"
		ポイント += 100
	CASE "鞭", "蝋燭", "縄", "腹パン", "踏む", "裸締め", "鞭打ちしてもらう", "踏んでもらう", "叩いてもらう", "尿道挿入", "根性焼き"
		ポイント += 150
	CASE "針", "首を絞める", "異物挿入", "採血", "臍フィスト", "尿道針挿入", "臍針挿入", "乳管針挿入"
		ポイント += 200
	;助手系、レズ系
	CASE "クンニさせる"
		ポイント += 100
	CASE "貝あわせ", "ダブルフェラ"
		ポイント += 150
	CASE "助手を犯させる", "ダブルパイズリ", "双頭ディルド", "アナル双頭ディルド"
		ポイント += 200
	CASE "顔面騎乗する"
		ポイント += 120
	CASE "乳の揉み合い"
		ポイント += 90
	;ハードなコマンド
	CASE "イラマチオ", "浣腸器", "アナル電極", "乳房電極", "飲尿する", "飲尿させる"
		ポイント += 150
	CASE "フィストファック", "アナルフィスト", "乳ファック", "フィストファックさせる", "アナルフィストさせる", "体内放尿する", "体内放尿させる"
		ポイント += 200
	CASE "両穴フィスト"
		ポイント += 250
	CASE "放尿", "空気浣腸", "拡張バルーン"
		ポイント += 100
	CASE "アナルを舐めさせる"
		ポイント += 60
	;触手系
	CASE "触手召喚", "触手搾乳", "触手緊縛", "触手浣腸", "触手ペニス責め", "触手搾精"
		ポイント += 150
	CASE "触手挿入", "アナル触手", "触手強制搾精", "触手強制放尿", "触手臍挿入", "触手乳管挿入"
		ポイント += 200
	CASE "触手クリ責め", "触手乳首責め", "触手口辱", "触手射精止め"
		ポイント += 100
	;その他
	CASE "ローション", "写真撮影", "強精神薬", "尿道カテーテル", "粘液ぶっかけ", "尿道ポッキー", "ライブ配信", "蜂蜜ローション", "スライム尿道責め", "コナステで遊ぶ"
		ポイント += 30
	CASE "媚薬", "利尿剤", "シャワー", "ローション注入", "腸洗浄", "魅惑の粘液"
		ポイント += 60
	CASE "野外プレイ", "羞恥プレイ", "お風呂場プレイ", "新妻プレイ", "コンドーム精飲（奴隷）", "コンドーム精飲（助手）", "女装させる", "裸体盛り", "蘇生薬投与"
		ポイント += 100
	CASE "助手を犯す"
		ポイント += 150
	;龍姦系
	CASE "キス（ドラゴン）"
		ポイント += 150
	CASE "フェラさせる（ドラゴン）"
		ポイント += 200
	CASE "舐め回す（ドラゴン）", "ペットに舐めさせる"
		ポイント += 100
	CASE "睨みつける（ドラゴン）"
		ポイント += 50
	CASE "膣を犯す（ドラゴン）", "アナルを犯す（ドラゴン）", "ペットに犯させる"
		ポイント += 300
	;派生コマンド
	CASE "3P", "二本フェラ", "数珠繋ぎ"
		ポイント += 300
	CASE "シックスナイン", "岩清水", "搾乳"
		ポイント += 150
	CASE "Gスポット刺激", "挿入Gスポ責め", "挿入子宮口責め", "逆レイプ首絞め", "授乳手コキ", "前立腺刺激"
		ポイント += 200
	CASE "乱れ牡丹", "69パイズリ", "ダブル素股", "正常位キス", "対面座位キス", "正常位アナルキス", "対面座位アナルキス"
		ポイント += 250
	CASE "騎乗乳房自慰", "騎乗位キス", "騎乗位アナルキス", "正常位キスさせる"
		ポイント += 350
	CASE "正常位首絞め", "後背位首絞め", "正常位アナル首絞め", "後背位アナル首絞め"
		ポイント += 250
	CASE "バイブオナニー", "アナルバイブオナニー", "シャワーオナニー", "PC筋エネマグラオナニー"
		ポイント += 250
	CASE "二穴バイブオナニー", "オナフェラ", "二輪挿し", "アナル二輪挿し"
		ポイント += 300
	CASE "後背位手淫", "騎乗位手淫", "後背位アナル手淫", "騎乗位アナル手淫", "対面座位吸血", "背面座位吸血", "正常位吸血させる", "後背位吸血させる"
		ポイント += 250
	CASEELSE
		;#; DEBUGPRINTFORML %コマンド名%にビデオ価格が設定されていません
ENDSELECT

RETURNF ポイント

@PRICE_VIDEO
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC 価格
#DIM DYNAMIC 補正

FOR LCOUNT, 0, TFLAG:ビデオ撮影回数
	価格 += ビデオポイント(ビデオ撮影内容:LCOUNT)
NEXT

補正 = 100
SIF TALENT:無関心
	補正 -= 30
SIF TALENT:感情乏しい
	補正 -= 30
SIF TALENT:恥じらい
	補正 += 10

;ロリ巨乳・トランジスタグラマーだとさらにボーナスが付く
IF TALENT:体格 == 1 || TALENT:小人体型
	SELECTCASE TALENT:バストサイズ
		;絶壁
		CASE 1
			補正 += 25
		;貧乳
		CASE 2
			補正 += 30
		;普乳
		CASE 3
			補正 += 40
		;巨乳
		CASE 4
			補正 += 55
		;豊乳
		CASE 5
			補正 += 75
		;爆乳
		CASE IS >= 6
			補正 += 100
	ENDSELECT
ELSEIF TALENT:体格 == 2
	SELECTCASE TALENT:バストサイズ
		;絶壁
		CASE 1
			補正 += 15
		;貧乳
		CASE 2
			補正 += 18
		;普乳
		CASE 3
			補正 += 24
		;巨乳
		CASE 4
			補正 += 33
		;豊乳
		CASE 5
			補正 += 45
		;爆乳
		CASE IS >= 6
			補正 += 63
	ENDSELECT
ELSE
	SELECTCASE TALENT:バストサイズ
		;絶壁
		CASE 1
			補正 += 5
		;貧乳
		CASE 2
			補正 += 7
		;普乳
		CASE 3
			補正 += 11
		;巨乳
		CASE 4
			補正 += 17
		;豊乳
		CASE 5
			補正 += 25
		;爆乳
		CASE IS >= 6
			補正 += 35
	ENDSELECT
ENDIF

SIF TALENT:動物耳
	補正 += 5
SIF TALENT:尻尾
	補正 += 5
SIF TALENT:ファーリー
	補正 += 10
SIF TALENT:人気
	補正 += 20
SIF TALENT:謎の魅力
	補正 += 30
SIF TALENT:魅力
	補正 += 20

;被写経験に応じて売却額上昇
;淫乱・娼婦なら映像になることを意識するため売却額が伸びる
IF TALENT:淫乱 || TALENT:娼婦
	SELECTCASE EXP:被写経験
		CASE IS >= EXPLV:10
			補正 += 325
		CASE IS >= EXPLV:9
			補正 += 275
		CASE IS >= EXPLV:8
			補正 += 220
		CASE IS >= EXPLV:7
			補正 += 180
		CASE IS >= EXPLV:6
			補正 += 155
		CASE IS >= EXPLV:5
			補正 += 125
		CASE IS >= EXPLV:4
			補正 += 100
		CASE IS >= EXPLV:3
			補正 += 80
		CASE IS >= EXPLV:2
			補正 += 65
		CASE IS >= EXPLV:1
			補正 += 55
		CASEELSE
			補正 += 50
	ENDSELECT
	SIF TALENT:娼婦
		補正 += 50
ELSE
	SELECTCASE EXP:被写経験
		CASE IS >= EXPLV:10
			補正 += 73
		CASE IS >= EXPLV:9
			補正 += 61
		CASE IS >= EXPLV:8
			補正 += 50
		CASE IS >= EXPLV:7
			補正 += 40
		CASE IS >= EXPLV:6
			補正 += 31
		CASE IS >= EXPLV:5
			補正 += 23
		CASE IS >= EXPLV:4
			補正 += 16
		CASE IS >= EXPLV:3
			補正 += 10
		CASE IS >= EXPLV:2
			補正 += 5
		CASE IS >= EXPLV:1
			補正 += 1
		CASEELSE
			補正 -= 2
	ENDSELECT
ENDIF

;撮影者を見る
;主人
SELECTCASE TFLAG:ビデオ撮影者
	CASE 1
		;撮影技能を見る
		SELECTCASE ABL:MASTER:撮影技能
			CASE 0
				補正 -= 20
			CASE 1
				補正 -= 10
			CASE 3
				補正 += 10
			CASE 4
				補正 += 30
			CASE IS >= 5
				補正 += 50
		ENDSELECT
	;助手
	CASE 2
		;撮影技能を見る
		SELECTCASE ABL:ASSI:撮影技能
			CASE 0
				補正 -= 20
			CASE 1
				補正 -= 10
			CASE 3
				補正 += 10
			CASE 4
				補正 += 30
			CASE IS >= 5
				補正 += 50
		ENDSELECT
ENDSELECT

;倍率処理
SIF 補正 < 1
	補正 = 1

価格 *= 補正
価格 /= 100

;難易度処理
SELECTCASE FLAG:難易度
	;BEGINNER
	CASE -1
		TIMES 価格 , 1.20
	;NORMAL
	CASE 1
		TIMES 価格 , 1.00
	;HYPER
	CASE 2
		TIMES 価格 , 0.80
	;ANOTHER
	CASE 3
		TIMES 価格 , 0.60
	;LEGGENDARIA
	CASE 4
		TIMES 価格 , 0.05
ENDSELECT

;ゲイビデオならボーナスが付く
SIF SEX(MASTER) == SEX(TARGET)
	TIMES 価格 , 1.10
SIF ASSI > 0 && SEX(ASSI) == SEX(TARGET)
	TIMES 価格 , 1.10

;霊体の撮影なら価格は0になる
SIF TALENT:霊体
	価格 = 0

TFLAG:ビデオ価格 += 価格
TFLAG:ビデオ撮影回数 = 0

@SELL_VIDEO
#DIM DYNAMIC 加算額
#DIM DYNAMIC 所持ビデオ本数

;調教時に録画したビデオの売却
;撮影中のまま売却まで来た場合はテープの数を減らす
;テープの減数は撮影後処理のため、さらにビデオの値段および経験もここで処理をする
IF TEQUIP:ビデオカメラ
	ITEM:ビデオテープ--
	CALL PRICE_VIDEO
ENDIF

;録画してなければ終了
SIF TFLAG:ビデオ価格 == 0
	RETURN 0

DRAWLINE

;ビデオボーナス、もしくは処女喪失シーンがあれば内容を表示
IF VIDEOBONUS:主人中出し || VIDEOBONUS:主人アナル中出し || VIDEOBONUS:助手中出し || VIDEOBONUS:助手アナル中出し || VIDEOBONUS:射精 || VIDEOBONUS:放尿 || VIDEOBONUS:噴乳 || TFLAG:撮影破瓜
	PRINTL 【ビデオ内容】
	;処女喪失シーンがあれば加算
	IF TFLAG:撮影破瓜
		加算額 += 5000
		EXP:異常経験++
		PRINTFORML 　このビデオには%CALLNAME:TARGET%の処女喪失シーンが記録されている
		PRINTFORML 　異常経験+1
	ENDIF

	;ビデオボーナスを見る
	;主人→奴隷 中出し&アナル中出し
	IF VIDEOBONUS:主人中出し && VIDEOBONUS:主人アナル中出し
		PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:MASTER%に{VIDEOBONUS:主人中出し}回膣内射精され、{VIDEOBONUS:主人アナル中出し}回アナルにも射精されている
		加算額 += VIDEOBONUS:主人中出し*300
		加算額 += VIDEOBONUS:主人アナル中出し*250
	;中出しのみ
	ELSEIF VIDEOBONUS:主人中出し
		PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:MASTER%に{VIDEOBONUS:主人中出し}回、膣内に射精されている
		加算額 += VIDEOBONUS:主人中出し*300
	;アナル中出しのみ
	ELSEIF VIDEOBONUS:主人アナル中出し
		PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:MASTER%に{VIDEOBONUS:主人アナル中出し}回、アナルに射精されている
		加算額 += VIDEOBONUS:主人アナル中出し*250
	ENDIF

	;助手→奴隷 中出し&アナル中出し
	IF VIDEOBONUS:助手中出し && VIDEOBONUS:助手アナル中出し
		PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:ASSI%に{VIDEOBONUS:助手中出し}回膣内射精され、{VIDEOBONUS:助手アナル中出し}回アナルにも射精されている
		加算額 += VIDEOBONUS:助手中出し*300
		加算額 += VIDEOBONUS:助手アナル中出し*250
	;中出しのみ
	ELSEIF VIDEOBONUS:助手中出し
		PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:ASSI%に{VIDEOBONUS:助手中出し}回、膣内に射精されている
		加算額 += VIDEOBONUS:助手中出し*300
	;アナル中出しのみ
	ELSEIF VIDEOBONUS:助手アナル中出し
		PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:ASSI%に{VIDEOBONUS:助手アナル中出し}回、アナルに射精されている
		加算額 += VIDEOBONUS:助手アナル中出し*250
	ENDIF

	;奴隷射精
	IF VIDEOBONUS:射精
		PRINTFORML 　{VIDEOBONUS:射精}回、%CALLNAME:TARGET%が射精している
		加算額 += VIDEOBONUS:射精*150
	ENDIF
	;奴隷放尿
	IF VIDEOBONUS:放尿
		PRINTFORML 　{VIDEOBONUS:放尿}回、%CALLNAME:TARGET%が放尿している
		加算額 += VIDEOBONUS:放尿*100
	ENDIF
	;奴隷噴乳
	IF VIDEOBONUS:噴乳
		PRINTFORML 　{VIDEOBONUS:噴乳}回、%CALLNAME:TARGET%が噴乳している
		加算額 += VIDEOBONUS:噴乳*100
	ENDIF
	;主人のドラゴン→奴隷中出し&アナル中出し
	IF VIDEOBONUS:ドラゴン中出し && VIDEOBONUS:ドラゴンアナル中出し
		IF CSTR:MASTER:ドラゴン == "ドラゴン"
			PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:MASTER%のドラゴンに{VIDEOBONUS:ドラゴン中出し}回膣内射精され、{VIDEOBONUS:ドラゴンアナル中出し}回アナルにも射精されている
		ELSE
			PRINTFORML 　%CALLNAME:TARGET%は%CSTR:MASTER:ドラゴン%に{VIDEOBONUS:ドラゴン中出し}回膣内射精され、{VIDEOBONUS:ドラゴンアナル中出し}回アナルにも射精されている
		ENDIF
		加算額 += VIDEOBONUS:ドラゴン中出し*350
		加算額 += VIDEOBONUS:ドラゴンアナル中出し*300
	;中出しのみ
	ELSEIF VIDEOBONUS:ドラゴン中出し
		IF CSTR:MASTER:ドラゴン == "ドラゴン"
			PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:MASTER%のドラゴンに{VIDEOBONUS:ドラゴン中出し}回、膣内に射精されている
		ELSE
			PRINTFORML 　%CALLNAME:TARGET%は%CSTR:MASTER:ドラゴン%に{VIDEOBONUS:ドラゴン中出し}回、膣内に射精されている
		ENDIF
		加算額 += VIDEOBONUS:ドラゴン中出し*350
	;アナル中出しのみ
	ELSEIF VIDEOBONUS:ドラゴンアナル中出し
		IF CSTR:MASTER:ドラゴン == "ドラゴン"
			PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:MASTER%のドラゴンに{VIDEOBONUS:ドラゴンアナル中出し}回、アナルに射精されている
		ELSE
			PRINTFORML 　%CALLNAME:TARGET%は%CSTR:MASTER:ドラゴン%に{VIDEOBONUS:ドラゴンアナル中出し}回、アナルに射精されている
		ENDIF
		加算額 += VIDEOBONUS:ドラゴンアナル中出し*300
	ENDIF
	;助手のドラゴン→奴隷中出し&アナル中出し
	IF VIDEOBONUS:助手ドラゴン中出し && VIDEOBONUS:助手ドラゴンアナル中出し
		IF CSTR:ASSI:ドラゴン == "ドラゴン"
			PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:ASSI%のドラゴンに{VIDEOBONUS:助手ドラゴン中出し}回膣内射精され、{VIDEOBONUS:助手ドラゴンアナル中出し}回アナルにも射精されている
		ELSE
			PRINTFORML 　%CALLNAME:TARGET%は%CSTR:ASSI:ドラゴン%に{VIDEOBONUS:助手ドラゴン中出し}回膣内射精され、{VIDEOBONUS:助手ドラゴンアナル中出し}回アナルにも射精されている
		ENDIF
		加算額 += VIDEOBONUS:助手ドラゴン中出し*350
		加算額 += VIDEOBONUS:助手ドラゴンアナル中出し*300
	;中出しのみ
	ELSEIF VIDEOBONUS:助手ドラゴン中出し
		IF CSTR:MASTER:ドラゴン == "ドラゴン"
			PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:ASSI%のドラゴンに{VIDEOBONUS:助手ドラゴン中出し}回、膣内に射精されている
		ELSE
			PRINTFORML 　%CALLNAME:TARGET%は%CSTR:ASSI:ドラゴン%に{VIDEOBONUS:助手ドラゴン中出し}回、膣内に射精されている
		ENDIF
		加算額 += VIDEOBONUS:助手ドラゴン中出し*350
	;アナル中出しのみ
	ELSEIF VIDEOBONUS:助手ドラゴンアナル中出し
		IF CSTR:MASTER:ドラゴン == "ドラゴン"
			PRINTFORML 　%CALLNAME:TARGET%は%CALLNAME:ASSI%のドラゴンに{VIDEOBONUS:助手ドラゴンアナル中出し}回、アナルに射精されている
		ELSE
			PRINTFORML 　%CALLNAME:TARGET%は%CSTR:ASSI:ドラゴン%に{VIDEOBONUS:助手ドラゴンアナル中出し}回、アナルに射精されている
		ENDIF
		加算額 += VIDEOBONUS:助手ドラゴンアナル中出し*300
	ENDIF
	;触手中出し
	IF VIDEOBONUS:触手中出し
		PRINTFORML 　%CALLNAME:TARGET%が触手に{VIDEOBONUS:触手中出し}回、膣内に射精されている
		加算額 += VIDEOBONUS:触手中出し*400
	ENDIF
	;触手アナル中出し
	IF VIDEOBONUS:触手アナル中出し
		PRINTFORML 　%CALLNAME:TARGET%が触手に{VIDEOBONUS:触手アナル中出し}回、アナルに射精されている
		加算額 += VIDEOBONUS:触手アナル中出し*350
	ENDIF
ENDIF

;人気度も加算する
加算額 += 人気度:(NOF(TARGET))

CALL PASELIL, @"録画したビデオは{TFLAG:ビデオ価格+加算額}＄程度の値が付きそうだ"
PRINTL 録画したビデオをどうするか選択してください
PRINTL [0] - 売る
PRINT [1] - 手元に残す
SIF CFLAG:所持ビデオ数
	PRINTFORM （%CALLNAME:TARGET%のビデオは既に持っている）
PRINTL 
BINPUT

SELECTCASE RESULT
	CASE 0
		CALL PASELIW, @"調教時に録画したビデオを売って、{TFLAG:ビデオ価格+加算額}＄手に入れました"
		CALL PAYMENT, TFLAG:ビデオ価格+加算額, @"%NAME:TARGET%のAV売却"
		TFLAG:ビデオ価格 = 0
		人気度:(NOF(TARGET)) += 10
		;ニンフちゃん出演のAVを売りさばく
		SIF CHARANAMEF(TARGET) == "ニンフちゃん"
			CALL 実績解除, "ニンフちゃんのファンやめます"
		CALL KOJO_MESSAGE_EVENT, "ビデオ売却時"
		バンめし = 0
	CASE 1
		PRINTFORMW %CALLNAME:TARGET%との思い出を保管した({CFLAG:所持ビデオ数+1}本目)
		CFLAG:所持ビデオ数++
		所持ビデオ本数 = FINDELEMENT(所持ビデオ, "", 0, VARSIZE("所持ビデオ"), 1)
		IF INRANGE(所持ビデオ本数, 0, VARSIZE("所持ビデオ")-1)
			PRINTL ビデオにタイトルを付けますか？
			PRINTL [0] - はい
			PRINTL [1] - いいえ
			BINPUT
			SELECTCASE RESULT
				CASE 0
					PRINTL タイトルを入力てください
					INPUTS
					SIF RESULTS == ""
						GOTO NOTITLE
					所持ビデオ:所持ビデオ本数 = 主演:%NAME:TARGET% \@ ASSI > 0 ? 助演:%NAME:ASSI% # \@「%RESULTS%」
				CASE 1
					$NOTITLE
					所持ビデオ:所持ビデオ本数 = 主演:%NAME:TARGET% \@ ASSI > 0 ? 助演:%NAME:ASSI% # \@のアダルトビデオ
			ENDSELECT
		ENDIF
		;理々奈のハメ撮り中出しビデオを保管する
		SIF CHARANAMEF(TARGET) == "雛月 理々奈" && VIDEOBONUS:主人中出し
			CALL 実績解除, "お兄さんにビデオレター送っとこうね"
		CALL KOJO_MESSAGE_EVENT, "ビデオ保管時"
ENDSELECT

@SELL_PICTURE
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC LCOUNT2
#DIM DYNAMIC 総額
#DIM DYNAMIC 保持, 10
#DIM DYNAMIC 総枚数
#DIM DYNAMIC 売却枚数

;売れる写真が無ければ関数終了
SIF !写真価格:0
	RETURN 0
FOR LCOUNT, 0, 50
	SIF PICS:(NOF(TARGET)):LCOUNT != ""
		総枚数++
NEXT

CALL LOOP_CLEARLINE

DRAWLINE
;PRINTL 撮影した写真を売却します
PRINTL □今回撮影した写真
FOR LCOUNT, 0, 10
	SIF !写真価格:LCOUNT
		BREAK
	PRINTFORM {LCOUNT+1, 2}枚目.%写真内容:LCOUNT% [{LCOUNT}] 
	SIF 総枚数 < 50
		PRINTFORM \@ 保持:LCOUNT ? 保持 # 売却 \@
	PRINTL 
	CALL PASELIL, @"       売却額:{写真価格:LCOUNT}＄"
NEXT
IF 総枚数 == 50
	PRINTL アルバムに空きが無いため売却します
	GOTO SELL
ENDIF
DRAWLINE
PRINT 　[10] 決定 [11] 全て売却 
SIF 総枚数 < 40
	PRINT [12] 全て保持
PRINTL 
BINPUT
SELECTCASE RESULT
	CASE 11
		VARSET 保持
	CASE 12
		SIF 総枚数 >= 40
			CONTINUE
		VARSET 保持, 1
	CASE 0 TO 9
		IF !保持:RESULT && SUMARRAY(保持)+総枚数 >= 50
			PRINTW 保管できるのは一人につき50枚までです
			RESTART
		ENDIF
		INVERTBIT 保持:RESULT, 0
		RESTART
ENDSELECT
売却枚数 = 0
FOR LCOUNT, 0, 10
	IF !保持:LCOUNT
		総額 += 写真価格:LCOUNT
		売却枚数++
	ENDIF
NEXT
IF 総額
	$SELL
	バンめし = 0
	CALL PASELIL, @"計{総額}＄で売れました"
	CALL PAYMENT, 総額, @"%NAME:TARGET%の写真売却"
	人気度:(NOF(TARGET)) += 売却枚数
	WAIT
	CALL KOJO_MESSAGE_EVENT, "写真売却時"
ENDIF

IF SUMARRAY(保持)
	PRINTFORML {10-MATCH(写真内容, "")}枚の写真を保管しました
	FOR LCOUNT, 0, 10
		IF 保持:LCOUNT
			;二次元配列でFINDELEMENTは使えないので手作業
			FOR LCOUNT2, 0, 50
				IF PICS:(NOF(TARGET)):LCOUNT2 == ""
					PIC:(NOF(TARGET)):LCOUNT2 = 写真価格:LCOUNT
					PICS:(NOF(TARGET)):LCOUNT2 = %写真内容:LCOUNT%
					BREAK
				ENDIF
			NEXT
		ENDIF
	NEXT
ENDIF

@SELL_MILK
#DIM DYNAMIC 売却額
#DIM DYNAMIC 採取量

売却額 = TFLAG:搾乳量*50
採取量 = TFLAG:搾乳量*10
IF 売却額 > 0
	SIF TALENT:淫乳
		売却額 *= 2

	;難易度処理
	SELECTCASE FLAG:難易度
		;BEGINNER
		CASE -1
			TIMES 売却額 , 1.20
		;NORMAL
		CASE 1
			TIMES 売却額 , 1.00
		;HYPER
		CASE 2
			TIMES 売却額 , 0.80
		;ANOTHER
		CASE 3
			TIMES 売却額 , 0.60
		;LEGGENDARIA
		CASE 4
			TIMES 売却額 , 0.05
	ENDSELECT

	IF ISSLIME(TARGET)
		DRAWLINE
		PRINTFORML 今回の調教で%CALLNAME:TARGET%から{採取量}mlの粘液を採取した
		CALL PASELIL, @"%CALLNAME:TARGET%の粘液は{売却額}＄で売れそうです。売却しますか？"
		PRINTL [0] - 売却する
		PRINTL [1] - 保管する
		BINPUT
		SELECTCASE RESULT
			CASE 0
				MONEY += 売却額
				CALL 現金出納帳記録, @"%NAME:TARGET%の粘液売却/{売却額}"
				CALL PASELIW, @"所持金 {売却額}＄増加"
				バンめし = 0
			CASE 1
				リソース:粘液 += 採取量
				採取精液:(NOF(TARGET))++
				PRINTFORMW 保管粘液量 {採取量}ml増加
		ENDSELECT
	ELSE
		DRAWLINE
		PRINTFORML 今回の調教で%CALLNAME:TARGET%から{採取量}mlの母乳を採取した
		CALL PASELIL, @"%CALLNAME:TARGET%の母乳は{売却額}＄で売れそうです。売却しますか？"
		PRINTL [0] - 売却する
		PRINTL [1] - 保管する
		BINPUT
		SELECTCASE RESULT
			CASE 0
				MONEY += 売却額
				CALL 現金出納帳記録, @"%NAME:TARGET%の母乳売却/{売却額}"
				CALL PASELIW, @"所持金 {売却額}＄増加"
				バンめし = 0
			CASE 1
				リソース:母乳 += 採取量
				CALL PASELIW, @"保管母乳量 {採取量}ml増加"
		ENDSELECT
	ENDIF
	DRAWLINE
ENDIF

