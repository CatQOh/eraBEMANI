@COM_ORDER_手淫
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC 実効値, 2

;ABL:欲望
IF ABL:欲望
	実効値 += ABL:欲望
	PRINTFORM +欲望LV{ABL:欲望}({ABL:欲望})
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	実効値 += ABL:奉仕精神*4
	PRINTFORM +奉仕精神LV{ABL:奉仕精神}({ABL:奉仕精神*4})
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	実効値 += ABL:精液中毒*2
	PRINTFORM +精液中毒LV{ABL:精液中毒}({ABL:精液中毒*2})
ENDIF

CALL COM_ORDER_欲情, 1
実効値 += RESULT

FOR LCOUNT, 0, VARSIZE("TALENT")
	実効値:1 = 0
	SIF !TALENT:LCOUNT
		CONTINUE
	SELECTCASE TALENTNAME:LCOUNT
		CASE "親愛"
			SIF PLAYER == MASTER
				実効値:1 = 10
		CASE "恋慕"
			SIF PLAYER == MASTER
				実効値:1 = 5
		CASE "淫魔", "隷属"
			実効値:1 = 10
		CASE "淫乱", "服従"
			実効値:1 = 5
		CASE "恥じらい"
			実効値:1 = -2
		CASE "献身的"
			実効値:1 = 6
		CASE "汚臭敏感"
			実効値:1 = -5
		CASE "汚臭鈍感"
			実効値:1 = 3
		CASE "サキュバス"
			実効値:1 = 10
	ENDSELECT
	IF 実効値:1
		PRINTFORM \@SIGN(実効値:1) == 1 ? + # - \@%TALENTNAME:LCOUNT%({ABS(実効値:1)})
		実効値 += 実効値:1
	ENDIF
NEXT

実効値:1 = 0
;愛液の汚れ
SIF GETBIT(STAIN:PLAYER:ペニス, 0)
	実効値:1 += 1
;精液の汚れ
SIF GETBIT(STAIN:PLAYER:ペニス, 2)
	実効値:1 += 3
;アナルの汚れ
SIF GETBIT(STAIN:PLAYER:ペニス, 3)
	実効値:1 += 5
;母乳汚れ
SIF GETBIT(STAIN:PLAYER:ペニス, 4)
	実効値:1 += 1
;尿
SIF GETBIT(STAIN:PLAYER:ペニス, 7)
	実効値:1 += 5

SIF TALENT:汚れ無視
	実効値:1 /= 3
SIF TALENT:潔癖症
	実効値:1 *= 2

;汚れあり
IF 実効値:1
	実効値 -= 実効値:1
	IF TALENT:汚れ無視
		PRINT -汚れあり、汚れ無視
	ELSEIF TALENT:潔癖症
		PRINT -汚れあり、潔癖症
	ELSE
		PRINT -汚れあり
	ENDIF
	PRINTFORM ({実効値:1})
ENDIF

RETURN 実効値

