@COM_ORDER
#DIM DYNAMIC 実効値, 2

IF OPTION:口上表示 && !CFLAG:口上非表示 && EXISTFUNCTION(@"COM_ORDER_%SELECTCOMS%_%CSVNAMERPS(NO:TARGET)%")
	CALLFORM COM_ORDER_%SELECTCOMS%_%CSVNAMERPS(NO:TARGET)%
	IF RESULT
		実効値 += RESULT
		PRINTFORM \@ SIGN(RESULT) == 1 ? + # - \@口上補正({RESULT})
	ENDIF
ENDIF

IF ABL:従順
	実効値 += ABL:従順*4
	PRINTFORM +従順LV{ABL:従順}({ABL:従順*4})
ENDIF
IF ABL:マゾっ気
	実効値 += ABL:マゾっ気*2
	PRINTFORM +マゾっ気LV{ABL:マゾっ気}({ABL:マゾっ気*2})
ENDIF

;レズ調教
IF SEX(PLAYER) == 2 && SEX(TARGET) == 2
	IF ABL:レズっ気
		実効値 += ABL:レズっ気*3
		PRINTFORM +レズっ気LV{ABL:レズっ気}({ABL:レズっ気*3})
	ENDIF
	IF ABL:レズ中毒
		実効値 += ABL:レズ中毒*3
		PRINTFORM +レズ中毒LV{ABL:レズ中毒}({ABL:レズ中毒*3})
	ENDIF
	IF TALENT:両刀
		実効値 += 10
		PRINTFORM +両刀(10)
	ENDIF
	IF TALENT:好奇心
		実効値 += 7
		PRINTFORM +好奇心(7)
	ENDIF
	IF TALENT:保守的
		実効値 -= 13
		PRINTFORM -保守的(13)
	ENDIF
;ゲイ調教
ELSEIF SEX(PLAYER) == 1 && SEX(TARGET) == 1
	IF ABL:ホモっ気
		実効値 += ABL:ホモっ気*3
		PRINTFORM +ホモっ気LV{ABL:ホモっ気}({ABL:ホモっ気*3})
	ENDIF
	IF ABL:ゲイ中毒
		実効値 += ABL:ゲイ中毒*3
		PRINTFORM +ゲイ中毒LV{ABL:ゲイ中毒}({ABL:ゲイ中毒}*3)
	ENDIF
	IF TALENT:両刀
		実効値 += 10
		PRINTFORM +両刀(10)
	ENDIF
	IF TALENT:好奇心
		実効値 += 7
		PRINTFORM +好奇心(7)
	ENDIF
	IF TALENT:保守的
		実効値 -= 13
		PRINTFORM -保守的(13)
	ENDIF
ELSE
	IF TALENT:好奇心
		実効値 += 5
		PRINTFORM +好奇心(5)
	ENDIF
	IF TALENT:保守的
		実効値 -= 10
		PRINTFORM -保守的(10)
	ENDIF
ENDIF

;刻印チェック
IF MARK:苦痛刻印
	実効値 += MARK:苦痛刻印*5
	PRINTFORM +苦痛刻印LV{MARK:苦痛刻印}({MARK:苦痛刻印*5})
ENDIF

;プライド
IF TALENT:プライド高い
	実効値:1 = 4
ELSEIF TALENT:プライド低い
	実効値:1 = 1
ELSE
	実効値:1 = 2
ENDIF

;屈服刻印および反発刻印とプライド
IF MARK:屈服刻印
	実効値 += MARK:屈服刻印*3*実効値:1
	PRINTFORM +屈服刻印LV{MARK:屈服刻印}({MARK:屈服刻印*3*実効値:1})
ENDIF
IF MARK:反発刻印
	実効値 -= MARK:反発刻印*2*実効値:1
	PRINTFORM -反発刻印LV{MARK:反発刻印}({MARK:反発刻印*2*実効値:1})
ENDIF

実効値:1 = GETPALAMLV(PALAM:恭順, 5)
IF 実効値:1
	実効値 += 実効値:1*3
	PRINTFORM +恭順LV{実効値:1}({実効値:1*3})
ENDIF

;PALAM:恐怖
実効値:1 = GETPALAMLV(PALAM:恐怖, 5)
IF 実効値:1
	実効値 += 実効値:1*3
	PRINTFORM +恐怖LV{実効値:1}({実効値:1*3})
ENDIF

IF TALENT:服従
	実効値 += 10
	PRINTFORM +服従(10)
ENDIF
IF TALENT:隷属
	実効値 += 20
	PRINTFORM +隷属(20)
ENDIF
IF TALENT:反抗的 && !陥落(TARGET)
	実効値 -= 5
	PRINTFORM -反抗的(5)
ENDIF
IF TALENT:気丈
	実効値 -= 5
	PRINTFORM -気丈(5)
ENDIF
IF TALENT:素直
	実効値 += 5
	PRINTFORM +素直(5)
ENDIF
IF TALENT:プライド高い
	実効値 -= 15
	PRINTFORM -プライド高い(15)
ENDIF
IF TALENT:プライド低い
	実効値 += 5
	PRINTFORM +プライド低い(5)
ENDIF
IF TALENT:ツンデレ
	IF ABL:従順 < 4
		実効値 -= 7
		PRINTFORM -ツンデレ(7)
	ELSE
		実効値 += 7
		PRINTFORM +ツンデレ(7)
	ENDIF
ENDIF
IF TALENT:目立ちたがり
	実効値 += 2
	PRINTFORM +目立ちたがり(2)
ENDIF
IF TALENT:抑圧
	実効値 -= 10
	PRINTFORM -抑圧(10)
ENDIF
IF TALENT:抵抗
	実効値 -= 12
	PRINTFORM -抵抗(12)
ENDIF
IF TALENT:弱味
	実効値 += 12
	PRINTFORM +弱味(12)
ENDIF
IF TALENT:弱気
	実効値 += 8
	PRINTFORM +弱気(8)
ENDIF
IF TALENT:強気
	実効値 -= 8
	PRINTFORM -強気(8)
ENDIF
IF TALENT:即落ち
	実効値 += 10
	PRINTFORM +即落ち(10)
ENDIF
IF TALENT:妄信
	実効値 += 8
	PRINTFORM +妄信(8)
ENDIF
IF TALENT:寂しがり屋 && ASSI > 0
	実効値 += 6
	PRINT +寂しがり屋(6)
ENDIF
IF TALENT:同族嫌悪 && CSTR:種族 == CSTR:PLAYER:種族
	実効値 -= 10
	PRINT -同族嫌悪(10)
ENDIF

IF TALENT:男好き && SEX(PLAYER) == 1
	実効値 += 5
	PRINT +男好き(5)
ENDIF
IF TALENT:女好き && SEX(PLAYER) == 2
	実効値 += 5
	PRINT +女好き(5)
ENDIF
IF TALENT:男嫌い && SEX(PLAYER) == 1
	実効値 -= 10
	PRINT -男嫌い(10)
ENDIF
IF TALENT:女嫌い && SEX(PLAYER) == 2
	実効値 -= 10
	PRINT -女嫌い(10)
ENDIF
IF TALENT:サボり魔
	実効値 -= 3
	PRINT -サボり魔(3)
ENDIF
IF TALENT:子供好き && TALENT:PLAYER:体格 <= 2
	実効値 += 3
	PRINT +子供好き(3)
ENDIF
SELECTCASE CFLAG:スライム度
	CASE 1 TO 10
		実効値 += 3
		PRINT +スライム侵食Lv1(3)
	CASE 11 TO 20
		実効値 += 5
		PRINT +スライム侵食Lv2(5)
	CASE 21 TO 30
		実効値 += 7
		PRINT +スライム侵食Lv3(7)
	CASE 31 TO 1499
		実効値 += 7
		PRINT +スライム(9)
	CASE IS >= 1500
		実効値 += 15
		PRINT +溶ける快(15)
ENDSELECT

;調教者の能力、素質
IF ABL:PLAYER:サドっ気
	実効値 += ABL:PLAYER:サドっ気*2
	PRINTFORM +%CALLNAME:PLAYER%のサドっ気LV{ABL:PLAYER:サドっ気}({ABL:PLAYER:サドっ気*2})
ENDIF
IF TALENT:PLAYER:魅惑
	実効値 += 6
	PRINTFORM +%CALLNAME:PLAYER%の魅惑(6)
ENDIF
IF TALENT:PLAYER:謎の魅力
	実効値 += 8
	PRINTFORM +%CALLNAME:PLAYER%の謎の魅力(8)
ENDIF
IF TALENT:PLAYER:サド
	実効値 += 3
	PRINTFORM +%CALLNAME:PLAYER%がサド(3)
ENDIF
IF TALENT:PLAYER:強気
	実効値 += 5
	PRINTFORM +%CALLNAME:PLAYER%が強気(5)
ENDIF
IF TALENT:PLAYER:弱気
	実効値 -= 5
	PRINTFORM -%CALLNAME:PLAYER%が弱気(5)
ENDIF

;助手の素質
IF ASSI >= 0 && TALENT:ASSI:鼓舞
	実効値 += 2
	PRINTFORM +%CALLNAME:ASSI%が鼓舞(2)
ENDIF
IF ASSI >= 0 && TALENT:ASSI:ハイテンション
	実効値 += 2
	PRINTFORM +%CALLNAME:ASSI%がハイテンション(2)
ENDIF

;アニマルは依存度（なつき度）が足りないと猛反発
IF TALENT:アニマル
	SELECTCASE CFLAG:依存度
		CASE IS <= 0
			PRINT -なつき度皆無(100)
			実効値 -= 100
		CASE IS <= 100
			PRINT -懐いていない(70)
			実効値 -= 70
		CASE IS <= 300
			PRINT -警戒している(50)
			実効値 -= 50
		CASE IS <= 500
			PRINT -好かれていない(30)
			実効値 -= 30
		CASE IS <= 700
			PRINT -嫌いじゃない(10)
			実効値 -= 10
		CASE IS <= 1000
			PRINT -対等(3)
			実効値 -= 3
		CASE IS <= 1300
			PRINT +気になる相手(5)
			実効値 += 5
		CASE IS <= 1500
			PRINT +可愛がってくれる相手(10)
			実効値 += 10
		CASE IS <= 2000
			PRINT +飼い主(15)
			実効値 += 15
		CASE IS <= 3000
			PRINT +大好きな飼い主(30)
			実効値 += 30
		CASEELSE
			PRINT +ご主人様♥(50)
			実効値 += 50
	ENDSELECT
ENDIF

;酔
IF CFLAG:酔
	実効値 += CFLAG:酔/10+1
	PRINTFORM +酔({CFLAG:酔/10+1})
ENDIF

;相性チェック
SELECTCASE 相性値(TARGET, PLAYER)
	CASE 0 TO 30
		実効値 -= 10
		PRINT -相性最悪(10)
	CASE 30 TO 70
		実効値 -= 6
		PRINT -相性悪い(6)
	CASE 70 TO 99
		実効値 -= 3
		PRINT -相性いまいち(3)
	CASE 101 TO 130
		実効値 += 3
		PRINT +相性ややよい(3)
	CASE 130 TO 170
		実効値 += 6
		PRINT +相性よい(6)
	CASE IS >= 170
		実効値 += 10
		PRINT +相性最高(10)
ENDSELECT

IF ゲームモード() == "学園モード"
	IF SIGN(好感度合計(TARGET)) == 1
		実効値 += 好感度合計(TARGET)/100
		PRINTFORM +好感度({好感度合計(TARGET)/100})
	ENDIF
	IF CFLAG:倫理観
		実効値 += CFLAG:倫理観
		PRINTFORM +倫理観崩壊({CFLAG:倫理観})
	ENDIF
ENDIF

IF TFLAG:ヒートストローク && FLAG:気温 >= 200
	実効値 += (FLAG:気温-200)/10+1
	PRINTFORM +ヒートストローク({(FLAG:気温-200)/10+1})
ENDIF
IF TFLAG:ホワイトアウト && FLAG:気温 <= 200
	実効値 += ABS((FLAG:気温-200)/10+1)
	PRINTFORM +ホワイトアウト({ABS((FLAG:気温-200)/10+1)})
ENDIF
IF TFLAG:メタリックマインド
	実効値 -= 15
	PRINT -メタリックマインド(15)
ENDIF

IF 特質("意志薄弱")
	実効値 += 特質("意志薄弱")
	PRINTFORM +意志薄弱({特質("意志薄弱")})
ENDIF

RETURN 実効値

@COM_ORDER_欲情, タイプ
#DIM DYNAMIC 実効値, 3
#DIM DYNAMIC タイプ
;欲情Lv(最大で5)に対して、以下のタイプによって別々に変動する
;タイプ0...[衝動的]で×1.5, [冷静]で×0.5, 最後に×3
;タイプ1...[衝動的]で+2, [冷静]で×0.75

IF タイプ == 0
	実効値:1 = GETPALAMLV(PALAM:欲情, 5)
	実効値:2 = 実効値:1
	SIF TALENT:衝動的
		TIMES 実効値:2 , 1.50
	SIF TALENT:冷静
		TIMES 実効値:2 , 0.50
	IF 実効値:2
		実効値 += 実効値:2*3
		PRINTFORM +欲情LV{実効値:1}({実効値:2*3})
	ENDIF
	RETURN 実効値:2*3
ELSEIF タイプ == 1
	実効値:1 = GETPALAMLV(PALAM:欲情, 5)
	実効値:2 = 実効値:1
	SIF TALENT:衝動的
		実効値:2 += 2
	SIF TALENT:冷静
		TIMES 実効値:2 , 0.75
	IF 実効値:2
		実効値 += 実効値:2
		PRINTFORM +欲情LV{実効値:1}({実効値:2})
	ENDIF
	RETURN 実効値:2
ENDIF

