@USERCOM_ABLE_体内放尿する
#FUNCTION
コマンド実行条件 += "小スカMODが必要/"
SIF !MOD:SCAT2
	COMRESULT = 0
コマンド実行条件 += "上級売春中は実行不可/"
SIF 上級売春中
	COMRESULT = 0
コマンド実行条件 += "実行者にペニスが必要/"
SIF !PENIS(PLAYER)
	COMRESULT = 0
コマンド実行条件 += "避妊具使用時は実行不可/"
SIF 避妊具(PLAYER)
	COMRESULT = 0

;前回と今回の調教者が同じ
コマンド実行条件 += "前回と今回の実行者が同じで、/"
コマンド実行条件 += "性交系から派生可能/"
IF TFLAG:前コマンド実行者 == PLAYER
	;対応する体位か連続実行なら派生可能
	SELECTCASE PREVCOMS
		CASE "正常位", "後背位", "騎乗位", "対面座位", "背面座位", "下着ずらし挿入", "正常位アナル", "後背位アナル", "騎乗位アナル", "対面座位アナル", "背面座位アナル", "下着ずらし挿入アナル", "臍挿入", "乳管挿入", "尿道挿入",;暫定的に派生コマンドからは派生しないようにしてみる
			RETURNF COMRESULT
	ENDSELECT
ENDIF

COMRESULT = 0
RETURNF 0

@COM_体内放尿する
#DIM DYNAMIC 増加量
#DIM DYNAMIC 射精先

SELECTCASE PREVPOSITIONS
	CASE "正常位", "後背位", "騎乗位", "対面座位", "背面座位", "下着ずらし挿入"
		PRINTL 膣内放尿する
		TFLAG:膣コマンド = 1
	CASE "正常位アナル", "後背位アナル", "騎乗位アナル", "対面座位アナル", "背面座位アナル", "下着ずらし挿入アナル"
		PRINTL 腸内放尿する
	CASE "臍挿入"
		PRINTL 臍内放尿する
	CASE "乳管挿入"
		PRINTL 乳管内放尿する
	CASE "尿道挿入"
		PRINTL 尿道内放尿する
ENDSELECT
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;射精ゲージチェック（奴隷）
;-------------------------------------------------
SELECTCASE PREVPOSITIONS
	CASE "正常位", "後背位", "騎乗位", "対面座位", "背面座位", "下着ずらし挿入"
		増加量 = 射精ゲージ計算_Ｖ挿入(400)

		;ABL:調教者の技巧をみる
		増加量 += (増加量/5)*ABL:PLAYER:技巧

		;ABL:技巧をみる
		増加量 += (増加量/10)*ABL:技巧

		;ABL:奉仕精神をみる
		SELECTCASE ABL:奉仕精神
			CASE 0
				TIMES 増加量 , 0.50
			CASE 1
				TIMES 増加量 , 0.80
			CASE 2
				TIMES 増加量 , 1.20
			CASE 3
				TIMES 増加量 , 1.50
			CASE 4
				TIMES 増加量 , 1.80
			CASEELSE
				増加量 += (増加量/3)*ABL:奉仕精神
		ENDSELECT

		CALL 射精ゲージ増加, 増加量, TARGET
	CASE "正常位アナル", "後背位アナル", "騎乗位アナル", "対面座位アナル", "背面座位アナル", "下着ずらし挿入アナル"
		増加量 = 射精ゲージ計算_Ａ挿入(400)

		;ABL:調教者の技巧をみる
		増加量 += (増加量/5)*ABL:PLAYER:技巧

		;ABL:技巧をみる
		増加量 += (増加量/10)*ABL:技巧

		;ABL:奉仕精神をみる
		SELECTCASE ABL:奉仕精神
			CASE 0
				TIMES 増加量 , 0.50
			CASE 1
				TIMES 増加量 , 0.80
			CASE 2
				TIMES 増加量 , 1.20
			CASE 3
				TIMES 増加量 , 1.50
			CASE 4
				TIMES 増加量 , 1.80
			CASEELSE
				増加量 += (増加量/3)*ABL:奉仕精神
		ENDSELECT

		CALL 射精ゲージ増加, 増加量, TARGET
ENDSELECT

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 70
LOSEBASE:気力 += 400

SELECTCASE PREVPOSITIONS
	CASE "正常位", "後背位", "騎乗位", "対面座位", "背面座位", "下着ずらし挿入"
		SOURCE:快V = 300
	CASE "正常位アナル", "後背位アナル", "騎乗位アナル", "対面座位アナル", "背面座位アナル", "下着ずらし挿入アナル"
		SOURCE:快A = 300
	;CASE "臍挿入"
	CASE "乳管挿入"
		SOURCE:快B = 100
	;CASE "尿道挿入"
ENDSELECT
SOURCE:情愛 = 150
SOURCE:苦痛 = 400
SOURCE:中毒 = 200
SOURCE:露出 = 200
SOURCE:反感 = 5000
SOURCE:恭順 = 3000
SOURCE:恐怖 = 1500
SOURCE:不潔 = 8000

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 300

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (FALLTYPE(TARGET) != 1 || PLAYER != MASTER)
	SOURCE:反感 += 1000

SELECTCASE PREVPOSITIONS
	CASE "正常位", "後背位", "騎乗位", "対面座位", "背面座位", "下着ずらし挿入"
		CALL 特殊ソース処理_Ｖ
		CALL 特殊ソース処理_セックス
	CASE "正常位アナル", "後背位アナル", "騎乗位アナル", "対面座位アナル", "背面座位アナル", "下着ずらし挿入アナル"
		CALL 特殊ソース処理_Ａ
		CALL 特殊ソース処理_セックス
ENDSELECT

;-------------------------------------------------
;絶頂チェック
;-------------------------------------------------
CALL SOURCE_CHECK_CVAB

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
SELECTCASE PREVPOSITIONS
	CASE "正常位", "後背位", "騎乗位", "対面座位", "背面座位", "下着ずらし挿入"
		射精先 = 4
	CASE "正常位アナル", "後背位アナル", "騎乗位アナル", "対面座位アナル", "背面座位アナル", "下着ずらし挿入アナル"
		射精先 = 5
	;CASE "臍挿入"
	;CASE "乳管挿入"
	;CASE "尿道挿入"
ENDSELECT
CALL SAMEN_CHECK_T, 射精先

CALL SAMEN_SHOOT_T, 射精先, NOWEX:射精

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
SELECTCASE PREVPOSITIONS
	CASE "正常位", "後背位", "騎乗位", "対面座位", "背面座位", "下着ずらし挿入"
		SETBIT STAIN:膣内, 7
		;奴隷の膣内⇔調教者のペニスの汚れが移動
		STAIN:膣内 |= STAIN:PLAYER:ペニス
		STAIN:PLAYER:ペニス |= STAIN:膣内
	CASE "正常位アナル", "後背位アナル", "騎乗位アナル", "対面座位アナル", "背面座位アナル", "下着ずらし挿入アナル"
		SETBIT STAIN:アナル, 7
		;奴隷の膣内⇔調教者のペニスの汚れが移動
		STAIN:アナル |= STAIN:PLAYER:ペニス
		STAIN:PLAYER:ペニス |= STAIN:アナル
	;CASE "臍挿入"
	;CASE "乳管挿入"
	;CASE "尿道挿入"
ENDSELECT

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
CALL EXPUP_BASE, 4, 2

SELECTCASE PREVPOSITIONS
	CASE "正常位", "後背位", "騎乗位", "対面座位", "背面座位", "下着ずらし挿入"
		CALL EXPUP, "Ｖ経験", 1
		CALL EXPUP, "性交経験", 1
		CALL EXPUP, "性交経験", 1, PLAYER
	CASE "正常位アナル", "後背位アナル", "騎乗位アナル", "対面座位アナル", "背面座位アナル", "下着ずらし挿入アナル"
		CALL EXPUP, "Ａ経験", 1
		CALL EXPUP, "性交経験", 1
		CALL EXPUP, "性交経験", 1, PLAYER
	CASE "臍挿入"
		CALL EXPUP, "臍拡張経験", 2
	CASE "乳管挿入"
		CALL EXPUP, "乳管拡張経験", 2
	CASE "尿道挿入"
		CALL EXPUP, "尿道経験", 4
ENDSELECT

CALL 魔力回復, 5
SIF !避妊具(PLAYER)
	CALL 魔力回復, 3
CALL 魔力回復, 5, PLAYER

CALL 獣人催淫, 1
CALL 獣人催淫, 1, PLAYER

SELECTCASE PREVPOSITIONS
	CASE "正常位", "後背位", "騎乗位", "対面座位", "背面座位", "下着ずらし挿入", "正常位アナル", "後背位アナル", "騎乗位アナル", "対面座位アナル", "背面座位アナル", "下着ずらし挿入アナル"
		経験回数:PLAYER:(NOF(TARGET)) += 1
		経験回数:TARGET:(NOF(PLAYER)) += 1
ENDSELECT

RETURN 1
