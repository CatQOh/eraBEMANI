	SELECTCASE ABL:Ａ感覚
		CASE 0 TO 4
			受けＡ = ABL:Ａ感覚+1
		CASE IS >= 5
			受けＡ = 6
	ENDSELECT
	プレイ回数 = (奉仕ポイント/5+1)*受けＡ
	IF PENIS(MASTER)
		EXP:性交経験 += プレイ回数
		EXP:MASTER:性交経験 += プレイ回数
		経験回数:MASTER:(NOF(TARGET)) += プレイ回数
		経験回数:(NOF(MASTER)) += プレイ回数
		射精回数:0:2 += 射精回数基礎値:0
		アナル中出し回数:MASTER:(NOF(TARGET)) += 射精回数基礎値:0
		アナル中出され回数:(NOF(MASTER)) += 射精回数基礎値:0
	ENDIF
