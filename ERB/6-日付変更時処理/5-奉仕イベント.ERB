;奴隷からの奉仕などのイベント
;-------------------------------------------------
;朝に起こるイベント
;-------------------------------------------------
@EVENT_NEWDAY_SELF
#DIM DYNAMIC 不発確率
#DIM DYNAMIC 不発
#DIM DYNAMIC 分岐
#DIM DYNAMIC LCOUNT

;調教対象が居ない場合は終了
SIF TARGET <= 0
	RETURN 0
;抑制してる場合も終了
SIF CFLAG:奉仕イベント抑制
	RETURN 0

;DISABLEチェック
CALL EVENT_CHECK, "朝イベント"
SIF RESULT == 0
	RETURN 0

;アニマルで懐いてる場合は朝に甘えるイベント
IF TALENT:アニマル && CFLAG:依存度 >= 1500
	CALL EVENT_CHECK, "アニマル"
	RETURN
ENDIF

;陥落素質が無い場合は関数終了
SIF !陥落(TARGET) || TALENT:アニマル
	RETURN 0

;対象の性経験が乏しい（経営のパートナーなど）の場合は関数終了
SIF ABL:欲望 < 2 || ABL:技巧 < 2 || ABL:従順 < 2
	RETURN 0

;主人が射精ができない場合は関数終了
SIF !PENIS(MASTER)
	RETURN 0

;対象が二日酔い起こしてると発生しない
SIF CFLAG:二日酔い
	RETURN 0

;上位イベントの条件満たしている場合に各奉仕イベントがランダムで発生するように
IF MOD:朝パイズリ特化 && ゲームモード() != "段位認定"
	分岐 = 1
ELSE
	分岐 = RAND:3
ENDIF
DO
	SELECTCASE 分岐
		CASE 0
			;朝騎乗位判定
			;主人童貞だと発生しない（情け）
			IF TALENT:MASTER:童貞
				分岐 = RAND:3
				CONTINUE
			ENDIF
			;出産トラウマがある場合は発生しない時がある
			不発確率 = CFLAG:出産トラウマ
			;RAND:(トラウマ値/10+2) <= (トラウマ値/10)だと発生しない 1あるだけでも50%で不発
			IF ((!TALENT:処女 && !TALENT:再生処女 && ABL:Ｖ感覚 >= 3) || ABL:Ａ感覚 >= 3) && ABL:奉仕精神 >= 7
				IF 不発確率 && RAND:(不発確率/10+2) <= 不発確率/10
					不発 = 1
				ELSEIF !不発
					CALL EVENT_CHECK, "朝騎乗位"
					BREAK
				ENDIF
			ENDIF
		CASE 1
			;朝パイズリ判定
			IF ABL:奉仕精神 >= 3 && TALENT:バストサイズ >= 1 && TALENT:バストサイズ+ABL:奉仕精神 >= 7
				CALL EVENT_CHECK, "朝パイズリ"
				BREAK
			ENDIF
		CASE 2
			;朝フェラ判定
			CALL EVENT_CHECK, "朝フェラ"
			BREAK
	ENDSELECT
	分岐 = RAND:3
LOOP 1

SELECTCASE FALLTYPE(TARGET)
	CASE 1
		PRINTFORMW %CALLNAME:TARGET%たちは\@ ゲームモード() == "新婚モード" ? 朝から濃厚な # 調教そっちのけで \@情事に耽っている……
		CALL EXPUP, "愛情経験", 5
		CALL JUELUP, "恭順", 250
		IF SEX(MASTER) == 1 && SEX(TARGET) == 1
			CALL EXPUP, "ゲイ経験", 5
			CALL EXPUP, "ゲイ経験", 5, MASTER
		ELSEIF SEX(MASTER) == 2 && SEX(TARGET) == 2
			CALL EXPUP, "レズ経験", 5
			CALL EXPUP, "レズ経験", 5, MASTER
		ENDIF
	CASE 2
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の腕を離さずに次の夢へと誘っている……
		CALL EXPUP, "精液経験", 5
		CALL JUELUP, "欲情", 250
		IF SEX(MASTER) == 1 && SEX(TARGET) == 1
			CALL EXPUP, "ゲイ経験", 5
			CALL EXPUP, "ゲイ経験", 5, MASTER
		ELSEIF SEX(MASTER) == 2 && SEX(TARGET) == 2
			CALL EXPUP, "レズ経験", 5
			CALL EXPUP, "レズ経験", 5, MASTER
		ENDIF
	CASE 3
		PRINTFORMW %CALLNAME:TARGET%は今朝も%CALLNAME:MASTER%への忠誠を誓っている……
		CALL EXPUP, "主人調教経験", 20
		CALL JUELUP, "屈服", 250
		IF SEX(MASTER) == 1 && SEX(TARGET) == 1
			CALL EXPUP, "ゲイ経験", 5
			CALL EXPUP, "ゲイ経験", 5, MASTER
		ELSEIF SEX(MASTER) == 2 && SEX(TARGET) == 2
			CALL EXPUP, "レズ経験", 5
			CALL EXPUP, "レズ経験", 5, MASTER
		ENDIF
ENDSELECT

CALL CHECKDRAWLINEW

@EVENT_朝フェラ
#DIM DYNAMIC フェラ回数
#DIM DYNAMIC 射精回数

CALL CHECKDRAWLINE
CALL KOJO_MESSAGE_EVENT, "朝フェラ"

;ここからフェラ回数の計算
フェラ回数 = 相性値(TARGET, MASTER)/100
フェラ回数 += ABL:精液中毒

SIF TALENT:汚臭鈍感
	フェラ回数++
SIF TALENT:汚臭敏感
	フェラ回数--
SIF TALENT:潔癖症
	フェラ回数--
SIF TALENT:献身的
	フェラ回数++
SIF TALENT:淫乱
	フェラ回数 += 2
SIF TALENT:淫魔
	フェラ回数 += 3

SIF フェラ回数 < 1
	フェラ回数 = 1

;ここから射精回数の計算
;ただしどれだけ多くてもフェラ回数よりは多く射精しない
射精回数 = 10*フェラ回数;整数しか扱えない関係で一旦10倍に
SIF !TALENT:汚れ無視
	TIMES 射精回数, 0.90
SIF !TALENT:舌使い
	TIMES 射精回数, 0.80
SIF !TALENT:MASTER:絶倫
	TIMES 射精回数, 0.60
射精回数 *= ABL:技巧
射精回数 /= 30
射精回数 *= (10 + ABL:MASTER:Ｃ感覚)
射精回数 /= 10
射精回数 /= 10
射精回数 = LIMIT(射精回数, 1, フェラ回数)
IF TALENT:MASTER:絶倫
	射精回数 = MIN(射精回数, 15)
ELSE
	射精回数 = MIN(射精回数, 10)
ENDIF

PRINTFORMW 今朝は%CALLNAME:TARGET%のフェラで{射精回数}回射精した

CALL EXPUP, "フェラ経験", フェラ回数
CALL EXPUP, "精液経験", 射精回数
CALL JUELUP, "恭順", フェラ回数*100
CALL JUELUP, "屈服", フェラ回数*30
CALL JUELUP, "習得", フェラ回数*40

CALL EXPUP, "射精経験", 射精回数, MASTER
CALL EXPUP, "絶頂経験", 射精回数, MASTER
WAIT

CFLAG:朝フェラ経験 += 1
CFLAG:MASTER:朝フェラ経験 += 1
CFLAG:口で射精させた経験 += 射精回数
SIF ISSLIME(MASTER)
	CALL スライム度増加, 0, TARGET, 5
IF CFLAG:TARGET:スライム度 >= 30 && ISSLIME(MASTER) && !ISSLIME(TARGET)
	LOADGLOBAL
	スライム化イベント回数++
	SAVEGLOBAL
	SIF スライム化イベント回数 >= 10
		CALL 実績解除, "そして世界は粘液に満ちた"
	TALENT:スライム = 1
	CSTR:種族 = スライム
ENDIF

@EVENT_朝パイズリ
#DIM DYNAMIC パイズリカウント
#DIM DYNAMIC 射精回数

CALL CHECKDRAWLINE
CALL KOJO_MESSAGE_EVENT, "朝パイズリ"

;ここからパイズリカウントの計算
パイズリカウント = 相性値(TARGET, MASTER)/75
パイズリカウント += ABL:従順/2
パイズリカウント += ABL:奉仕精神/2
パイズリカウント += ABL:精液中毒/3
パイズリカウント += ABL:Ｂ感覚/10

SIF TALENT:汚臭鈍感
	パイズリカウント++
SIF TALENT:汚臭敏感
	パイズリカウント--
SIF TALENT:潔癖症
	パイズリカウント--
SIF TALENT:献身的
	パイズリカウント += 2
SIF TALENT:淫乱
	パイズリカウント += 2
SIF TALENT:淫魔
	パイズリカウント += 3
SIF TALENT:淫乳
	パイズリカウント += 3
SIF TALENT:恋慕
	パイズリカウント += 4
SIF TALENT:親愛
	パイズリカウント += 5

SIF MOD:朝パイズリ特化 && ゲームモード() != "段位認定"
	TIMES パイズリカウント, 1.30

SIF パイズリカウント < 1
	パイズリカウント = 1

;ここから射精回数の計算
;ただしどれだけ多くてもパイズリカウントよりは多く射精しない
射精回数 = 100*パイズリカウント;整数しか扱えない関係で一旦100倍に
SELECTCASE TALENT:バストサイズ
	CASE 1
		TIMES 射精回数, 0.40
	CASE 2
		TIMES 射精回数, 0.70
	CASE 3
		TIMES 射精回数, 1.00
	CASE 4
		TIMES 射精回数, 1.20
	CASE 5
		TIMES 射精回数, 1.40
	CASE 6
		TIMES 射精回数, 1.60
	CASE 7
		TIMES 射精回数, 1.80
	CASE IS >= 8
		TIMES 射精回数, 2.00
ENDSELECT
SIF !TALENT:汚れ無視
	TIMES 射精回数, 0.90
SIF !TALENT:舌使い
	TIMES 射精回数, 0.80
SIF !TALENT:MASTER:絶倫
	TIMES 射精回数, 0.60
射精回数 *= (10 + ABL:MASTER:Ｃ感覚)
射精回数 /= 10
射精回数 *= (600 + MIN(EXP:パイズリ経験, 1000))
射精回数 /= 800
SELECTCASE ABL:技巧
	CASE 1
		TIMES 射精回数, 0.01
	CASE 2
		TIMES 射精回数, 0.03
	CASE 3
		TIMES 射精回数, 0.06
	CASE 4
		TIMES 射精回数, 0.10
	CASE 5
		TIMES 射精回数, 0.15
	CASE 6
		TIMES 射精回数, 0.21
	CASE 7
		TIMES 射精回数, 0.28
	CASE 8
		TIMES 射精回数, 0.36
	CASE 9
		TIMES 射精回数, 0.45
	CASE IS >= 10
		TIMES 射精回数, 0.55
ENDSELECT
射精回数 /= 100
射精回数 = LIMIT(射精回数, 1, パイズリカウント)
IF !MOD:朝パイズリ特化 || ゲームモード() == "段位認定"
	IF TALENT:MASTER:絶倫
		射精回数 = MIN(射精回数, 15)
	ELSE
		射精回数 = MIN(射精回数, 10)
	ENDIF
ENDIF

PRINTFORMW その後も%CALLNAME:TARGET%のパイズリを楽しみ、今朝は{射精回数}回射精した

CALL EXPUP, "パイズリ経験", パイズリカウント
SIF TALENT:バストサイズ >= 3 && パイズリカウント >= 2
	CALL EXPUP, "フェラ経験", パイズリカウント/2
CALL EXPUP, "精液経験", 射精回数
IF MOD:パイズリ奉仕特化 && ゲームモード() != "段位認定"
	CALL JUELUP, "恭順", パイズリカウント*110
	CALL JUELUP, "屈服", パイズリカウント*33
	CALL JUELUP, "習得", パイズリカウント*44
ELSE
	CALL JUELUP, "快Ｂ", パイズリカウント*5
	CALL JUELUP, "恭順", パイズリカウント*100
	CALL JUELUP, "屈服", パイズリカウント*30
	CALL JUELUP, "習得", パイズリカウント*40
ENDIF

CALL EXPUP, "射精経験", 射精回数, MASTER
CALL EXPUP, "絶頂経験", 射精回数, MASTER
WAIT

CFLAG:朝パイズリ経験 += 1
CFLAG:MASTER:朝パイズリ経験 += 1
CFLAG:胸で射精させた経験 += 射精回数
パイズリ回数:TARGET:(NOF(PLAYER)) += パイズリカウント
挟射回数:TARGET:(NOF(PLAYER)) += 射精回数
SIF ISSLIME(MASTER)
	CALL スライム度増加, 0, TARGET, 5
IF CFLAG:TARGET:スライム度 >= 30 && ISSLIME(MASTER) && !ISSLIME(TARGET)
	LOADGLOBAL
	スライム化イベント回数++
	SAVEGLOBAL
	SIF スライム化イベント回数 >= 10
		CALL 実績解除, "そして世界は粘液に満ちた"
	TALENT:スライム = 1
	CSTR:種族 = スライム
ENDIF

@EVENT_朝騎乗位
#DIM DYNAMIC CHK_V;1ならＶ、0ならＡ
#DIM DYNAMIC CHK_A
#DIM DYNAMIC プレイ回数
#DIM DYNAMIC 射精回数

CALL CHECKDRAWLINE

;どこの穴でするか
IF ABL:Ｖ感覚 >= 3 && VAGINA(TARGET) && !処女膜(TARGET)
	CHK_V = 1
	TFLAG:主人挿入箇所 = 1
ELSE
	CHK_V = 0
	TFLAG:主人挿入箇所 = 2
ENDIF
;Ａ感覚3以上ならＶとＡの感度で乱数分岐する
IF ABL:Ａ感覚 >= 3 && CHK_V
	IF (RAND:(ABL:Ｖ感覚+ABL:Ａ感覚))+1 > ABL:Ｖ感覚
		CHK_V = 0
		TFLAG:主人挿入箇所 = 2
	ENDIF
ENDIF

CALL KOJO_MESSAGE_EVENT, "朝騎乗位"

;何回するか
プレイ回数 = 相性値(TARGET, MASTER)/60
プレイ回数 += ABL:性交中毒
プレイ回数 += ABL:精液中毒/2
プレイ回数 += ABL:奉仕精神/4
SIF TALENT:献身的
	プレイ回数++
SIF TALENT:淫乱
	プレイ回数 += 2
SIF TALENT:淫魔
	プレイ回数 += 3
IF CHK_V
	IF TALENT:淫膣
		プレイ回数 += 5
		プレイ回数 += ABL:Ｖ感覚/10
	ENDIF
ELSE
	IF TALENT:淫尻
		プレイ回数 += 5
		プレイ回数 += ABL:Ａ感覚/10
	ENDIF
ENDIF
SIF プレイ回数 < 1
	プレイ回数 = 1

;何発出したか
射精回数 = 10*プレイ回数;小数を扱うため一旦10倍
SIF !TALENT:MASTER:絶倫
	TIMES 射精回数, 0.60
SIF TALENT:体格 <= 2
	TIMES 射精回数, 1.20
射精回数 *= ABL:技巧
射精回数 /= 30
射精回数 *= (10 + ABL:MASTER:Ｃ感覚)
射精回数 /= 10
射精回数 /= 10
射精回数 = LIMIT(射精回数, 1, プレイ回数)
IF TALENT:MASTER:絶倫
	射精回数 = MIN(射精回数, 15)
ELSE
	射精回数 = MIN(射精回数, 10)
ENDIF

PRINTFORM %CALLNAME:MASTER%のペニスを咥え込んで離さない%CALLNAME:TARGET%は、卑猥な腰使いで
SIF 射精回数 > 1
	PRINTFORM {射精回数}回ほど
PRINTW 精液を搾り取っていった

CALL EXPUP, "性交経験", プレイ回数
経験回数:MASTER:(NOF(TARGET)) += プレイ回数
経験回数:TARGET:(NOF(MASTER)) += プレイ回数

CALL EXPUP, "精液経験", 射精回数

IF CHK_V
	CALL EXPUP, "Ｖ経験", プレイ回数
	CALL EXPUP, "膣射経験", 射精回数
	CALL JUELUP, "快Ｖ", プレイ回数*100
	中出し回数:MASTER:(NOF(TARGET)) += 射精回数
	中出され回数:(NOF(MASTER)) += 射精回数
	CFLAG:騎乗位で射精させた経験 += 射精回数
	IF PENIS(TARGET)
		CALL EXPUP, "射精経験", 1
		CALL EXPUP, "絶頂経験", 1
		CFLAG:膣を責められて射精した経験++
		CFLAG:犯されて射精した経験++
	ENDIF
	PREG:主人膣内射精 += 射精回数
	CALL IN_VAGINA
ELSE
	CALL EXPUP, "Ａ経験", プレイ回数
	CALL JUELUP, "快Ａ", プレイ回数*100
	アナル中出し回数:MASTER:(NOF(TARGET)) += 射精回数
	アナル中出され回数:(NOF(MASTER)) += 射精回数
	CFLAG:アナル騎乗位で射精させた経験 += 射精回数
	IF PENIS(TARGET)
		CALL EXPUP, "射精経験", 1
		CALL EXPUP, "絶頂経験", 1
		CFLAG:アナルを責められて射精した経験++
		CFLAG:アナルを犯されて射精した経験++
	ENDIF
ENDIF

CALL JUELUP, "恭順", プレイ回数*100
CALL JUELUP, "屈服", プレイ回数*30
CALL JUELUP, "習得", プレイ回数*40

CALL EXPUP, "性交経験", プレイ回数, MASTER
CALL EXPUP, "射精経験", 射精回数, MASTER
CALL EXPUP, "絶頂経験", 射精回数, MASTER
WAIT

CFLAG:朝騎乗位経験++
CFLAG:MASTER:朝騎乗位経験++
SIF ISSLIME(MASTER)
	CALL スライム度増加, 0, TARGET, 5
IF CFLAG:TARGET:スライム度 >= 30 && ISSLIME(MASTER) && !ISSLIME(TARGET)
	LOADGLOBAL
	スライム化イベント回数++
	SAVEGLOBAL
	SIF スライム化イベント回数 >= 10
		CALL 実績解除, "そして世界は粘液に満ちた"
	TALENT:スライム = 1
	CSTR:種族 = スライム
ENDIF

;-------------------------------------------------
;調教後に起こるイベント
;-------------------------------------------------
@SELF_CHECK
#DIM DYNAMIC 不発確率
;調教者がペニス持ちで、調教者の射精残数が0の場合は発生しない
;SIF PENIS(MASTER) && CFLAG:MASTER:残数 == 0
;	RETURN 0

;抑制中なら発生しない
SIF CFLAG:奉仕イベント抑制
	RETURN 0

SIF CFLAG:使用不可
	RETURN 0

SIF CFLAG:死亡
	RETURN 0

;アニマルだと発生しない
SIF TALENT:アニマル
	RETURN 0

;対象が霊体かつ主人に霊感もネクロマンサーも無いと発生しない
SIF TALENT:霊体 && !TALENT:MASTER:霊感 && !TALENT:MASTER:ネクロマンサー
	RETURN 0

;調教後3P、調教後セックスどちらか一つ
;対象の体力500以上必要、強精神剤の反動が起こる時は無効
SIF BASE:体力 < 500 || TEQUIP:強精神薬 > 2
	GOTO ELSE
;助手がいる場合は3P判定 対象と助手が陥落済み、対象と助手間の相性がそれぞれ100より↑
IF ASSI >= 0 && 陥落(TARGET) && 陥落(ASSI) && 相性値(TARGET, ASSI) > 100 && 相性値(ASSI, TARGET) > 100
	;どちらかが貞操帯つけてると発生しない
	SIF CFLAG:貞操帯 || CFLAG:ASSI:貞操帯
		GOTO ELSE
	;メス度10以上でも発生しない
	SIF ABL:メス度 >= 10 || ABL:ASSI:メス度 >= 10
		GOTO ELSE
	;去勢済みだと発生しない（暫定）
	SIF TALENT:去勢済み || TALENT:ASSI:去勢済み
		GOTO ELSE
	;ただし出産トラウマがある場合は発生しない時がある
	不発確率 = CFLAG:出産トラウマ+CFLAG:ASSI:出産トラウマ
	;RAND:(合計値/10+2) <= (合計値/10)
	SIF !(不発確率 && RAND:(不発確率/10+2) <= 不発確率/10)
		CALL EVENT_CHECK, "調教後3P"
;ふたりエッチ判定 対象が陥落済み
ELSEIF 陥落(TARGET) && BASE:体力 >= 500
	;貞操帯装着中は発生しない
	SIF CFLAG:貞操帯
		GOTO ELSE
	;出産トラウマがある場合はry 式は上と同じ
	不発確率 = CFLAG:出産トラウマ
	SIF !(不発確率 && RAND:(不発確率/10+2) <= 不発確率/10)
		CALL EVENT_CHECK, "調教後セックス"
ENDIF

$ELSE

;強精神薬の反動、調教後搾乳、調教後自慰。どれか一つ
IF TEQUIP:強精神薬 || TFLAG:ラブキャンセーブユー
	CALL EVENT_CHECK, "強精神薬の反動"
ELSEIF ABL:Ｂ感覚 >= 4 && (ABL:噴乳中毒 > ABL:自慰中毒) && TALENT:母乳体質
	CALL EVENT_CHECK, "調教後搾乳"
ELSEIF ABL:自慰中毒 >= 3 && !CFLAG:貞操帯
	CALL EVENT_CHECK, "調教後自慰"
ENDIF

;調教後夜這い
SIF 陥落(TARGET) && ターン == "夜"
	CALL EVENT_CHECK, "調教後夜這い"

;調教後3P
@EVENT_調教後3P
#DIM DYNAMIC プレイ回数
#DIM DYNAMIC 射精回数基礎値, 3;0,1,2でMASTER,ASSI,TARGET
#DIM DYNAMIC 射精回数, 3, 3;射精回数:(出す人):(出される人)
#DIM DYNAMIC V2
#DIM DYNAMIC 奉仕ポイント
#DIM DYNAMIC 受けＡ

射精回数基礎値:0 = CFLAG:MASTER:残数/5+1
射精回数基礎値:1 = CFLAG:ASSI:残数/5+1
射精回数基礎値:2 = CFLAG:TARGET:残数/5+1

;イベント口上呼び出し
CALL CHECKDRAWLINE
CALL KOJO_MESSAGE_EVENT, "調教後3P"
奉仕ポイント = 1

;対象：主人か助手に同性がいればレズ中毒、ゲイ中毒を見る
IF SEX(TARGET) == SEX(MASTER) || SEX(TARGET) == SEX(ASSI)
	IF SEX(TARGET) == 1
		SELECTCASE ABL:ゲイ中毒
			CASE 1
				奉仕ポイント += 1
			CASE 2
				奉仕ポイント += 2
			CASE 3
				奉仕ポイント += 3
			CASE 4
				奉仕ポイント += 5
			CASE IS >= 5
				奉仕ポイント += 7
		ENDSELECT
	ELSEIF SEX(TARGET) == 2
		SELECTCASE ABL:レズ中毒
			CASE 1
				奉仕ポイント += 1
			CASE 2
				奉仕ポイント += 2
			CASE 3
				奉仕ポイント += 3
			CASE 4
				奉仕ポイント += 5
			CASE IS >= 5
				奉仕ポイント += 7
		ENDSELECT
	ENDIF
;いなければ奉仕精神を見る
ELSE
	SELECTCASE ABL:奉仕精神
		CASE 1 TO 4
			奉仕ポイント += ABL:奉仕精神+1
		CASE IS >= 5
			奉仕ポイント += 6
	ENDSELECT
ENDIF

;助手：奴隷が同性ならレズ中毒、ゲイ中毒を見る
IF SEX(ASSI) == SEX(TARGET)
