		SELECTCASE ABL:ゲイ中毒
			CASE 1
				奉仕ポイント += 1
			CASE 2
				奉仕ポイント += 2
			CASE 3
				奉仕ポイント += 5
			CASE 4
				奉仕ポイント += 8
			CASE IS >= 5
				奉仕ポイント += 13
		ENDSELECT
	ELSEIF SEX(TARGET) == 2
		SELECTCASE ABL:レズ中毒
			CASE 1
				奉仕ポイント += 1
			CASE 2
				奉仕ポイント += 2
			CASE 3
				奉仕ポイント += 5
			CASE 4
				奉仕ポイント += 8
			CASE IS >= 5
				奉仕ポイント += 13
		ENDSELECT
	ENDIF
