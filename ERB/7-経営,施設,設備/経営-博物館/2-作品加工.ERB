SELECTCASE 作品種類
	CASE "白骨死体", "肋骨除去"
		RETURNF COLOR_FROMNAME("ivory")
	CASE "ホルマリン漬け", "臓器摘出"
		RETURNF COLOR_FROMNAME("yellowgreen")
	CASE "透明標本"
		RETURNF COLOR_FROMNAME("cyan")
	CASE "人形"
		RETURNF COLOR_FROMNAME("gray")
	CASE "性器リソース"
		RETURNF COLOR_FROMNAME("wheat")
	;汎用は剥製と同じにしておく
	CASEELSE
		RETURNF COLOR_FROMNAME("red")
ENDSELECT


@加工技術, 対象
#FUNCTION
#DIM DYNAMIC 対象
#DIM DYNAMIC PT
#DIM DYNAMIC 名前文字数
#DIM DYNAMIC LCOUNT

PT += LIMIT(ABL:対象:医療技能, 0, 10)
PT += LIMIT(ABL:対象:工作技能/2, 0, 5)

FOR LCOUNT, 0, CHARANUM
	SIF CSTR:LCOUNT:配属 == "博物館"
		名前文字数 = MAX(名前文字数, STRLENS(NAMEDISP(LCOUNT)))
NEXT

PRINTFORM 　%NAMEDISP(対象), 名前文字数% 医療技能Lv{ABL:対象:医療技能}(+{ABL:対象:医療技能}) 工作技能Lv{ABL:対象:工作技能}(+{ABL:対象:工作技能/2}) 
IF TALENT:対象:ネクロマンサー
	PT += 2
	PRINT ネクロマンサー(+2) 
ENDIF
IF TALENT:対象:倒錯的
	PT += 1
	PRINT 倒錯的(+1) 
ENDIF
IF TALENT:対象:禁断の知識
	PT += 1
	PRINT 禁断の知識(+1) 
ENDIF
IF TALENT:対象:戦士
	PT += 1
	PRINT 戦士(+1) 
ENDIF
PRINTFORML 合計{PT}
RETURNF PT


@加工技術合計
#FUNCTION
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC PT
SKIPDISP 1
FOR LCOUNT, 0, CHARANUM
	SIF CSTR:LCOUNT:配属 != "博物館" || !CFLAG:LCOUNT:配属完了
		CONTINUE
	PT += 加工技術(LCOUNT)
NEXT
SKIPDISP 0
RETURNF PT
