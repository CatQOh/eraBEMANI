			;PRINT_IMGでPRINTC系命令使うとエラーになるので疑似再現
			IF OPTION:パセリ表示
				単位文字数 = 2
			ELSEIF 通貨単位 != ""
				単位文字数 = STRLENS(通貨単位)
			ELSE
				単位文字数 = 1
			ENDIF
			;＄の分2文字と通貨単位分
			FOR LCOUNT:1, 0, 28-(STRLENS(表示用)-2)-単位文字数
				PRINT  
			NEXT
		ENDIF
	ENDIF
	表示数++
	IF 表示数 == 4
		CALL NEWLINE
		表示数 = 0
	ENDIF
NEXT
