DRAWLINE
表示数 = 0
FOR LCOUNT, GETNUM(BRITEM, "ローター"), GETNUM(BRITEM, "お風呂場")
	SIF ERDNAME(BRITEM, LCOUNT) == ""
		CONTINUE
	SIF 表示数%3 == 0
		PRINT 　
	PRINTFORM [{LCOUNT, 2}] %ERDNAME(BRITEM, LCOUNT), 15, LEFT%
