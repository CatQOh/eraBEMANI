			適性 += (ABL:売春キャラ:マゾっ気+1)/2

			収入 += (収入/2)*(ABL:売春キャラ:サドっ気+1)
			適性 += (ABL:売春キャラ:サドっ気+1)/2
		ELSE
			収入 += (収入/10)*ABL:売春キャラ:従順
			適性 += ABL:売春キャラ:従順/3+1

			収入 += (収入/10)*ABL:売春キャラ:欲望
			適性 += ABL:売春キャラ:欲望/5

			収入 += (収入/10)*ABL:売春キャラ:技巧
			適性 += ABL:売春キャラ:技巧/5

			IF VAGINA(売春キャラ)
				収入 += (収入/10)*ABL:売春キャラ:Ｖ感覚
				適性 += ABL:売春キャラ:Ｖ感覚/5
			ENDIF

			収入 += (収入/10)*ABL:売春キャラ:Ａ感覚
			適性 += ABL:売春キャラ:Ａ感覚/5

			収入 += (収入/5)*ABL:売春キャラ:露出癖
			適性 += ABL:売春キャラ:露出癖/5

			収入 += (収入/2)*ABL:売春キャラ:マゾっ気
			適性 += ABL:売春キャラ:マゾっ気/2

			収入 += (収入/2)*ABL:売春キャラ:サドっ気
			適性 += ABL:売春キャラ:サドっ気/2
		ENDIF
	;ルナティック
	CASE 4
		;マイナス
		不適性 = 20
		;崩壊が有利
		IF TALENT:売春キャラ:崩壊
			収入 += 2000
			適性++
		ENDIF
		FOR LCOUNT, 0, VARSIZE("TALENT")
			SIF !TALENT:売春キャラ:LCOUNT
				CONTINUE
			SELECTCASE TALENTNAME:LCOUNT
				CASE "狂気"
					収入 += 1000
					適性++
				CASE "小人体型"
					収入 += 1000
				CASE "倒錯的", "妄信", "無機物", "リミットバースト"
					収入 += 500
					適性++
				CASE "ふたなり", "弱味", "ネクロマンサー"
					収入 += 300
					適性++
				CASE "性器破損", "吸血鬼"
					収入 += 500
				CASE "狐", "幼稚"
					収入 += 300
				CASE "ファーリー", "夢喰い", "ドラゴン使い"
					収入 += 200
				CASE "バストサイズ"
					SIF TALENT:売春キャラ:バストサイズ >= 6
						収入 += 200
				CASE "羽"
					収入 += 100
				;CASE "幼児／幼児退行"
				;	収入 += 300
			ENDSELECT
		NEXT
		;娼館レベルに応じて変動
		収入 = 収入*BR:レベル/50
		;ここから能力で倍率計算
		IF 統率者("娼館", CFLAG:売春キャラ:娼館部屋番号) && !TALENT:売春キャラ:統率
			収入 += (収入/10)*(ABL:売春キャラ:従順+1)
			適性 += (ABL:売春キャラ:従順+1)/3

			収入 += (収入/10)*(ABL:売春キャラ:欲望+1)
			適性 += (ABL:売春キャラ:欲望+1)/3

			収入 += (収入/10)*(ABL:売春キャラ:技巧+1)
			適性 += (ABL:売春キャラ:技巧+1)/3
		ELSE
			収入 += (収入/10)*ABL:売春キャラ:従順
			適性 += ABL:売春キャラ:従順/3

			収入 += (収入/10)*ABL:売春キャラ:欲望
			適性 += ABL:売春キャラ:欲望/3

			収入 += (収入/10)*ABL:売春キャラ:技巧
			適性 += ABL:売春キャラ:技巧/3
		ENDIF
ENDSELECT
;体力気力消費計算
消耗量 = MAX(不適性-適性, 1)
SELECTCASE BR:方針
	;ソフト
	CASE 1
		BASE:売春キャラ:体力 -= 消耗量*50
		BASE:売春キャラ:気力 -= 消耗量*30
	;ハード
	CASE 2
		BASE:売春キャラ:体力 -= 消耗量*100
		BASE:売春キャラ:気力 -= 消耗量*50
	;SM
	CASE 3
		BASE:売春キャラ:体力 -= 消耗量*300
		BASE:売春キャラ:気力 -= 消耗量*70
	;狂気
	CASE 4
		BASE:売春キャラ:体力 -= 消耗量*200
		BASE:売春キャラ:気力 -= 消耗量*150
ENDSELECT
SIF BASE:売春キャラ:体力 <= 0
	BASE:売春キャラ:体力 = 1

;最低1000は保証
CALL SETMAX, 収入, 1000

収入 += 収入*BR:レベル/100

SELECTCASE BR:方針
	CASE 1
		収入 += ソフトポイント*10
	CASE 2
		収入 += ハードポイント*10
	CASE 3
		収入 += SMポイント*10
	CASE 4
		収入 += 狂気ポイント*10
ENDSELECT

;お風呂場があるとストレス溜まりづらくなる
IF BRITEM:お風呂場
	CFLAG:売春キャラ:ストレス値 += MAX(不適性-適性, 0)
ELSE
	CFLAG:売春キャラ:ストレス値 += MAX((不適性-適性)*2, 0)
ENDIF
;換気口あるとさらに軽減
SIF BRITEM:換気口
	CFLAG:売春キャラ:ストレス値 -= 2
;実務経験を加算
EXP:売春キャラ:実務経験++

;適性-不適性をモチベーションに加算
CFLAG:売春キャラ:モチベーション = LIMIT(CFLAG:売春キャラ:モチベーション+適性-不適性, 1, 100)

;適性*10が習得に入る
SIF 適性 > 0
	JUEL:売春キャラ:習得 += 適性*10

RETURN 収入

