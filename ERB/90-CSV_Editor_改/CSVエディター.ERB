					表示用変数 = %CSTR:@"%CSTRNAME:LCOUNT%{服装プリセット}"%
				ELSE
					表示用変数 = %CSTR:LCOUNT%
				ENDIF
			ENDIF
			PRINTBUTTON @"%@"　[%CSTRNAME:LCOUNT%]", 14, LEFT%:%表示用変数% ", @"%CSTRNAME:LCOUNT%{服装プリセット}"
			IF 表示用変数 != "着衣不可"
				SELECTCASE CSTRNAME:LCOUNT
					CASE "下半身上着"
						IF 服装プリセット > 1
							PRINTBUTTON @"[挿入可フラグ]:上着を着たまま挿入\@ CFLAG:@"挿入可{服装プリセット}" == 1 ? 可能 # 不可能 \@", @"挿入可{服装プリセット}"
