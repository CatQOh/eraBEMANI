							PRINTBUTTON @"[着衣パイズリフラグ]:%表示用変数%", @"%SUBSTRINGU(CSTRNAME:LCOUNT,3, 2)%パイズリ{服装プリセット}"
						ENDIF
						PRINTS " "
				ENDSELECT
			ENDIF
