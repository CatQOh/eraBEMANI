				PRINTFORMW 「あぁん♪ 出ちゃったね。それもこんなのいっぱい♪ 
			ENDIF
	ENDSELECT
ELSEIF ASSI > 0 && NOWEX:ASSI:射精
	SELECTCASE SELECTCOMS
		CASE "ペットに犯させる"
			;地の文表示関数 地の文を表示させたくない場合はコメントアウト
			TRYCALL SHOOT_MESSAGE
