            ENDSELECT
		CASE "正常位アナルさせる", "後背位アナルさせる"
        ;汎用陥落セリフから流用
            SELECTCASE RAND:2
                CASE 0
                    PRINTFORMW 「俺もイキそうだ…うっ、くぅ…！」
                CASE 1
                    PRINTFORMW 「出る…！はぁ…はっ…」
            ENDSELECT
		CASE "騎乗位アナル手淫", "後背位アナル手淫"
