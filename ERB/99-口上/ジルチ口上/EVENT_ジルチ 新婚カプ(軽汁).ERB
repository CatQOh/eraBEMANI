	SELECTCASE SELECTCOMS
		CASE "アナルバイブ", "アナルビーズ"
            TRYCALL EX_MESSAGE
            SELECTCASE ABL:Ａ感覚
                ;Ａ感覚Lv4以下
                CASE IS <= 4
                    PRINTFORMW 「あ、ぐぅ…！ま、まだ痛気持ちいいというか…」
                    PRINTFORMW 「い、一応イってる…………」
                ;Ａ感覚Lv5以上
                CASE IS >= 5
                    PRINTFORMW 「ふぅ…ぁああ……はぁああ…！」
                    PRINTFORMW 「イイっ…すごくイイっ……！」
            ENDSELECT
		CASE "フェラする"
            SELECTCASE RAND:3
                CASE 0
                    PRINTFORMW 「ふぅ…っ、ケイナ…………」
                    PRINTFORMW 「ん、大丈夫…飲み切れたから」
                CASE 1
                    PRINTFORMW 「はぁっ、イったのにまだしゃぶり続けてるのかよ…」
                    PRINTFORMW 「つうかフェラしてる時が楽しそうだな、お前」
                CASE 2
                    PRINTFORMW 「うぁ……ケイナ、出てるのに舌先で遊ぶな…っ！」
            ENDSELECT
		CASE "手淫する"
            SELECTCASE RAND:4
                CASE 0
                    PRINTFORMW 「あ～あ、手ぇ汚すほど出せたなぁ…ん…っ」
                    PRINTFORMW 「おいおい…俺の精液を美味そうに舐めて…味良くねぇだろ…」
