                    PRINTFORMW 「っはぁ……お前…絶対サドっ気あるぞ」
                    PRINTFORMW 「さもありなん？あまり自覚しとらんかったけど」
                CASE 2
                    PRINTFORMW 「勢いよく出とるな……余程気持ち良かったん？」
                    PRINTFORMW 「っあ…」
                CASE 3
                    PRINTFORMW 「ケイナ…あぁ…で、出る…っ」
                    PRINTFORMW 「うぁ…出てるのに亀頭扱くな…んっ…」
            ENDSELECT
		CASE  "パイズリする", "素股する"
        ;汎用陥落セリフから流用
            SELECTCASE ABL:Ｃ感覚
                ;Ｃ感覚Lv0
                CASE 0
                    PRINTFORMW 「ん………出そうだ…」
                ;Ｃ感覚Lv1～Lv3
                CASE 1 TO 3
                    PRINTFORMW 「くぅ…この感覚…たまらん…出るっ！」
                ;Ｃ感覚Lv4以上
                CASE IS >= 4
                    PRINTFORMW 「はぁ…くっ…あぁああ…！」
            ENDSELECT
		CASE "正常位アナル", "騎乗位アナル", "後背位アナル", "背面座位アナル", "正面座位アナル"
        ;汎用陥落セリフから流用
