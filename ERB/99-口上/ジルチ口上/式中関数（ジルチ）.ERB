;識汁新婚モード判定
@ZILCH_SHIKI
#FUNCTION
IF ゲームモード() == "新婚モード" && GROUPMATCH("ジルチ", CHARANAMEF(MASTER), CHARANAMEF(TARGET)) && GROUPMATCH("識", CHARANAMEF(MASTER), CHARANAMEF(TARGET))
  RETURNF 1
ELSE
  RETURNF 0
ENDIF

;軽汁新婚モード判定
@ZILCH_KENA
#FUNCTION
IF ゲームモード() == "新婚モード" && GROUPMATCH("ジルチ", CHARANAMEF(MASTER), CHARANAMEF(TARGET)) && GROUPMATCH("光伊 計名", CHARANAMEF(MASTER), CHARANAMEF(TARGET))
  RETURNF 1
ELSE
  RETURNF 0
ENDIF
