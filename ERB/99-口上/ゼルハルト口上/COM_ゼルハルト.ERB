;-------------------------------------------------
@COM_IIDXをプレイする_ゼルハルト
;プレイ回数
SELECTCASE GERHARD:7
	CASE IS <= 50
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「お皿回してみたい！ あははっ、変な音出る！ 十段なうだって！」
			CASE 1
				PRINTFORMW 「ちょっと手が届かなくてやりづらい……台座とか無いの？」
		ENDSELECT
	CASE IS <= 100
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「やっとレベル4くらいが出来るようになったぞ。ねーねー、その落ちてくるのが速くなるやつどうやって使うの？」
			CASE 1
				PRINTFORMW 「見て見て！ レベル3のやつフルコンボした！ すごいでしょ！」
		ENDSELECT
	CASE IS <= 300
		SELECTCASE RAND:2
			CASE 0
				PRINT 「この段位認定ってやつやってみようかな。
				IF FCOM:SELECTCOM == 0
					PRINTFORMW まずは7級でいいよね」
					FCOM:SELECTCOM = 1
				ELSE
					PRINTFORMW 7級はちょっと簡単すぎたから、初段受けてみようかな」
				ENDIF
			CASE 1
				PRINTFORMW 「レベル7とかわけわかんないや。運指？ 緑数字？ なにそれ」
		ENDSELECT
	CASE IS <= 500
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「七段難しすぎ！ ライオンの曲がクリアできないよぅ」
			CASE 1
				PRINTFORMW 「ダブルプレーっていうのやってみようかな。シングルプレー飽きてきたし」
		ENDSELECT
	CASE IS <= 700
		SELECTCASE RAND:2
			CASE 0
				IF FCOM:SELECTCOM <= 1
					PRINTFORMW 「やった！ 初めてレベル12クリアした！ その気になればできるもんだな」
					FCOM:SELECTCOM = 2
				ELSE
					PRINTFORMW 「レベル12のクリアランプも増えてきたなー。難しいやつは今でもわけわかんないけど」
				ENDIF
			CASE 1
				PRINTFORMW 「プレミアムフリーでレベル11くらいの曲にランダムかけるのにハマってるんだ。お前はどのくらいできる？」
		ENDSELECT
	CASE IS <= 1000
		SELECTCASE RAND:2
			CASE 0
				IF FCOM:SELECTCOM <= 2
					PRINTFORMW 「とうとう皆伝受かった！ やったー！ 褒めて褒めて♪」
					FCOM:SELECTCOM = 3
				ELSE
					PRINTFORMW 「皆伝より難しいやつって無いの？ BMS？ なにそれ」
				ENDIF
			CASE 1
				PRINTFORMW 「ダブルプレーもがんばろーっと。お前はそっちのビートマニアで遊びなよ」
		ENDSELECT
	CASE IS > 1000
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「へっへーん、見ろよ、レベル12全曲ハードの僕を！」
				PRINTFORMW 「お前なんてもう相手にならないよ。じゃあ僕はダブルプレーやるからどいて」
			CASE 1
				PRINTFORMW 「EX-H埋めもやっていこうっと。でもフルコンボできるならそれが一番だよね」
		ENDSELECT
ENDSELECT
GERHARD:7++

RETURN 0

