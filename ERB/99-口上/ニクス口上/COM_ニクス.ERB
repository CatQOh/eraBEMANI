;-------------------------------------------------
;eraBEMANI用 コマンド口上テンプレ
;キャラが調教対象の場合に表示する口上はKX1を対象のフルネームに置換
;キャラが主人の場合に表示する口上はKX1をキャラ名、「COM_」を「MASTER_COM_」に置換
;キャラが産まれた子供の場合に表示する口上はKX1を「{父親のキャラ名}×{母親のキャラ名}に置換
;(例:あなたとファウラの間に産まれた子供の場合はKX1→あなた×ファウラ のように)
;-------------------------------------------------
;コマンド実行時のセリフ FCOM:(COM番号)
;-------------------------------------------------
;=================================================
;愛撫系コマンド
;=================================================
;-------------------------------------------------
;愛撫
;-------------------------------------------------
@COM_愛撫_ニクス
;初めて
IF FCOM:SELECTCOM == 0 && !CFLAG:陥落
	PRINTFORMW 「やめろ、気安く触んな」
	PRINTFORMW 「俺に関わるとろくでもない事が起きるぞ」
	FCOM:SELECTCOM = 1
;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		PRINT 「うみゃ～ん
		SELECTCASE RAND:2
			CASE 0
				PRINTW 、ゴロゴロ……」
				PRINTFORMW 「やっぱりお前の手はあったけぇな」
			CASE 1
				PRINTW …撫で過ぎだ、この野郎」
		ENDSELECT
	;屈服刻印
	ELSE
		SELECTCASE MARK:屈服刻印
			CASE 2, 3
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「クソッ、調子に乗りやがって…絶対靡かねぇからな」
					CASE 1
						PRINTFORMW 「お前の手は温かいな…」
						PRINTFORMW 「――ハッ！何でもねぇ、忘れろ」
					CASE 2
						PRINTFORMW 「俺は猫じゃねぇつってんだろ、撫でりゃその気になるか……んに…」
				ENDSELECT
			CASEELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「うざってぇ……」
					CASE 1
						PRINTFORMW 「……だからベタベタ触るなって、ぶん殴られてぇのか」
				ENDSELECT
		ENDSELECT
	ENDIF
ENDIF

RETURN 0

;-------------------------------------------------
;フェラする
;-------------------------------------------------
@COM_フェラする_ニクス
;初めて
IF FCOM:SELECTCOM == 0 && !CFLAG:陥落
	SELECTCASE SEX(PLAYER)
		CASE 1
			PRINTFORMW 「勝手にしゃぶってんじゃねぇ、お前……」
		CASE 2
			PRINTFORMW 「くっ、やめろ……こんのアマ…っ…」
	ENDSELECT
	FCOM:SELECTCOM = 1
;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		SELECTCASE RAND:2
			CASE 0
				SELECTCASE SEX(PLAYER)
					CASE 1
						PRINTFORMW 「は…ぅ…マジ気持ちいい、どうするよお前……俺が下でも構わないが」
					CASE 2
						PRINTFORMW 「最高だ…そのまま咥えてろよ」
				ENDSELECT
			CASE 1
				PRINTFORMW 「んっ、うぅ……いいぜ、そのまま……っ」
		ENDSELECT
	;屈服刻印
	ELSE
		SELECTCASE MARK:屈服刻印
			CASE 1
				PRINTFORMW 「くっ……クソっ、咥えるんじゃねぇ……！」
			CASE 2
				PRINTFORMW 「うぅ、んぁ……やめろ、このっ！」
			CASE 3
				PRINT 「はぁ…うっ、
				SELECTCASE SEX(PLAYER)
					CASE 1
						PRINTW 男同士でも流石によぉ……っ」
					CASE 2
						PRINTW 好きでしゃぶってるのか…」
				ENDSELECT
			CASEELSE
				PRINTFORMW 「げぇ、勝手に咥えるな！」
		ENDSELECT
	ENDIF
ENDIF

RETURN 0

;-------------------------------------------------
;手淫する
;-------------------------------------------------
@COM_手淫する_ニクス
;初めて
IF FCOM:SELECTCOM == 0 && !CFLAG:陥落
	PRINT 「他人のを触るんじゃねぇって、
	SELECTCASE SEX(PLAYER)
		CASE 1
			PRINTW 死ねっ！この野郎！」
		CASE 2
			PRINTW 離せ！」
	ENDSELECT
	FCOM:SELECTCOM = 1
	;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		SELECTCASE CHARANAMEF(PLAYER)
			CASE "神崎 士朗"
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「あは…っ、やめ…ああ…っ、んー…」
						PRINTFORMW 「口で嫌がっても腰、動いてるぞ」
					CASE 1
						IF PALAM:勃起 >= PALAMLV:3
							PRINT 「出ちまいそうだから、扱くな…
						ELSE
							PRINT 「うぅ…はぁっ、
						ENDIF
						PRINTW ぁ…んにゃ…ぅ…シロウ…！」
				ENDSELECT
			CASEELSE
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「うぁ……あ、はぁ……」
						PRINTFORMW 「やめっ……ん……」
					CASE 1
						PRINTFORMW 「ん…%CALLNIX%、そこはまだ早ぇ…って、んぁ…あっ……」
					CASEELSE
						PRINTFORMW 「待てよ%CALLNIX%っ、テメェ…あ、にゃ…っ！」
				ENDSELECT
		ENDSELECT
	;屈服刻印
	ELSE
		SELECTCASE MARK:屈服刻印
			CASE 2
				PRINTFORMW 「チィッ、きたねぇ手で扱くな…んっ…」
				PRINTFORMW 「感じてない、って…言ってるだろ……っ」
			CASE 3
				PRINTFORMW 「ふぁ……ダメだ、扱く手を離せよ……」
			CASEELSE
				PRINTFORMW 「くぁっ…自分のでも触ってろ……」
		ENDSELECT
	ENDIF
ENDIF

RETURN 0
;-------------------------------------------------
;アナル舐め
;-------------------------------------------------
@COM_アナル舐め_ニクス
;初めて
IF FCOM:SELECTCOM == 0
	PRINTFORMW 「あ……やめろ、いきなり舐めて来るな！」
	PRINTFORMW 「ここでもケツ舐められるハメになるなんてよ……」
	FCOM:SELECTCOM = 1
;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		IF PREVCOMS == "手淫する"
			PRINTFORMW %CALLNAME:PLAYER%はニクスのペニスを扱きながら下着ごとずりおろして、菊門に舌を這わせた
			PRINTFORMW 前後からの刺激に堪え切れず、ニクスは快感に震え喘いでいる…
		ELSE
			PRINTFORMW 「待てよ……はぁ……っ、俺のアナル舐めてもきたねぇだろ…」
			SELECTCASE CHARANAMEF(PLAYER)
				CASE "神崎 士朗"
					PRINT 「シロウ
				CASEELSE
					PRINTFORM 「%CALLNIX%
			ENDSELECT
			PRINTW 、んっ、あぁ！」
		ENDIF
	;屈服刻印
	ELSE
		SELECTCASE MARK:屈服刻印
			CASE 2, 3
				PRINTFORMW 「お前なぁ、少し拭いたからって丹念に舐め回すな…汚いだろが……っあ、はぁ……」
				PRINTFORMW ニクスからの憎まれ口なぞ、お構いなしに菊門の皺を舐る%CALLNAME:PLAYER%。抵抗もせず文句を言うニクスの身体は正直だ…
			CASEELSE
				;PRINTFORMW 「」
		ENDSELECT
	ENDIF
ENDIF

RETURN 0

;-------------------------------------------------
;アナル愛撫
;-------------------------------------------------
@COM_アナル愛撫_ニクス
;初めて
IF FCOM:SELECTCOM == 0
	PRINTFORMW 「うげっ、無理くり指突っ込むな…」
	FCOM:SELECTCOM = 1
;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		IF TEQUIP:アナルビーズ && TEQUIP:アナルバイブ
			PRINT 「はぁううぅ！やめろ、
			SELECTCASE ABL:Ａ感覚
				CASE IS <= 3
					PRINTW 痛いつってんだろが……」
					PRINTFORMW 「感じてなんかねぇよ、んぁ…っ」
				CASE IS >= 4
					PRINTW 動かすの止めっ、ぁは……んぅ……」
					PRINT 「遊ぶなって……
					SELECTCASE CHARANAMEF(PLAYER)
						CASE "神崎 士朗"
							PRINT シロウ
						CASEELSE
							PRINTFORM %CALLNAME:PLAYER%
					ENDSELECT
					PRINTW ！」
			ENDSELECT
		ELSE
			SELECTCASE ABL:Ａ感覚
				CASE IS <= 3
					PRINTFORMW 「優しくしろよ、って言っても、なんだかこそばゆい…」
				CASE IS >= 4
					PRINTFORMW 「あ、あぅ…んぁ…っ…イイ……も、っと……」
			ENDSELECT
		ENDIF
	;屈服刻印
	ELSE
		SELECTCASE MARK:屈服刻印
			CASE 2, 3
				IF ABL:Ａ感覚 >= 4
					PRINTFORMW 「ひぁっ、あぅ…ぅ、クソッ！テメェの良い様にされてたまるかよ……ぁん！」
				ELSE
					PRINTFORMW 「うっ…うぅ……あまり気持ち良くねぇ…」
				ENDIF
			CASEELSE
				IF ABL:PLAYER:技巧 <= 3
					PRINTFORMW 「ケツの中、グリグリされて気持ちわりぃって…この下手くそ。いきなり指二本突っ込むやつがあるか」
					PRINTFORMW 開発技術が足りていない%CALLNAME:PLAYER%に向けて、ニクスが不服を訴えてくる…
				ELSE
					PRINTFORMW 「あぐ、ぅう……なんか痛ぇ……」
				ENDIF
		ENDSELECT
	ENDIF
ENDIF

RETURN 0

;-------------------------------------------------
;胸共通
;-------------------------------------------------
@胸共通_ニクス
IF CFLAG:陥落
	SELECTCASE RAND:3
		CASE 0
			IF SELECTCOMS == "乳首舐め"
				PRINTFORMW 「はぁうう……っ、何ニヤついてんだ、よせ…んぁ！」
				PRINTFORMW 「俺の乳首を嬉しそうに舐めてやがって、美味しくもねぇってのによぉ……」
			ELSE
				PRINTFORMW 「ふぅう……んっ、ぁ……っ」
			ENDIF
		CASE 1
			PRINTFORMW 「うにゃ、しつこく揉み過ぎだ。別にいいけど」
		CASE 2
			SELECTCASE CHARANAMEF(PLAYER)
				CASE "神崎 士朗"
					PRINT 「シロウ
				CASEELSE
					PRINTFORM 「%CALLNIX%
			ENDSELECT
			PRINTW の手は、ホントあったけぇな…」
	ENDSELECT
ELSE
	SELECTCASE MARK:屈服刻印
		CASE 2, 3
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「う、うっせぇ……お前の撫で方が、やらし過ぎるからだ……っはぁ…」
					PRINTFORMW 「だから…耳元で、変なこと言うな…っ！」
				CASE 1
					PRINT 「あう、
					SELECTCASE ABL:Ｂ感覚
						CASE IS <= 2
							PRINT まだだ…その程度かよ」
						CASE 3
							PRINT ひっ…やめ、んぁ……」
						CASE IS >= 4
							PRINT はぁ……はぁっ、続けるな……ぁん……」
					ENDSELECT
				CASE 2
					PRINTFORMW 「…おもしれー奴だな、そんなに俺の胸揉むの好きかよ」
			ENDSELECT
		CASEELSE
			SELECTCASE RAND:3
				CASE 0
					SELECTCASE ABL:Ｂ感覚
						CASE IS <= 2
							PRINT 「胸揉まれても気持ち良くねぇ、いいから離れろ
						CASE 3
							PRINT 「あ、っく……こんなんで感じてなんか
						CASE IS >= 4
							PRINT 「うぁ…ん…や、やめろ…
					ENDSELECT
					PRINTW …」
				CASE 1
					PRINTFORMW 「うぁ、擦り過ぎだ、乳首痛ってぇ……」
				CASE 2
					PRINTFORMW 「こういうのは嫌いだって、言ってんだろ！」
			ENDSELECT
	ENDSELECT
ENDIF
;-------------------------------------------------
;胸揉み
;-------------------------------------------------
@COM_胸揉み_ニクス
CALL 胸共通_ニクス
RETURN 0
