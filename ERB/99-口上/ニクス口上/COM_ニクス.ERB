@ANALSEX_ニクス
;初めて
IF CFLAG:陥落
	SELECTCASE SELECTCOMS
		CASE "正常位アナル"
			PRINTFORMW 「は…っ、んぅ…んにぁあ、はぅん！」
			PRINT 「あ、
			SELECTCASE CHARANAMEF(PLAYER)
				CASE "神崎 士朗"
					PRINT シロウ
				CASE "神崎 慧靂"
					PRINT エレキ
				CASEELSE
					PRINTFORM %CALLNAME:PLAYER%
			ENDSELECT
			PRINTW …ニヤニヤしやがって…はぁああっ！」
		CASE "後背位アナル", "背面座位アナル"
			PRINT 「んっ、んん…っ…
			SELECTCASE CHARANAMEF(PLAYER)
				CASE "神崎 士朗"
					PRINT シロウ、シロウっ
				CASE "神崎 慧靂"
					PRINT エ、エレキっ
				CASEELSE
					PRINTFORM %CALLNIX%ッ、%CALLNIX%
			ENDSELECT
			PRINTW ……はぁん…」
		CASE "対面座位アナル"
			PRINTFORMW 「お…おい、こんなのって…ずりぃだろ……」
			PRINTFORMW 「んぁ……うぅ…ん！」
		CASEELSE
			PRINTFORMW 「今回はそれでいくのか？」
			PRINTFORMW 「ほらよ…股開いてやっから、遠慮なく犯してくれ」
	ENDSELECT
ELSE
	SELECTCASE SELECTCOMS
		CASE "正常位アナル"
			PRINTFORMW 「どうせ俺の身体なんか、オナホ感覚で抱いてんだろ」
			PRINTFORMW 「細かい事気にしてねぇし…早く済ませろよ」
		CASE "後背位アナル"
			PRINTFORMW 「ケツ貸してくれってか、お前の顔見なくてもいいから楽」
		CASE "騎乗位アナル"
			PRINTFORMW 「はぁ～～、最っ低だな！テメェはよ……」
		CASEELSE
			PRINTFORMW 「……これで満足したら離れろ」
	ENDSELECT
ENDIF

RETURN 0
