;-------------------------------------------------
;1,クンニ
;-------------------------------------------------
;ファウラお姉さんのもふもふした脚に挟まれながらもふもふスーハーぺろぺろしたいので
;恋慕・親愛の場合は「必ず」挟まれるようにしました
@COM_クンニ_ファウラ
#DIM DYNAMIC 地の文分岐
#DIM DYNAMIC 乱数分岐
SIF FCOM:SELECTCOM == 0
	FCOM:SELECTCOM = 1
IF K111CHOCOLATE
	CALL ファウラCKISS(4)
	RETURN 0
ENDIF
;陥落済み
IF 陥落(TARGET)
	SELECTCASE FALLTYPE(TARGET)
		;恋慕・親愛のみ特殊
		CASE 1
			;/*地の文引用その1ここから*/
			PRINTFORM %CALLNAME:TARGET%の
			;着衣中
			IF TEQUIP:下半身上着 || (TEQUIP:全身上着 && CFLAG:着衣併用 != 1)
				PRINTFORM %OVER_GENITAL(TARGET)%を
				IF CFLAG:挿入可
					PRINTFORM 捲り上げ、
				;スク水とか
				ELSEIF CFLAG:着衣併用 == 2
					PRINTFORM ずらし、
				ELSE
					PRINTFORM 降ろし、
				ENDIF
			ENDIF
			;同じ形でもう一度使うので保存
			SELECTCASE RAND:3
				CASE 0
					PRINTFORM 獣特有の生臭さを放つ膣に舌を突き入れ
					地の文分岐 = 0
				CASE 1
					PRINTFORM \@ PALAM:潤滑 >= PALAMLV:3 ? 愛液に濡れた # 湿り気を帯びる \@体毛を掻き分けて柔らかい牝穴を舐め
					地の文分岐 = 1
				CASE 2
					PRINTFORM \@ TALENT:体格 <= 2 ? 幼くも交尾を誘うように作られた # 牡を発情させるような臭いを発する \@獣の女性器に熱い舌を這わせ
					地の文分岐 = 2
			ENDSELECT
			;/*地の文引用その1ここまで*/
			PRINTFORML ようとすると、
			PRINTFORMW %CALLNAME:TARGET%はクンニを促すかのように%CALLNAME:PLAYER%の頭部を脚で圧迫してきた…………
			;非処女かつ妊娠してなくて調教者がペニス有りなら分岐が増える
			IF TALENT:処女 == 0 && TALENT:妊娠 == 0 && PENIS(PLAYER)
				乱数分岐 = RAND:5
			ELSE
				乱数分岐 = RAND:3
			ENDIF
			SELECTCASE 乱数分岐
				CASE 0
					PRINTFORML 「はい%UNICODE(0x2665)%もふもふ～%UNICODE(0x2665)%%UNICODE(0x2665)%」
					PRINTFORMW 「君の気が済むまでもふもふしたら……私のここ…………たくさん舐めてほしいなっ%UNICODE(0x2665)%」
				CASE 1
					PRINTFORML 「脚でぎゅーってしてあげるねっ%UNICODE(0x2665)%」
					PRINTW 「私のメスの臭い……いっぱい感じてほしいの……」
				CASE 2
					;本当に「捕まえる」ような挟み方ではない
					;むしろ主人の方から顔を埋めにいっているくらい
					PRINTFORML 「くすっ%UNICODE(0x2665)%……捕まえちゃった%UNICODE(0x2665)%…………」
					PRINTW 「満足させるまで逃がさないからねっ♪」
				CASE 3
					PRINTFORML 「ここを舐めて準備ができたら…………早く君と交尾したいのっ%UNICODE(0x2665)%%UNICODE(0x2665)%」
					SELECTCASE MONTH
						;発情期かそうでないかで分岐
						CASE 3 TO 7
							PRINTFORMW 「…………発情期なんだから仕方がないでしょ！バカっ%UNICODE(0x2665)%」
						CASEELSE
							PRINTFORMW 「…………君のせいで年中発情期なんだからっ！さっさと終えて責任とって交尾しなさいよこのバカっ%UNICODE(0x2665)%」
					ENDSELECT
				CASE 4
					SELECTCASE MONTH
						;発情期かそうでないかで分岐
						CASE 3 TO 7
							PRINTFORML 「この時期って鳥獣も発情期なのっ%UNICODE(0x2665)%…………」
							PRINTFORMW 「だから……いっぱい舐めて%UNICODE(0x2665)%…………そのあと……君のペニスを挿れてほしいの%UNICODE(0x2665)%」
						CASEELSE
							PRINTFORML 「君にここを舐められると……体が熱くなってくの%UNICODE(0x2665)%…………」
							PRINTFORMW 「発情期じゃなくても…………君の精液……受け入れる準備はいつでもできてるよ%UNICODE(0x2665)%」
					ENDSELECT
			ENDSELECT
			PRINTFORM %CALLNAME:PLAYER%は%CALLNAME:TARGET%の脚を十分に堪能すると、改めて
			;/*地の文引用その2ここから*/
			SELECTCASE 地の文分岐
				CASE 0
					PRINTFORM 膣に舌を突き入れ
				CASE 1
					PRINTFORM 体毛を掻き分けて牝穴を舐め
				CASE 2
					PRINTFORM 女性器に舌を這わせ
			ENDSELECT
			;ペニス有り
			IF PENIS(TARGET)
				IF PALAM:勃起 >= PALAMLV:3
					SELECTCASE RAND:3
						CASE 0
							PRINTL 、そそり勃ったペニスに舌を這わせて舐め上げた……
						CASE 1
							PRINTL 、すっかり硬くなった肉棒の裏筋から亀頭までを舐め上げた……
						CASE 2
							PRINTL 、女性器の刺激にびくびくと震える熱い男根を舌で責め立てた……
					ENDSELECT
				;欲情が足りない場合(勃っていないのを想定)
				ELSE
					SELECTCASE RAND:3
						CASE 0
							PRINTL 、ペニスの付け根を舌で舐め上げた……
						CASE 1
							PRINTL 、まだ半勃ちのペニスを舌で弄んだ……
						CASE 2
							PRINTL 、勃ちきっていない柔らかな肉竿を唾液で濡らした……
					ENDSELECT
