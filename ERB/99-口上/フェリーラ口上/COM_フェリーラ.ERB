	IF CFLAG:陥落
		PRINTFORMW 「ん？休憩かな？」
	;屈服刻印Lv3
	ELSEIF MARK:屈服刻印 == 3
		PRINTFORMW 「休憩でしょうか？」
	;それ以外
	ELSE
		PRINTFORMW 「……何もせぬなら早々に立ち去らんか」
	ENDIF
	FCOM:SELECTCOM = 1
;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		SELECTCASE RAND:3
			;名前変更のイベント用
			CASE 0
				PRINTFORMW 「そういえば%CALLFERRIRA%のこと何て呼べばいいかな？」
				PRINTFORMW 「呼んで欲しい名前とかる？」
				PRINTL [0]今のままでいい
				PRINTL [1]変えてほしい
				BINPUT
				SELECTCASE RESULT
					;そのまま
					CASE 0
						PRINTFORMW 「うん。そしたら今まで通りの%CALLFERRIRA%だね♪」
					CASE 1
						PRINTFORMW 「そうしたら何て呼べばいいかなぁ？」
						PRINTL [0] 主様
						PRINTL [1] ご主人様
						PRINTL [2] 旦那様
						PRINTL [3] あなた
						PRINTL [4] ダーリン
						PRINTL [5] キミ
						PRINTL [99] やっぱり変えないでいい
						BINPUT
						SELECTCASE RESULT
							;0 主様
							CASE 0
								CALLFERRIRA = 主様
								PRINTFORMW 「主様……なんだか恭しくて素敵♪」
								PRINTFORMW 「そしたらこれから%CALLFERRIRA%って呼ぶね♪」
							;1 ご主人様
							CASE 1
								CALLFERRIRA = ご主人様
								PRINTFORMW 「あはっ♪ここならそれが一番それっぽいかもね♪」
								PRINTFORMW 「そしたらこれから%CALLFERRIRA%って呼ぶね♪」
							;2 旦那様
							CASE 2
								CALLFERRIRA = 旦那様
								PRINTFORMW 「旦那様……フェリーラめは旦那様の生涯の伴侶となり申します……なんてね♪」
								PRINTFORMW 「そしたらこれから%CALLFERRIRA%って呼ぶね♪」
							;3 あなた
							CASE 3
								CALLFERRIRA = あなた
								PRINTFORMW 「あなた……きゃっ♥なんだか新婚さんみたい♪」
								PRINTFORMW 「そしたらこれから%CALLFERRIRA%って呼ぶね♪」
							;4 ダーリン
							CASE 4
								CALLFERRIRA = ダーリン
								PRINTFORMW 「ダーリンなんて……なんだかラブラブなカップルみたい……♪」
								PRINTFORMW 「え？ちょっと価値観古いって？そ、そんなことないよ！！」
								PRINTFORMW 「えっと、こほん！そしたらこれから%CALLFERRIRA%って呼ぶね♪」
							;5 キミ（某イベントのデフォルトがこれらしいのでデフォルトもこれ）
							CASE 5
								CALLFERRIRA = キミ
								PRINTFORMW 「そうだね、やっぱり“キミ”が一番だね♪」
								PRINTFORMW 「そしたらこれから%CALLFERRIRA%って呼ぶね♪」
							;99 やっぱり変えないでいい
							CASE 99
								PRINTFORMW 「そう？そしたら今まで通りの%CALLFERRIRA%だね♪」
						ENDSELECT
				ENDSELECT
			CASE 1
				PRINTFORMW 「休憩かなあ？休憩も良いけど……ね？」
				PRINTFORMW そう言うとフェリーラはもじもじとしながら頬を紅潮させて期待に満ちた目で%CALLNAME:MASTER%を見つめてくる。
			CASE 2
				PRINTFORMW 「はい！もう休憩終わり！」
