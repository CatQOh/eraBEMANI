;憑依時に口上を表示しない 表示する場合はコメントアウト
SIF CFLAG:憑依 >= 0
	RETURN 0
;ボールギャグ着用時には地の文を表示して口上をスキップする
SIF TEQUIP:ボールギャグ &&  SELECTCOMS != "ボールギャグ"
	RETURN 0
SIF OPTION:口上表示 == 0
	RETURN 0
;主人が男性でもスキップ
SIF SEX(PLAYER) == 1
