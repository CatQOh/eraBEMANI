;-------------------------------------------------
;アナル愛撫
;アナル弄りにワセリンはあるものだと思って
;-------------------------------------------------
@COM_アナル愛撫_光伊_計名
	TRYCALLFORM MESSAGE_COM_%SELECTCOMS%
;陥落済み
IF CFLAG:陥落
	SELECTCASE ABL:Ａ感覚
		CASE 0 TO 2
			IF PALAM:潤滑 >= PALAMLV:3
				PRINTFORMW ローションが絡み付く指先で抜き差ししても、これといった感覚が掴めていない様だ
				PRINTFORMW 「あ……うっ、少しはマシな方かな…」
			ELSE
				PRINTFORMW 「いっ、ぁ…このままいじっちゃ、やだぁ…！」
				PRINTFORMW 「優しくしてぇな…もう」
			ENDIF
		CASE 3
			PRINTFORMW 「はぁっ、これも…イイっ…」
			PRINTFORMW 「……なんだか嬉しそうな顔しとる…」
		CASE 4
			PRINTFORMW 「あ、あぁーっ……！」
			PRINTFORMW 「愛撫済んだら…はよ、挿入れてほしい…」
			PRINTFORMW 丁寧にアナルを解されたケイナは、とろんとした表情をさせて%CALLNAME:PLAYER%を見つめている
		CASE IS >= 5
			PRINTFORMW 「はぁん、んっ…んぁああ……っ」
			PRINTFORMW 「も……イっちゃうっ……」
	ENDSELECT
ELSE
	SELECTCASE MARK:屈服刻印
		CASE 2, 3
			SELECTCASE ABL:Ａ感覚
			;Ａ感覚Lv0～Lv3
				CASE 0 TO 3
					PRINTFORMW 「くぅ…っ、中が痛い…無理ですったら…！」
				;Ａ感覚Lv4～Lv6
				CASE 4 TO 6
					PRINTFORMW 「はぁ…ぐぅ…んんっ……乱暴に、しないで…」
					PRINTFORMW 「いっ！あぁ！はぁあっ！あぁああ…」
				;Ａ感覚Lv7以上
				CASE IS >= 7
					PRINTFORMW 「ひはぁ…あぅ…！」
					PRINTFORMW 「お尻っ…おかしくなるぅ…あぁ…！」
			ENDSELECT
		CASEELSE
			SELECTCASE ABL:Ａ感覚
				;Ａ感覚Lv3以下
				CASE IS <= 3
					PRINTFORMW 「痛っ、優しく出来ないんですか…」
				;Ａ感覚Lv4以上
				CASE IS >= 4
					PRINTFORMW 「くぅうう…やめろ、それ以上弄るな…っ！」
					PRINTFORMW 「あぁ…あはぁああっ！」
			ENDSELECT
	ENDSELECT
ENDIF

RETURN 0

;-------------------------------------------------
;胸弄る系全般
;-------------------------------------------------
@胸共通_光伊_計名
	TRYCALLFORM MESSAGE_COM_%SELECTCOMS%
;陥落済み
IF CFLAG:陥落
	SELECTCASE RAND:4
		CASE 0
			PRINTFORMW ｢ちょっ、そんなに触っても大きくならないってのに……ひぁ！｣
		CASE 1
			SELECTCASE SELECTCOMS
				CASE "胸揉み"
					PRINTFORMW 「ふぅ…揉まれてるだけなのにっ…」
				CASE "乳首舐め"
					PRINTFORMW 「美味しいって言われても恥ずかし…ぁ…もっと舐めて……」
					PRINTFORMW %CALLNAME:PLAYER%に乳首を舐られるケイナは吐息を漏らしている……
				CASEELSE
					PRINTFORMW 「私の胸、ホンマに好きなんやな……んっ」
			ENDSELECT
		CASE 2
			IF ABL:Ｂ感覚 <= 3
				PRINTW ｢む、胸かぁ…まぁ、撫で回しても…ええよ？｣
			ELSEIF PREVCOMS == "愛撫"
				PRINTFORMW 「出来れば他の所も……触ってくれへんか」
				PRINTFORMW 「色んな部分を味わうのも…悪ないやろ」
			ELSE
				PRINTFORMW 「はぁ…は……あ、感じすぎて少し、興奮してきた…」
			ENDIF
		CASE 3
			IF SELECTCOMS == "乳首責め" && ABL:Ｂ感覚 >= 3
				PRINTW 「はぁ…っ、も…少しでイケそう…もっとぉ…強く、して…」
				PRINTW 乳首への刺激でケイナの喘ぐ声が止まらない…
			ELSE
				PRINTW 「はは…、ホンマに好きモノやなぁ…！」
			ENDIF
	ENDSELECT
;未陥落で魔法掛かってる時
ELSEIF ハニーバニー
	SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「わっ、急に胸触るのは良くないよ。でも……今はこうして愛し…合えるのも、いいかな」
			PRINTFORMW %CALLNAME:PLAYER%の指で触れられているにも関わらず、錯覚している…
		CASE 1
			IF SELECTCOMS == "乳首舐め"
				PRINTFORMW 「ひゃ、あぁっ…くっ……リリスちゃ…はぁああっ…！」
			ELSE
				PRINTFORMW （彼女は一体何処でこんな事を覚えてきたんだろう、気にしてても仕方ないか）
			ENDIF
	ENDSELECT
ELSEIF MARK:屈服刻印 >= 2
	SELECTCASE ABL:Ｂ感覚
		;Ｂ感覚Lv4以下
		CASE IS <= 4
			SELECTCASE RAND:2
				CASE 0
					PRINTFORMW 「はぁ、んん…っ…ううっ…くすぐったい…」
					PRINTFORMW 「撫でまわされてるだけなのに…ふっ…あぁ…」
				CASE 2
					PRINTFORMW 「あ…ぅ…、優しく触れて欲しい…」
			ENDSELECT
		;Ｂ感覚Lv5以上
		CASE IS >= 5
			SELECTCASE RAND:2
				CASE 0
					PRINT 「あぁ…
					IF SELECTCOMS == "乳首責め"
						PRINTW お願いします…強く弄って、ください…！」
					ELSE
						PRINTW 胸だけで…変な気分になるっ……！」
					ENDIF
				CASE 1
					PRINTFORMW 「違う…感じてなんか…」
			ENDSELECT
			PRINTFORMW 今まで胸を触れられる事を嫌がっていたケイナも、大分素直になりつつある…
	ENDSELECT
ELSE
	SELECTCASE ABL:Ｂ感覚
		;Ｂ感覚Lv0～Lv2
		CASE 0 TO 2
			SELECTCASE SELECTCOMS
				CASE "胸愛撫"
					PRINTFORMW 「いっ、あぁっ！ちょっと痛い…擦り過ぎですよ、もう」
				CASE "胸揉み"
					PRINTFORMW 「マッサージのつもりですか、よくわからないな…」
				CASE "乳首舐め"
					PRINTFORMW 「ひゃああっ！、そ、そんな所なんか舐めないで！」
				CASEELSE
					PRINTFORMW 「揉んでも変化しないのに…」
			ENDSELECT
			PRINTFORMW それでも%CALLNAME:PLAYER%に対して毛嫌いする様子を変えない…
		;Ｂ感覚Lv3～Lv4
		CASE 3 TO 4
			SELECTCASE SELECTCOMS
				CASE "胸愛撫"
					PRINTFORMW 「あっ……来るっ、ん…うぅ……」
				CASE "乳首舐め"
					PRINTFORMW 「うぁ…ひぃ、やっ…気持ち良いっ…！」
				CASEELSE
					PRINTFORMW 「地味にやりますね、このっ……変態……」
			ENDSELECT
		;Ｂ感覚Lv5以上
		CASE IS >= 5
			PRINTFORMW 「っ…何かちょっとだけ…」
			PRINTFORMW 「あ、いや…感じていない…っ！」
	ENDSELECT
ENDIF

