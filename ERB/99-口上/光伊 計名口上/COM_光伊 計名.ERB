;愛撫
;-------------------------------------------------
@COM_愛撫_光伊_計名
;陥落済み
IF CFLAG:陥落
	TRYCALLFORM MESSAGE_COM_%SELECTCOMS%
	SELECTCASE RAND:3
		CASE 0
			PRINTFORMW ｢ふっ…あ…す、きぃ……｣
			PRINTFORMW 全身を組まなく撫で回されるケイナから熱の篭った喘ぎを漏らしている…
		CASE 1
			PRINTFORMW ｢やぁ…ぁ…そこばっか…｣
			PRINTFORMW わざと敏感な部分に集中して撫で続けると 可愛らしく喘いでいる…
		CASE 2
			PRINTFORMW 「ひっ…はぅ！…ちょっと、そんなとこ…っ！」
	ENDSELECT
;未陥落で魔法掛かってる時
ELSEIF ハニーバニー
	PRINTFORMW 「うぅ…リ、リリスちゃん……そんなに触ったらダメだって……」
	PRINTFORMW ケイナが恥ずかしそうに抵抗しているものの、意中の相手であれば弱々しくしている……
;屈服刻印Lv2
ELSEIF MARK:屈服刻印 >= 2
	TRYCALLFORM MESSAGE_COM_%SELECTCOMS%
	;快楽刻印あり
	SELECTCASE MARK:快楽刻印
		CASE 1
			PRINTFORMW 「い、嫌です…抱きしめないでっ…！」
			PRINTFORMW 「ひっ…あぁ…！」
		CASE 2, 3
			PRINTFORMW 「ひぁ…はっ…気安く触らないで…って、言ってるのに…！」
	;快楽なし
		CASEELSE
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「んんぅ…もっと…して…ください……」
					PRINTFORMW 快感に慣れて来ているものの、言動にまだ抵抗感が伺える
				CASE 1
					PRINTFORMW 「撫で方がいやらしすぎる…何なんですか…」
					PRINTFORMW 「その程度じゃ、ひゃうっ！」
				CASE 2
					PRINTFORMW ケイナは唸りながら抵抗している…
			ENDSELECT
	ENDSELECT
ELSE
	TRYCALLFORM MESSAGE_COM_%SELECTCOMS%
	SELECTCASE ABL:従順
		;従順Lv0～Lv2
		CASE 0 TO 2
			;反発刻印あり
			SELECTCASE MARK:反発刻印
				CASE 1
					SELECTCASE RAND:2
						CASE 0
							PRINTFORMW 「馴れ馴れしく触らないでくださいよ」
							PRINTFORMW 「愛撫されるだけでも、虫が這ってる感じだ」
						CASE 1
							PRINTFORMW 「貴方の恋人じゃないのに…」
					ENDSELECT
				CASE 2, 3
					PRINTFORMW 「気持ち悪い。触るなと言ってるんだ」
					PRINTFORMW %CALLNAME:PLAYER%に嫌悪感を表すケイナはすぐにでも、その手を払い除けようとする
			;反発なし
				CASEELSE
					PRINTDATAW
						DATA 「馬鹿馬鹿しい…そういう趣味をお持ちなんですね」
						DATA 「興味本位で触られても困るな…」
						DATA 「ただ愛撫しても気持ち悪いだけだ…っ」
					ENDDATA
			ENDSELECT
		;従順Lv3以上
		CASE IS >= 3
			SELECTCASE RAND:3
				CASE 0
					PRINTFORMW 「触って欲しくない、でも…」
				CASE 1
					PRINTFORMW 「っ…お願い……いや、もっと………」
				CASE 2
					PRINTFORMW 「こんなんでも…悪くない…」
			ENDSELECT
	ENDSELECT
ENDIF

RETURN 0

;-------------------------------------------------
;フェラする
;-------------------------------------------------
@COM_フェラする_光伊_計名
TRYCALLFORM MESSAGE_COM_%SELECTCOMS%
;初めて
IF FCOM:SELECTCOM == 0
;	PRINTFORMW 
	FCOM:SELECTCOM = 1
	;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「ひ、ぁ…はぁ……」
				PRINTFORMW 「そうされると、感じてきちゃ…あぁ…！」
			CASE 1
				PRINTFORMW 「うぁ…あ、はっ…上手過ぎだって…っ！」
		ENDSELECT
	;未陥落で魔法掛かってる時
	ELSEIF ハニーバニー
		PRINTDATAW
			DATA 「はぁっ、くぅ…お願いだから……口離して、いや……うぅ…」
			DATA 「そん、なっ……リリっ…ん……」
			DATA 「汚いよリリスちゃ……ぁはぁ……」
		ENDDATA
		PRINTFORMW ケイナが恥ずかしそうに抵抗しているものの、意中の相手にされるがままだ……
	ELSEIF MARK:屈服刻印 >= 2
		SELECTCASE RAND:2
			CASE 0
				PRINTFORMW 「やめてくださ……はぁ……あ、ううぅ……」
			CASE 1
				PRINTFORMW 「いやぁ…はな、して…放してくださいっ…！」
				PRINTFORMW %CALLNAME:PLAYER%の頭から引き剥がそうとしてるが、弱弱しく抵抗している…
		ENDSELECT
	;快楽刻印あり
	ELSEIF MARK:快楽刻印 >= 2
		PRINTFORMW 「くはぁ…あぁ…やめ…っ！」
		PRINTFORMW 「これっ、気持ち良すぎるのにぃ…！」
	ELSE
		SELECTCASE SEX(PLAYER)
			CASE 1
				PRINTDATAW
					DATA 「くぅ、勝手にそんな所咥えるな…っ！」
					DATA 「やめろ……男同士でこんな事…して、許される訳が…」
					DATA 「き…気持ち悪い、離せ…っ…」
				ENDDATA
			CASE 2
				PRINTDATAW
					DATA 「あぁっ、だ、駄目ですってば…！」
					DATA 「う……あ、頼むから……！」
					DATA 「うぅ…っ…嫌なのに…っ」
				ENDDATA
		ENDSELECT
	ENDIF
ENDIF

RETURN 0

