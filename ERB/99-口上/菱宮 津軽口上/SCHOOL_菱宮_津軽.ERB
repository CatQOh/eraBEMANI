;-------------------------------------------------
;最初に呼ばれる関数
;-------------------------------------------------
@SCHOOL_KOJO_START_菱宮_津軽
;主人公のタイプで全体が分岐
;同じ学校の生徒
IF CSTR:PLAYER:在校 == CSTR:GETCHARANAME("菱宮 津軽"):在校
	TSUGARU:分岐 = 1
;違う学校で年上
ELSEIF !GROUPMATCH(小中高(CSTR:PLAYER:在校), "小", "中")
	TSUGARU:分岐 = 2
;違う学校の同年代
ELSEIF !GROUPMATCH(小中高(CSTR:PLAYER:在校), "中")
	TSUGARU:分岐 = 3
;年下
ELSE
	TSUGARU:分岐 = 4
ENDIF

