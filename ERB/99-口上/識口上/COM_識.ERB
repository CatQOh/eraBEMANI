	ENDIF
	FCOM:SELECTCOM = 1
;二回目以降
ELSE
	;陥落済み
	IF SHIKI_KENA()
		SELECTCASE RAND:2
			CASE 0
				IF PALAM:勃起+UP:勃起 > PALAMLV:3
					PRINTFORML 「んん…ひゃうっ、ケイナぁ…」
					PRINTFORML 「ほーれ、結構硬なっとるて」
					PRINTFORMW 「あ、あぁ…扱かれる度音出てる…っ、んぅ…」
				ELSE
					PRINTFORMW 「下はその気になってないけど…ふふ…ケイナ、そのまま触って………」
				ENDIF
			CASE 1
				;前回使用コマンドで分岐
				SELECTCASE PREVCOMS
					CASE "愛撫", "胸愛撫"
						PRINTFORML 識の身体を愛撫しながら下半身に手を付けたケイナは、緩やかに扱きだした…
						PRINTFORMW 「あはぁ…下、触ってくれるんだ…激しくして…」
					CASE "キスする", "キスしてもらう"
						PRINTFORMW 「っ…ふぁ………んんっ…ケイナったら、いつの間に手を忍ばせてたの…」
					CASEELSE
						PRINTFORML 「識はこうされるのが好きやもんな……？」
						PRINTFORMW 「う、うん………」
				ENDSELECT
		ENDSELECT
	ELSEIF SHIKI_ZILCH()
		SELECTCASE RAND:2
			CASE 0
				IF PALAM:勃起+UP:勃起 > PALAMLV:3
					PRINTFORML 「こんなに硬くしやがって……識もエロい事考えてたんだろ？」
					PRINTFORML 「っはぁ…ジル、触られて軽く興奮しただけなんだ…信じてよ」
					PRINTFORMW 「フフ……喘いでる顔見てると説得力ねぇな」
				ELSE
					PRINTFORMW 「あ…その気分じゃないけど、触って欲しい……」
				ENDIF
			CASE 2
				SELECTCASE PREVCOMS
					CASE "キスする", "キスしてもらう"
						PRINTFORML 「んんっ……ジル、次は……」
						PRINTFORMW 「下の方も触ってやらないとな」
					CASEELSE
						PRINTFORMW 「ひぁ、もっと扱いてほしいよ……ジル」
				ENDSELECT
		ENDSELECT
	ELSEIF 陥落(TARGET)
		SELECTCASE FALLTYPE(TARGET)
			;恋慕・親愛＋淫乱・淫魔
			CASE 1, 2
				PRINTFORMW 「あぁ、ソコに触れられるとは…君ならいいよ。扱く時は優しくね？」
			;服従・隷属
			CASE 3
				PRINTFORMW 「あ…あはぁ…はぁ…っ、良いっ…手加減しなくてもいいから扱いて…」
		ENDSELECT
	;未陥落で魔法掛かってる時
	ELSEIF ハニーバニー
		PRINTFORMW 「っく、はぁっ…扱いてくれるんだ…良いよ、優しく扱いて……」
	ELSE
	;屈服刻印
		SELECTCASE MARK:屈服刻印
			CASE 1
			;反発刻印あり
				SELECTCASE MARK:反発刻印
					CASE 1, 2
						PRINTFORMW 「だから勝手に扱くのをやめろって、ふぅ、うぅ……」
					CASE 3
						PRINTFORML 「いい加減僕のペニスなんか扱くなよ…気持ち良くなんか、ないって」
						PRINTFORML 「聞いてるのか……！」
						WAIT
				;反発なし
					CASEELSE
						PRINT 「少し乱暴だな。優しくしてくれよ…
						SELECTCASE SEX(PLAYER)
							CASE 1
								PRINTW 同じ男なら分かるだろ」
							CASE 2
								PRINTW せめてゆっくり扱いてくれ」
						ENDSELECT
				ENDSELECT
			CASE 2, 3
				PRINT 「うぅ…
				IF TEQUIP:オナホール
					PRINTW あぁ！、っく……オナホごと扱かないでくれ、感じちゃうから……！」
				ELSE
					PRINTW 頼むよ、気持ち良いけど…」
				ENDIF
			CASEELSE
				PRINTFORMW 「嫌だ、やめてくれ…！」
		ENDSELECT
	ENDIF
ENDIF

RETURN 0

