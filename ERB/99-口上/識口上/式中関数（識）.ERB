;軽識新婚モード判定
@SHIKI_KENA, ARG
#FUNCTION
IF ゲームモード() == "新婚モード" && GROUPMATCH("識", CHARANAMEF(MASTER), CHARANAMEF(TARGET)) && GROUPMATCH("光伊 計名", CHARANAMEF(MASTER), CHARANAMEF(TARGET))
  RETURNF 1
ELSE
  RETURNF 0
ENDIF

;汁識新婚モード判定
@SHIKI_ZILCH, ARG
#FUNCTION
IF ゲームモード() == "新婚モード" && GROUPMATCH("識", CHARANAMEF(MASTER), CHARANAMEF(TARGET)) && GROUPMATCH("ジルチ", CHARANAMEF(MASTER), CHARANAMEF(TARGET))
  RETURNF 1
ELSE
  RETURNF 0
ENDIF
