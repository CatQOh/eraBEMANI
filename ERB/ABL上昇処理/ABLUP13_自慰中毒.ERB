@ABLUP13
SIF ABL:自慰中毒 == 10 && !TALENT:リミットバースト
	RETURN 0
CALL NEED_JUEL13
CALL CHECK_JUEL13
IF RESULT
	CALL NEED_JUEL13
	REPEAT 15
		JUEL:COUNT -= ABLUP:COUNT
	REND
	ABL:自慰中毒++
ENDIF

@NEED_JUEL13
SELECTCASE ABL:自慰中毒
	CASE 0
		ABLUP:GETNUM(PALAM, "快Ｃ") = 10000
		ABLUP:GETNUM(PALAM, "欲情") = 3000
		ABLUP:GETNUM(PALAM, "恐怖") = 1000
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 100
	CASE 1
		ABLUP:GETNUM(PALAM, "快Ｃ") = 25000
		ABLUP:GETNUM(PALAM, "欲情") = 6000
		ABLUP:GETNUM(PALAM, "恐怖") = 3000
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 250
	CASE 2
		ABLUP:GETNUM(PALAM, "快Ｃ") = 50000
		ABLUP:GETNUM(PALAM, "欲情") = 12000
		ABLUP:GETNUM(PALAM, "恐怖") = 6000
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 500
	CASE 3
		ABLUP:GETNUM(PALAM, "快Ｃ") = 100000
		ABLUP:GETNUM(PALAM, "欲情") = 20000
		ABLUP:GETNUM(PALAM, "恐怖") = 15000
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 1000
	CASE 4
		ABLUP:GETNUM(PALAM, "快Ｃ") = 300000
		ABLUP:GETNUM(PALAM, "欲情") = 40000
		ABLUP:GETNUM(PALAM, "恐怖") = 40000
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 2000
	CASE 5
		ABLUP:GETNUM(PALAM, "快Ｃ") = 350000
		ABLUP:GETNUM(PALAM, "欲情") = 45000
		ABLUP:GETNUM(PALAM, "恐怖") = 45000
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 2500
	CASE 6
		ABLUP:GETNUM(PALAM, "快Ｃ") = 400000
		ABLUP:GETNUM(PALAM, "欲情") = 50000
		ABLUP:GETNUM(PALAM, "恐怖") = 50000
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 3000
	CASE 7
		ABLUP:GETNUM(PALAM, "快Ｃ") = 450000
		ABLUP:GETNUM(PALAM, "欲情") = 55000
		ABLUP:GETNUM(PALAM, "恐怖") = 55000
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 3000
	CASE 8
		ABLUP:GETNUM(PALAM, "快Ｃ") = 500000
		ABLUP:GETNUM(PALAM, "欲情") = 60000
		ABLUP:GETNUM(PALAM, "恐怖") = 60000
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 3500
	CASE 9
		ABLUP:GETNUM(PALAM, "快Ｃ") = 600000
		ABLUP:GETNUM(PALAM, "欲情") = 70000
		ABLUP:GETNUM(PALAM, "恐怖") = 70000
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 5000
	CASE IS >= 10
		LOCAL = ABL:自慰中毒*ABL:自慰中毒
		ABLUP:GETNUM(PALAM, "快Ｃ") = 7000 * LOCAL
		ABLUP:GETNUM(PALAM, "欲情") = 800 * LOCAL
		ABLUP:GETNUM(PALAM, "恐怖") = 800 * LOCAL
		ABLUPEXP:GETNUM(EXP, "自慰経験") = 60 * LOCAL
ENDSELECT

;一線越えない
IF TALENT:一線越えない
	;未陥落
	IF !CFLAG:陥落
		TIMES ABLUP:GETNUM(PALAM, "快Ｃ") , 2.00
		TIMES ABLUP:GETNUM(PALAM, "欲情") , 2.00
		TIMES ABLUP:GETNUM(PALAM, "恐怖") , 2.00
		TIMES ABLUPEXP:GETNUM(EXP, "自慰経験") , 2.00
	;陥落
	ELSEIF CFLAG:陥落
		TIMES ABLUP:GETNUM(PALAM, "快Ｃ") , 0.70
		TIMES ABLUP:GETNUM(PALAM, "欲情") , 0.70
		TIMES ABLUP:GETNUM(PALAM, "恐怖") , 0.70
		TIMES ABLUPEXP:GETNUM(EXP, "自慰経験") , 0.70
	ENDIF
ENDIF

;自制心
IF TALENT:自制心
	;欲望LV4以下
	IF ABL:欲望 <= 4
		TIMES ABLUP:GETNUM(PALAM, "快Ｃ") , 2.00
		TIMES ABLUP:GETNUM(PALAM, "欲情") , 2.00
		TIMES ABLUP:GETNUM(PALAM, "恐怖") , 2.00
		TIMES ABLUPEXP:GETNUM(EXP, "自慰経験") , 2.00
	;欲望LV5以上
	ELSEIF ABL:欲望 >= 5
		TIMES ABLUP:GETNUM(PALAM, "快Ｃ") , 0.80
		TIMES ABLUP:GETNUM(PALAM, "欲情") , 0.80
		TIMES ABLUP:GETNUM(PALAM, "恐怖") , 0.80
		TIMES ABLUPEXP:GETNUM(EXP, "自慰経験") , 0.80
	ENDIF
ENDIF

;中毒しやすい
IF TALENT:中毒しやすい
	TIMES ABLUP:GETNUM(PALAM, "快Ｃ") , 0.50
	TIMES ABLUP:GETNUM(PALAM, "欲情") , 0.50
	TIMES ABLUP:GETNUM(PALAM, "恐怖") , 0.50
	TIMES ABLUPEXP:GETNUM(EXP, "自慰経験") , 0.50
ENDIF

;臆病、濡れやすい、小柄体型
SIF TALENT:臆病 || TALENT:濡れやすい || TALENT:小柄体型
	TIMES ABLUP:GETNUM(PALAM, "恐怖") , 0.70


;LV2からは異常経験必要 LV3からは異常経験2以上必要 LV4からは異常経験3以上必要 LV8からは異常経験7以上必要（素質：[中毒しやすい][自慰狂い]なら無視できる）
IF !TALENT:中毒しやすい && !TALENT:自慰狂い
	IF ABL:自慰中毒 >= 8
		ABLUPEXP:GETNUM(EXP, "異常経験") = 7
	ELSEIF ABL:自慰中毒 >= 4
		ABLUPEXP:GETNUM(EXP, "異常経験") = 3
	ELSEIF ABL:自慰中毒 >= 3
		ABLUPEXP:GETNUM(EXP, "異常経験") = 2
	ELSEIF ABL:自慰中毒 >= 2
		ABLUPEXP:GETNUM(EXP, "異常経験") = 1
	ENDIF
ENDIF

@CHECK_JUEL13
;異常経験が規定値に満たないと上昇できない
SIF EXP:異常経験 < ABLUPEXP:GETNUM(EXP, "異常経験")
	RETURN 0

;自慰中毒+1の露出癖が必要
SIF ABL:露出癖 <= ABL:自慰中毒
	RETURN 0

;欲情の珠、快Ｃの珠、恐怖の珠、自慰経験で上げる
SIF JUEL:快Ｃ >= ABLUP:GETNUM(PALAM, "快Ｃ") && JUEL:欲情 >= ABLUP:GETNUM(PALAM, "欲情") && JUEL:恐怖 >= ABLUP:GETNUM(PALAM, "恐怖") && EXP:自慰経験 >= ABLUPEXP:GETNUM(EXP, "自慰経験")
	RETURN 1

;上記の条件を満たせなかったら上げられない
RETURN 0

@ABLUP_MESSAGE13
;LV2からは異常経験必要 LV3からは異常経験2以上必要 LV4からは異常経験3以上必要 LV8からは異常経験7以上必要（素質：[中毒しやすい][自慰狂い]なら無視できる）
SIF EXP:異常経験 < ABLUPEXP:GETNUM(EXP, "異常経験")
	SETCOLOR 120, 120, 120
SIF ABLUPEXP:GETNUM(EXP, "異常経験")
	PRINTFORM [異常経験{ABLUPEXP:GETNUM(EXP, "異常経験")}以上]
RESETCOLOR

SIF ABL:露出癖 <= ABL:自慰中毒
	SETCOLOR 120, 120, 120
PRINTFORM [露出癖LV{ABL:自慰中毒+1}以上]
RESETCOLOR

SIF JUEL:快Ｃ < ABLUP:GETNUM(PALAM, "快Ｃ")
	SETCOLOR 120, 120, 120
PRINTFORM [快Ｃの珠×{ABLUP:GETNUM(PALAM, "快Ｃ")}]
RESETCOLOR

SIF JUEL:欲情 < ABLUP:GETNUM(PALAM, "欲情")
	SETCOLOR 120, 120, 120
PRINTFORM [欲情の珠×{ABLUP:GETNUM(PALAM, "欲情")}]
RESETCOLOR

SIF JUEL:恐怖 < ABLUP:GETNUM(PALAM, "恐怖")
	SETCOLOR 120, 120, 120
PRINTFORM [恐怖の珠×{ABLUP:GETNUM(PALAM, "恐怖")}]
RESETCOLOR

SIF EXP:自慰経験 < ABLUPEXP:GETNUM(EXP, "自慰経験")
	SETCOLOR 120, 120, 120
PRINTFORM [自慰経験{ABLUPEXP:GETNUM(EXP, "自慰経験")}以上]
RESETCOLOR

