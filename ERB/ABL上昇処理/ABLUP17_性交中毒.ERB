;一線越えない
IF TALENT:一線越えない
	;未陥落
	IF !CFLAG:陥落
		TIMES ABLUP:GETNUM(PALAM, "恭順") , 2.00
		TIMES ABLUP:GETNUM(PALAM, "欲情") , 2.00
		TIMES ABLUP:GETNUM(PALAM, "屈服") , 2.00
		TIMES ABLUPEXP:GETNUM(EXP, "性交経験") , 2.00
	;陥落
	ELSEIF CFLAG:陥落
		TIMES ABLUP:GETNUM(PALAM, "恭順") , 0.80
		TIMES ABLUP:GETNUM(PALAM, "欲情") , 0.80
		TIMES ABLUP:GETNUM(PALAM, "屈服") , 0.80
		TIMES ABLUPEXP:GETNUM(EXP, "性交経験") , 0.80
	ENDIF
ENDIF

;自制心
IF TALENT:自制心
	;欲望LV4以下
	IF ABL:欲望 <= 4
		TIMES ABLUP:GETNUM(PALAM, "恭順") , 2.00
		TIMES ABLUP:GETNUM(PALAM, "欲情") , 2.00
		TIMES ABLUP:GETNUM(PALAM, "屈服") , 2.00
		TIMES ABLUPEXP:GETNUM(EXP, "性交経験") , 2.00
	;欲望LV5以上
	ELSEIF ABL:欲望 >= 5
		TIMES ABLUP:GETNUM(PALAM, "恭順") , 0.80
		TIMES ABLUP:GETNUM(PALAM, "欲情") , 0.80
		TIMES ABLUP:GETNUM(PALAM, "屈服") , 0.80
		TIMES ABLUPEXP:GETNUM(EXP, "性交経験") , 0.80
	ENDIF
ENDIF

;中毒しやすい
IF TALENT:中毒しやすい
	TIMES ABLUP:GETNUM(PALAM, "恭順") , 0.50
	TIMES ABLUP:GETNUM(PALAM, "欲情") , 0.50
	TIMES ABLUP:GETNUM(PALAM, "屈服") , 0.50
	TIMES ABLUPEXP:GETNUM(EXP, "性交経験") , 0.50
ENDIF
;男
IF SEX(TARGET) == 1
	TIMES ABLUP:GETNUM(PALAM, "恭順") , 0.80
	TIMES ABLUP:GETNUM(PALAM, "欲情") , 0.80
	TIMES ABLUP:GETNUM(PALAM, "屈服") , 0.80
	TIMES ABLUPEXP:GETNUM(EXP, "性交経験") , 1.50
ENDIF

;LV2からは異常経験必要 LV3からは異常経験2以上必要 LV4からは異常経験3以上必要 LV8からは異常経験7以上必要（素質：[中毒しやすい][セックス狂][尻穴狂い]なら無視できる）
IF !TALENT:中毒しやすい && !TALENT:セックス狂 && !TALENT:尻穴狂い
	SELECTCASE ABL:性交中毒
		CASE IS >= 8
			ABLUPEXP:GETNUM(EXP, "異常経験") = 7
		CASE IS >= 4
			ABLUPEXP:GETNUM(EXP, "異常経験") = 3
		CASE IS >= 3
			ABLUPEXP:GETNUM(EXP, "異常経験") = 2
		CASE IS >= 2
			ABLUPEXP:GETNUM(EXP, "異常経験") = 1
	ENDSELECT
ENDIF

@CHECK_JUEL17
;異常経験が必要
SIF EXP:異常経験 < ABLUPEXP:GETNUM(EXP, "異常経験")
	RETURN 0

;恭順、欲情、屈服の珠及び性交経験で上げる
SIF JUEL:恭順 >= ABLUP:GETNUM(PALAM, "恭順") && JUEL:欲情 >= ABLUP:GETNUM(PALAM, "欲情") && JUEL:屈服 >= ABLUP:GETNUM(PALAM, "屈服") && EXP:性交経験 >= ABLUPEXP:GETNUM(EXP, "性交経験")
	RETURN 1

;上記の条件を満たせないと上げられない
RETURN 0

