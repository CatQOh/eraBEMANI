;経験表示
@SHOW_INFO_EXP, ARG
#DIMS DYNAMIC 表示用経験
#DIMS DYNAMIC 経験名
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC 一行表示数
#DIMS DYNAMIC 表示用, 5
#DIM DYNAMIC 表示数

;英語と日本語で文字数が違いすぎるので一行の表示数を変える
IF LANGUAGE == "ENG"
	一行表示数 = 4
ELSE
	一行表示数 = 5
ENDIF

FOR LCOUNT, 0, VARSIZE("EXP")
	IF LCOUNT == VARSIZE("EXP")-1
		HTML_PRINT STRJOIN(表示用, "")
		BREAK
	ELSEIF EXPNAME:LCOUNT == ""
		CONTINUE
	ENDIF

	;改行するのか？ 続けて出力するのか？
	IF 表示数%一行表示数 == 0 && 表示数 != 0
		HTML_PRINT STRJOIN(表示用, "")
		VARSET 表示用
	ENDIF

	表示用経験 = %TOSTR(EXP:ARG:LCOUNT), 12%
	;試したけど予想以上に見た目がウザかった
	;表示用経験 = %REPLACE(表示用経験, " ", "_")%

	SELECTCASE EXPNAME:LCOUNT
		CASE "レズ経験", "噴乳経験"
			;男ならレズ経験、噴乳経験は灰色表示
			IF SEX(ARG) == 1
				SETCOLORBYNAME gray
				表示用経験 = ------------
			ENDIF
		;女ならゲイ経験は灰色表示
		CASE "ゲイ経験"
			IF SEX(ARG) == 2
				SETCOLORBYNAME gray
				表示用経験 = ------------
			ENDIF
		;膣が無ければＶ経験、膣射経験、Ｖ拡張経験。出産経験、流産経験も灰色
		CASE "Ｖ経験", "膣射経験", "Ｖ拡張経験", "流産経験"
			IF !VAGINA(ARG)
				SETCOLORBYNAME gray
				表示用経験 = ------------
			ENDIF
		;ペニス無ければ射精経験も灰色
		CASE "射精経験", "射精管理経験"
			IF !PENIS(ARG)
				SETCOLORBYNAME gray
				表示用経験 = ------------
			ENDIF
	ENDSELECT


	IF LANGUAGE == "ENG"
		経験名 = %EXPDISP(LCOUNT), 16, LEFT%
	ELSE
		SELECTCASE EXPNAME:LCOUNT
			CASE "奉仕快楽経験"
				経験名 = 奉快経験
			CASE "フェラ経験"
				経験名 = 口淫経験
			CASE "パイズリ経験"
				経験名 = 乳擦経験
			CASE "射精管理経験"
				経験名 = 射精管理
			CASE "苦痛快楽経験"
				経験名 = 痛楽経験
			CASE "嗜虐快楽経験"
				経験名 = 虐快経験
			CASE "主人調教経験"
				経験名 = 主調経験
			CASE "助手調教経験"
				経験名 = 助調経験
			CASE "Ｖ拡張経験"
				経験名 = Ｖ拡経験
			CASE "Ａ拡張経験"
				経験名 = Ａ拡経験
			CASE "ダンス経験"
				経験名 = 舞踊経験
			CASE "音ゲー経験"
				経験名 = 音ゲ経験
			CASE "乳管拡張経験"
				経験名 = 乳拡経験
			CASE "臍拡張経験"
				経験名 = 臍拡経験
			CASEELSE
				経験名 = %EXPNAME:LCOUNT%
		ENDSELECT
	ENDIF

	表示用:(表示数%一行表示数) = %SET_TOOLTIP(@"　%経験名, 8%:%表示用経験%", EXP_COMMENT(EXPNAME:LCOUNT, ARG))%
	RESETCOLOR
	表示数++
NEXT
SIF !LINEISEMPTY()
	PRINTL 

