	IF LCOUNT == VARSIZE("TALENT")-1
		HTML_PRINT STRJOIN(表示用, "")
		BREAK
	ELSEIF TALENT:ARG:LCOUNT == 0
		CONTINUE
	ENDIF

	IF TALENTNAME:LCOUNT == "バストサイズ"
		IF LANGUAGE == "ENG"
			SELECTCASE TALENT:ARG:バストサイズ
				CASE 1
					素質名 = [NoneBreast]
				CASE 2
					素質名 = [LittleBreast]
				CASE 3
					素質名 = [BasicBreast]
				CASE 4
					素質名 = [LargeBreast]
				CASE 4
					素質名 = [XLargeBreast]
				CASE 6
					素質名 = [HugeBreast]
			ENDSELECT
		ELSE
			SELECTCASE TALENT:ARG:バストサイズ
				CASE 1
					素質名 = [絶壁]
				CASE 2
					素質名 = [貧乳]
				CASE 3
					素質名 = [並乳]
				CASE 4
					素質名 = [巨乳]
				CASE 5
					素質名 = [豊乳]
				CASE 6
					素質名 = [爆乳]
			ENDSELECT
		ENDIF
	ELSE
		素質名 = [%TALENTDISP(LCOUNT)%]
	ENDIF

	SIF 表示数%6 == 0
		表示用 = 　

	表示用:(表示数%6) += @"%SET_TOOLTIP(素質名, TALENT_COMMENT(TALENTNAME:LCOUNT, ARG))%"

	表示数++

	;改行するのか？ 続けて出力するのか？
	IF 表示数%6 == 0 && 表示数 != 0
		HTML_PRINT STRJOIN(表示用, "")
		VARSET 表示用
	ENDIF
NEXT

