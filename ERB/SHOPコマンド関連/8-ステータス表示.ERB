		CASE IS >= 9
			パラメータ = PALAM:LCOUNT / POWER(10,6)
			IF RESULT == 9
				記号 = .%CHARATU(TOSTR(PALAM:LCOUNT),3)%M
			ELSE
				記号 = M
			ENDIF
		CASE IS >= 7
			パラメータ = PALAM:LCOUNT / POWER(10,3)
