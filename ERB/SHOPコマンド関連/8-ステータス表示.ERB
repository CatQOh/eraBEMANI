	IF LCOUNT == VARSIZE("ABL")-1
		HTML_PRINT STRJOIN(表示用, "")
		BREAK
	ELSEIF ABLNAME:LCOUNT == ""
		CONTINUE
	ENDIF

	;改行するのか？ 続けて出力するのか？
	IF 表示数%6 == 0 && 表示数 != 0
		HTML_PRINT STRJOIN(表示用, "")
		VARSET 表示用
	ENDIF

	SELECTCASE SEX(ARG)
		CASE 1
			;男ならレズっ気、レズ中毒、噴乳中毒は灰色表示
			SELECTCASE ABLNAME:LCOUNT
				CASE "レズっ気", "レズ中毒", "噴乳中毒"
					SETCOLORBYNAME gray
					表示用レベル = -----
				CASE "Ｖ感覚"
					;ヴァギナ無ければＶ感覚も灰色
					IF VAGINA(ARG)
						SETCOLORBYNAME silver
						表示用レベル = {ABL:ARG:LCOUNT, 5, LEFT}
					ELSE
						SETCOLORBYNAME gray
						表示用レベル = -----
					ENDIF
				CASEELSE
					SETCOLORBYNAME silver
					表示用レベル = {ABL:ARG:LCOUNT, 5, LEFT}
			ENDSELECT
		CASE 2
			;女ならホモっ気、ゲイ中毒、メス度は灰色表示
			SELECTCASE ABLNAME:LCOUNT
				CASE "ホモっ気", "ゲイ中毒", "メス度"
					SETCOLORBYNAME gray
					表示用レベル = -----
				CASEELSE
					SETCOLORBYNAME silver
					表示用レベル = {ABL:ARG:LCOUNT, 5, LEFT}
			ENDSELECT
	ENDSELECT

	IF LANGUAGE == "ENG"
		表示用:(表示数%6) = %SET_TOOLTIP(@"　%ABLDISP(LCOUNT), 12%:LV%表示用レベル%", ABL_COMMENT(ABLNAME:LCOUNT, ARG))%
	ELSE
		表示用:(表示数%6) = %SET_TOOLTIP(@"　%ABLDISP(LCOUNT), 8%:LV%表示用レベル%", ABL_COMMENT(ABLNAME:LCOUNT, ARG))%
	ENDIF
	RESETCOLOR
	表示数++
NEXT

SIF !LINEISEMPTY()
	PRINTL 

