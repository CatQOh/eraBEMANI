;PALAM用ツールチップ
@PALAM_COMMENT, ARGS
#FUNCTIONS
#DIMS DYNAMIC ツールチップ

SELECTCASE ARGS
	CASE "快Ｃ", "快Ｖ", "快Ａ", "快Ｂ"
		IF ARGS == "快Ｖ" && !VAGINA(TARGET)
			ツールチップ = 男性には無縁のステータスです
		ELSE
			SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
				CASE 0
					ツールチップ = 特に感じていないようです
				CASE 1
					ツールチップ = 少し気持ち良くなってきたようです
				CASE 2
					ツールチップ = 確実に感じてきているようです
				CASE 3
					ツールチップ = もう少しで絶頂に達してしまいそうです
			ENDSELECT
		ENDIF
	CASE "潤滑"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				ツールチップ = ほとんど濡れていません
			CASE 1
				ツールチップ = 少し湿りを帯びています
			CASE 2
				ツールチップ = だんだんと濡れてきたようです
			CASE 3
				IF VAGINA(TARGET)
					ツールチップ = 愛液が目に見えて溢れてきました
				ELSE
					ツールチップ = それなりの潤滑を得ているようです
				ENDIF
			CASE IS >= 4
				IF VAGINA(TARGET)
					ツールチップ = 膣周りはすっかりと濡れそぼっています
				ELSE
					ツールチップ = 行為に支障をきたさない程度には濡れています
				ENDIF
		ENDSELECT
	CASE "勃起"
		IF !PENIS(TARGET)
			ツールチップ = 女性には無縁のステータスです
		ELSE
			SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
				CASE 0
					ツールチップ = まだ勃っていません
				CASE 1
					ツールチップ = 少し硬くなってきました
				CASE 2
					ツールチップ = 半勃ち状態です
				CASE 3
					ツールチップ = 十分に勃起しています
			ENDSELECT
		ENDIF
	CASE "恭順"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				IF CFLAG:陥落
					ツールチップ = %CALLNAME:PLAYER%との行為を心待ちにしているようです
				ELSE
					ツールチップ = あまり気乗りしていないようです
				ENDIF
			CASE 1 TO 3
				IF CFLAG:陥落
					ツールチップ = %CALLNAME:PLAYER%ともっと深く交わりたいようです
				ELSE
					ツールチップ = まだ%CALLNAME:PLAYER%に対してよそよそしさが目立ちます
				ENDIF
			CASE 4 TO 7
				IF CFLAG:陥落
					ツールチップ = 互いに求め合うことが幸せな様子です
				ELSE
					ツールチップ = 少しづつ%CALLNAME:PLAYER%に気を許し始めてきたようです
				ENDIF
			CASEELSE
				ツールチップ = %CALLNAME:PLAYER%と愛し合うことにすっかり夢中のようです
		ENDSELECT
	CASE "欲情"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				IF CFLAG:陥落
					ツールチップ = 期待に満ちた目で%CALLNAME:PLAYER%を見つめています
				ELSE
					ツールチップ = あまり関心が無い様子です
				ENDIF
			CASE 1 TO 3
				ツールチップ = まだ満足には至らないようです
			CASE 4 TO 7
				ツールチップ = 理性が働かなくなってきたようです
			CASEELSE
				ツールチップ = すっかり快楽の虜になってしまったようです
		ENDSELECT
	CASE "屈服"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				ツールチップ = 平常心を保っているようです
			CASE 1 TO 3
				IF CFLAG:陥落
					ツールチップ = もっと激しくされたいようです
				ELSE
					ツールチップ = まだ反抗心が伺えます
				ENDIF
			CASE 4 TO 7
				IF CFLAG:陥落
					ツールチップ = %CALLNAME:PLAYER%に身を委ねています
				ELSE
					ツールチップ = 素直に従うようになってきました
				ENDIF
			CASEELSE
				ツールチップ = %CALLNAME:PLAYER%のなすがままになっています
		ENDSELECT
	CASE "習得"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0 TO 2
				ツールチップ = まだ本調子ではないようです
			CASE 3 TO 5
				ツールチップ = コツを掴んできたようです
			CASEELSE
				ツールチップ = 互いの感じる部分を覚えてしまったようです
		ENDSELECT
	CASE "恥情"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				ツールチップ = 特に何も感じていないようです
			CASE 1 TO 3
				ツールチップ = 少し恥ずかしい様子です
			CASE 4 TO 7
				ツールチップ = 羞恥に快楽が伴ってきたようです
			CASEELSE
				ツールチップ = 恥ずかしいことが気持ち良いようです
		ENDSELECT
	CASE "苦痛"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				ツールチップ = 特に痛くはないようです
			CASE 1 TO 3
				ツールチップ = 痛みが伴っているようです
			CASE 4 TO 7
				ツールチップ = 必死に痛みに耐えています
			CASEELSE
				ツールチップ = この上ない痛みを感じているようです
		ENDSELECT
	CASE "恐怖"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				ツールチップ = 何も恐れてはいない様子です
			CASE 1 TO 3
				ツールチップ = 少しばかり%CALLNAME:PLAYER%を恐れているようです
			CASE 4 TO 7
				ツールチップ = 心底恐れおののいているようです
			CASEELSE
				ツールチップ = 恐怖のあまり体が震えているようにも見えます
		ENDSELECT
	CASE "反感"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				ツールチップ = まだ不満ではないようです
			CASE 1 TO 3
				ツールチップ = 少々満足いかないようです
			CASE 4 TO 7
				ツールチップ = 表情に苛つきが募り始めています
			CASEELSE
				ツールチップ = 態度に怒りが満ちています
		ENDSELECT
	CASE "不快"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				ツールチップ = 特に不快ではないようです
			CASE 1 TO 3
				ツールチップ = あまり快くはない様子です
			CASE 4 TO 7
				ツールチップ = 嫌がる素振りを見せています
			CASEELSE
				ツールチップ = すっかり気分を害してしまったようです
		ENDSELECT
	CASE "抑鬱"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				ツールチップ = 平常心を保っています
			CASE 1 TO 3
				ツールチップ = 若干気分が沈み始めてきたようです
			CASE 4 TO 7
				ツールチップ = モチベーションを失いかけています
			CASEELSE
				ツールチップ = すっかりやる気を失ってしまったようです
		ENDSELECT
	CASEELSE
		;#;THROW パラメータ名「%ARGS%」のツールチップが設定されていません
ENDSELECT

RETURNF ツールチップ

