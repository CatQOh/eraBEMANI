@EVENT_朝騎乗位
#DIM DYNAMIC CHK_V;1ならＶ、0ならＡ
#DIM DYNAMIC プレイ回数
#DIM DYNAMIC 射精回数

DRAWLINE
CALL KOJO_MESSAGE_EVENT, "朝騎乗位"

;どこの穴でするか
IF ABL:Ｖ感覚 >= 3 && VAGINA(TARGET) && !TALENT:処女 && !TALENT:再生処女
	CHK_V = 1
ELSE
	CHK_V = 0
ENDIF

;何回するか
プレイ回数 = (RELATION:TARGET:(NO:MASTER))/60
プレイ回数 += ABL:性交中毒
プレイ回数 += ABL:精液中毒/2
プレイ回数 += ABL:奉仕精神/4
SIF TALENT:献身的
	プレイ回数++
SIF TALENT:淫乱
	プレイ回数 += 2
SIF TALENT:娼婦
	プレイ回数 += 3
IF CHK_V
	IF TALENT:淫膣
		プレイ回数 += 5
		プレイ回数 += ABL:Ｖ感覚/10
	ENDIF
ELSE
	IF TALENT:淫尻
		プレイ回数 += 5
		プレイ回数 += ABL:Ａ感覚/10
	ENDIF
ENDIF
SIF プレイ回数 < 1
	プレイ回数 = 1

;何発出したか
射精回数 = 10*プレイ回数;小数を扱うため一旦10倍
SIF !TALENT:MASTER:絶倫
	TIMES 射精回数, 0.60
SIF TALENT:小柄体型
	TIMES 射精回数, 1.20
射精回数 *= ABL:技巧
射精回数 /= 30
射精回数 *= (10 + ABL:MASTER:Ｃ感覚)
射精回数 /= 10
射精回数 /= 10
射精回数 = LIMIT(射精回数, 1, プレイ回数)
IF TALENT:MASTER:絶倫
	射精回数 = MIN(射精回数, 15)
ELSE
	射精回数 = MIN(射精回数, 10)
ENDIF

IF CHK_V
	EXP:Ｖ経験 += プレイ回数
	CFLAG:騎乗位で射精させた経験++
	JUEL:快Ｖ += プレイ回数*100
	IF PENIS(TARGET)
		EXP:射精経験++
		CFLAG:膣を責められて射精した経験++
		CFLAG:犯されて射精した経験++
	ENDIF
	EXP:膣射経験 += 射精回数
	PREG:0 += 射精回数
	CALL IN_VAGINA
ELSE
	EXP:Ａ経験 += プレイ回数
	CFLAG:アナル騎乗位で射精させた経験++
	JUEL:快Ａ += プレイ回数*100
	IF PENIS(TARGET)
		EXP:射精経験++
		CFLAG:アナルを責められて射精した経験++
		CFLAG:アナルを犯されて射精した経験++
	ENDIF
ENDIF
EXP:性交経験 += プレイ回数
EXP:MASTER:性交経験 += プレイ回数
EXP:精液経験 += 射精回数
EXP:MASTER:射精経験 += 射精回数
JUEL:恭順 += プレイ回数*100
JUEL:屈服 += プレイ回数*30
JUEL:習得 += プレイ回数*40

PRINTFORM %CALLNAME:MASTER%のペニスを咥え込んで離さない%CALLNAME:TARGET%は、卑猥な腰使いで
SIF 射精回数 > 1
	PRINTFORM {射精回数}回ほど
PRINTW 精液を搾り取っていった

IF CHK_V
	PRINTFORML Ｖ経験+{プレイ回数}
ELSE
	PRINTFORML Ａ経験+{プレイ回数}
ENDIF
PRINTFORML 性交経験+{プレイ回数}
経験回数:MASTER:(NO:TARGET) += 1
経験回数:TARGET:(NO:MASTER) += 1

PRINTFORML 精液経験+{射精回数}
IF CHK_V
	PRINTFORML 膣射経験+{射精回数}
	PRINTFORML 快Ｖの珠+{プレイ回数*100}
	中出し回数:MASTER:(NO:TARGET) += 射精回数
	中出され回数:(NO:MASTER) += 射精回数
ELSE
	PRINTFORML 快Ａの珠+{プレイ回数*100}
	アナル中出し回数:MASTER:(NO:TARGET) += 射精回数
	アナル中出され回数:(NO:MASTER) += 射精回数
ENDIF
SIF PENIS(TARGET)
	PRINTFORML 射精経験+1
PRINTFORML 恭順の珠+{プレイ回数*100}
PRINTFORML 屈服の珠+{プレイ回数*30}
PRINTFORML 習得の珠+{プレイ回数*40}
PRINTFORMW 射精経験+{射精回数}（%CALLNAME:MASTER%）

CFLAG:朝騎乗位経験++
CFLAG:MASTER:朝騎乗位経験++

;-------------------------------------------------
;調教後に起こるイベント
;-------------------------------------------------
@SELF_CHECK
;調教者がペニス持ちで、調教者の射精残数が0の場合は発生しない
;SIF PENIS(MASTER) && CFLAG:MASTER:残数 == 0
;	RETURN 0

;アニマルだと発生しない
SIF TALENT:アニマル
	RETURN 0

;調教後3P、調教後セックスどちらか一つ（強精神剤の反動が起こる時は無効）
;助手がいる場合は3P判定 対象と助手が陥落済み、対象と助手間の相性がそれぞれ100より↑、対象の体力500以上
IF ASSI >= 0 && CFLAG:陥落 && CFLAG:ASSI:陥落 && RELATION:(NO:ASSI) > 100 && RELATION:ASSI:(NO:TARGET) > 100 && BASE:体力 >= 500 && TEQUIP:強精神薬 <= 2
	;どちらかが貞操帯つけてると発生しない
	SIF CFLAG:貞操帯 || CFLAG:ASSI:貞操帯
		GOTO ELSE
	;ただし出産トラウマがある場合は発生しない時がある
	LOCAL = CFLAG:出産トラウマ+CFLAG:ASSI:出産トラウマ
	;1+(合計値/10)/2+(合計値/10) 
	SIF !(LOCAL && RAND:(LOCAL/10+2) <= LOCAL/10)
		CALL EVENT_CHECK, "調教後3P"
;ふたりエッチ判定 対象が陥落済み、対象の体力500以上
ELSEIF CFLAG:陥落 && BASE:体力 >= 500 && TEQUIP:強精神薬 <= 2
	;貞操帯装着中は発生しない
	SIF CFLAG:貞操帯
		GOTO ELSE
	;出産トラウマがある場合はry 式は上と同じ1
	LOCAL = CFLAG:出産トラウマ
	SIF !(LOCAL && RAND:(LOCAL/10+2) <= LOCAL/10)
		CALL EVENT_CHECK, "調教後セックス"
ENDIF

$ELSE

;強精神薬の反動、調教後搾乳、調教後自慰。どれか一つ
IF TEQUIP:強精神薬 > 2
	CALL EVENT_CHECK, "強精神薬の反動"
ELSEIF ABL:Ｂ感覚 >= 4 && (ABL:噴乳中毒 > ABL:自慰中毒) && TALENT:母乳体質
	CALL EVENT_CHECK, "調教後搾乳"
ELSEIF ABL:自慰中毒 >= 3 && !CFLAG:貞操帯
	CALL EVENT_CHECK, "調教後自慰"
ENDIF

;調教後夜這い
SIF CFLAG:陥落 && TIME == 1
	CALL EVENT_CHECK, "調教後夜這い"

;調教後3P
@EVENT_調教後3P
#DIM DYNAMIC プレイ回数
#DIM DYNAMIC 射精回数基礎値, 3;0,1,2でMASTER,ASSI,TARGET
#DIM DYNAMIC 射精回数, 3, 3;射精回数:(出す人):(出される人)
#DIM DYNAMIC V2

射精回数基礎値:0 = CFLAG:MASTER:残数/5+1
射精回数基礎値:1 = CFLAG:ASSI:残数/5+1
射精回数基礎値:2 = CFLAG:TARGET:残数/5+1

;イベント口上呼び出し
DRAWLINE
CALL KOJO_MESSAGE_EVENT, "調教後3P"
LOCAL = 1

;対象：主人か助手に同性がいればレズ中毒、ゲイ中毒を見る
IF SEX(TARGET) == SEX(MASTER) || SEX(TARGET) == SEX(ASSI)
	IF SEX(TARGET) == 1
		SELECTCASE ABL:ゲイ中毒
			CASE 1
				LOCAL += 1
			CASE 2
				LOCAL += 2
			CASE 3
				LOCAL += 3
			CASE 4
				LOCAL += 5
			CASE IS >= 5
				LOCAL += 7
		ENDSELECT
	ELSEIF SEX(TARGET) == 2
		SELECTCASE ABL:レズ中毒
			CASE 1
				LOCAL += 1
			CASE 2
				LOCAL += 2
			CASE 3
				LOCAL += 3
			CASE 4
				LOCAL += 5
			CASE IS >= 5
				LOCAL += 7
		ENDSELECT
	ENDIF
;いなければ奉仕精神を見る
ELSE
	SELECTCASE ABL:奉仕精神
		CASE 1 TO 4
			LOCAL += ABL:奉仕精神+1
		CASE IS >= 5
			LOCAL += 6
	ENDSELECT
ENDIF

;助手：奴隷が同性ならレズ中毒、ゲイ中毒を見る
IF SEX(ASSI) == SEX(TARGET)
	IF SEX(TARGET) == 1
		SELECTCASE ABL:ゲイ中毒
			CASE 1
				LOCAL += 1
			CASE 2
				LOCAL += 2
			CASE 3
				LOCAL += 5
			CASE 4
				LOCAL += 8
			CASE IS >= 5
				LOCAL += 13
		ENDSELECT
	ELSEIF SEX(TARGET) == 2
		SELECTCASE ABL:レズ中毒
			CASE 1
				LOCAL += 1
			CASE 2
				LOCAL += 2
			CASE 3
				LOCAL += 5
			CASE 4
				LOCAL += 8
			CASE IS >= 5
				LOCAL += 13
		ENDSELECT
	ENDIF
;同性でなければ技巧を見る
ELSE
	SELECTCASE ABL:技巧
		CASE 1 TO 4
			LOCAL += ABL:技巧+2
		CASE IS >= 5
			LOCAL += 7
	ENDSELECT
ENDIF

;両刀
SIF TALENT:両刀
	LOCAL += 3

;相性処理
LOCAL *= RELATION:(NO:ASSI)
LOCAL /= 100

;主人か助手に同性がいれば同性愛経験増加
IF SEX(TARGET) == SEX(MASTER) || SEX(TARGET) == SEX(ASSI)
	SELECTCASE SEX(TARGET)
		CASE 1
			EXP:ゲイ経験 += LOCAL
			PRINTFORML ゲイ経験+{LOCAL}(%CALLNAME:TARGET%)
		CASE 2
			EXP:レズ経験 += LOCAL
			PRINTFORML レズ経験+{LOCAL}(%CALLNAME:TARGET%)
	ENDSELECT
ENDIF

;数値次第で絶頂
SIF LOCAL*100*ABL:従順/2500 > 0
	PRINTFORML 絶頂経験+{LOCAL*100*ABL:従順/2500}(%CALLNAME:TARGET%)
PRINTFORML 快Ｃの珠+{LOCAL*10*ABL:Ｃ感覚}(%CALLNAME:TARGET%)
PRINTFORML 欲情の珠+{LOCAL*50}(%CALLNAME:TARGET%)

EXP:絶頂経験 += LOCAL*100*ABL:従順/2500
JUEL:快Ｃ += LOCAL*10*ABL:Ｃ感覚
JUEL:欲情 += LOCAL*50

;奴隷が男で他に竿役がいれば奴隷掘られる
IF SEX(TARGET) == 1 && (PENIS(MASTER) || PENIS(ASSI))
	SELECTCASE ABL:Ａ感覚
		CASE 0 TO 4
			LOCAL:1 = ABL:Ａ感覚+1
		CASE IS >= 5
			LOCAL:1 = 6
	ENDSELECT
	プレイ回数 = (LOCAL/5+1)*LOCAL:1
	IF PENIS(MASTER)
		EXP:性交経験 += プレイ回数
		EXP:MASTER:性交経験 += プレイ回数
		経験回数:MASTER:(NO:TARGET) += プレイ回数
		経験回数:(NO:MASTER) += プレイ回数
		射精回数:0:2 += 射精回数基礎値:0
		アナル中出し回数:MASTER:(NO:TARGET) += 射精回数基礎値:0
		アナル中出され回数:(NO:MASTER) += 射精回数基礎値:0
	ENDIF
	IF PENIS(ASSI)
		EXP:性交経験 += プレイ回数
		EXP:ASSI:性交経験 += プレイ回数
		経験回数:ASSI:(NO:TARGET) += プレイ回数
		経験回数:(NO:ASSI) += プレイ回数
		射精回数:1:2 += 射精回数基礎値:1
		アナル中出し回数:ASSI:(NO:TARGET) += 射精回数基礎値:1
		アナル中出され回数:(NO:ASSI) += 射精回数基礎値:1
	ENDIF
	SIF PENIS(MASTER) && PENIS(ASSI)
