;奴隷からの奉仕などのイベント
;-------------------------------------------------
;朝に起こるイベント
;-------------------------------------------------
@EVENT_NEWDAY_SELF
;調教対象が居ない場合は終了
SIF TARGET < 0
	RETURN 0

;アニマルで懐いてる場合は朝に甘えるイベント
IF TALENT:アニマル && CFLAG:依存度 >= 1500
	CALL EVENT_CHECK, "アニマル"
	RETURN
ENDIF

;陥落素質が無い場合は関数終了
SIF !CFLAG:陥落
	RETURN 0

;主人が射精ができない場合は関数終了
SIF !PENIS(MASTER)
	RETURN 0

;対象が二日酔い起こしてると発生しない
SIF CFLAG:二日酔い
	RETURN 0

;朝騎乗位判定
;出産トラウマがある場合は発生しない時がある
LOCAL = CFLAG:出産トラウマ
;1+(トラウマ値/10)/2+(トラウマ値/10) 
IF ((!TALENT:処女 && !TALENT:再生処女 && ABL:Ｖ感覚 >= 3) || ABL:Ａ感覚 >= 3) && ABL:奉仕精神 >= 7 && !(LOCAL && RAND:(LOCAL/10+2) <= LOCAL/10)
	CALL EVENT_CHECK, "朝騎乗位"
;朝パイズリ判定
ELSEIF TALENT:バストサイズ >= 2 && ABL:奉仕精神 >= 5
	CALL EVENT_CHECK, "朝パイズリ"
;朝フェラ判定
ELSE
	CALL EVENT_CHECK, "朝フェラ"
ENDIF

;DISABLE_EVENTが行われていた場合は表示しない
SIF RESULT == 0
	RETURN 0

SELECTCASE FALLTYPE(TARGET)
	CASE 1
		PRINTFORMW %CALLNAME:TARGET%たちは\@ FLAG:ゲームモード == 7 ? 朝から濃厚な # 調教そっちのけで \@情事に耽っている……
		EXP:愛情経験 += 5
		JUEL:恭順 += 250
		PRINTL 愛情経験+5
		PRINTL 恭順の珠+250
		IF SEX(MASTER) == 1 && SEX(TARGET) == 1
			EXP:ゲイ経験 += 5
			PRINTL ゲイ経験+5
		ELSEIF SEX(MASTER) == 2 && SEX(TARGET) == 2
			EXP:レズ経験 += 5
			PRINTL レズ経験+5
		ENDIF
	CASE 2
		PRINTFORMW %CALLNAME:TARGET%は%CALLNAME:MASTER%の腕を離さずに次の夢へと誘っている……
		EXP:精液経験 += 5
		JUEL:欲情 += 250
		PRINTL 精液経験+5
		PRINTL 欲情の珠+250
		IF SEX(MASTER) == 1 && SEX(TARGET) == 1
			EXP:ゲイ経験 += 5
			PRINTL ゲイ経験+5
		ELSEIF SEX(MASTER) == 2 && SEX(TARGET) == 2
			EXP:レズ経験 += 5
			PRINTL レズ経験+5
		ENDIF
	CASE 3
		PRINTFORMW %CALLNAME:TARGET%は今朝も%CALLNAME:MASTER%への忠誠を誓っている……
		EXP:主人調教経験 += 20
		JUEL:屈服 += 250
		PRINTL 主人調教経験+20
		PRINTL 屈服の珠+250
		IF SEX(MASTER) == 1 && SEX(TARGET) == 1
			EXP:ゲイ経験 += 5
			PRINTL ゲイ経験+5
		ELSEIF SEX(MASTER) == 2 && SEX(TARGET) == 2
			EXP:レズ経験 += 5
			PRINTL レズ経験+5
		ENDIF
ENDSELECT

CALL CHECKDRAWLINEW


@EVENT_朝フェラ
#DIM DYNAMIC フェラ回数
#DIM DYNAMIC 射精回数

DRAWLINE
CALL KOJO_MESSAGE_EVENT, "朝フェラ"

;ここからフェラ回数の計算
フェラ回数 = (RELATION:TARGET:(NO:MASTER))/100
フェラ回数 += ABL:精液中毒

SIF TALENT:汚臭鈍感
	フェラ回数++
SIF TALENT:汚臭敏感
	フェラ回数--
SIF TALENT:潔癖症
	フェラ回数--
SIF TALENT:献身的
	フェラ回数++
SIF TALENT:淫乱
	フェラ回数 += 2
SIF TALENT:娼婦
	フェラ回数 += 3

SIF フェラ回数 < 1
	フェラ回数 = 1

;ここから射精回数の計算
;ただしどれだけ多くてもフェラ回数よりは多く射精しない
射精回数 = 10*フェラ回数;整数しか扱えない関係で一旦10倍に
SIF !TALENT:汚れ無視
	TIMES 射精回数, 0.90
SIF !TALENT:舌使い
	TIMES 射精回数, 0.80
SIF !TALENT:MASTER:絶倫
	TIMES 射精回数, 0.60
射精回数 *= ABL:技巧
射精回数 /= 30
射精回数 *= (10 + ABL:MASTER:Ｃ感覚)
射精回数 /= 10
射精回数 /= 10
射精回数 = LIMIT(射精回数, 1, フェラ回数)
IF TALENT:MASTER:絶倫
	射精回数 = MIN(射精回数, 15)
ELSE
	射精回数 = MIN(射精回数, 10)
ENDIF

EXP:フェラ経験 += フェラ回数
EXP:精液経験 += 射精回数
JUEL:恭順 += フェラ回数*100
JUEL:屈服 += フェラ回数*30
JUEL:習得 += フェラ回数*40
EXP:MASTER:射精経験 += 射精回数
PRINTFORMW 今朝は%CALLNAME:TARGET%のフェラで{射精回数}回射精した

PRINTFORML フェラ経験+{フェラ回数}
PRINTFORML 精液経験+{射精回数}
PRINTFORML 恭順の珠+{フェラ回数*100}
PRINTFORML 屈服の珠+{フェラ回数*30}
PRINTFORML 習得の珠+{フェラ回数*40}
PRINTFORMW 射精経験+{射精回数}（%CALLNAME:MASTER%）

CFLAG:朝フェラ経験++
CFLAG:MASTER:朝フェラ経験++
CFLAG:口で射精させた経験++


