;=============================================================================
;経験表示
@SHOW_INFO_EXP, ARG
#DIMS DYNAMIC 表示用経験
#DIMS DYNAMIC 経験名
VARSET LOCALS
LOCAL = 0
REPEAT VARSIZE("EXP")
	IF COUNT == VARSIZE("EXP")-1
		HTML_PRINT LOCALS+LOCALS:1+LOCALS:2+LOCALS:3+LOCALS:4+LOCALS:5
		BREAK
	ELSEIF EXPNAME:COUNT == ""
		CONTINUE
	ENDIF

	;改行するのか？ 続けて出力するのか？
	IF LOCAL%6 == 0 && LOCAL != 0
		HTML_PRINT LOCALS+LOCALS:1+LOCALS:2+LOCALS:3+LOCALS:4+LOCALS:5
		VARSET LOCALS
	ENDIF

	表示用経験 = %TOSTR(EXP:ARG:COUNT), 7%

	SELECTCASE EXPNAME:COUNT
		CASE "レズ経験", "噴乳経験"
			;男ならレズ経験、噴乳経験は灰色表示
			IF SEX(ARG) == 1
				SETCOLORBYNAME gray
				表示用経験 = -------
			ENDIF
		;女ならゲイ経験は灰色表示
		CASE "ゲイ経験"
			IF SEX(ARG) == 2
				SETCOLORBYNAME gray
				表示用経験 = -------
			ENDIF
		;膣が無ければＶ経験、膣射経験、Ｖ拡張経験。出産経験、流産経験も灰色
		CASE "Ｖ経験", "膣射経験", "Ｖ拡張経験", "流産経験"
			IF !VAGINA(ARG)
				SETCOLORBYNAME gray
				表示用経験 = -------
			ENDIF
		;ペニス無ければ射精経験も灰色
		CASE "射精経験"
			IF !PENIS(ARG)
				SETCOLORBYNAME gray
				表示用経験 = -------
			ENDIF
	ENDSELECT


	SELECTCASE EXPNAME:COUNT
		CASE "奉仕快楽経験"
			経験名 = 奉快経験
		CASE "フェラ経験"
			経験名 = 口淫経験
		CASE "苦痛快楽経験"
			経験名 = 痛楽経験
		CASE "嗜虐快楽経験"
			経験名 = 虐快経験
		CASE "主人調教経験"
			経験名 = 主調経験
		CASE "助手調教経験"
			経験名 = 助調経験
		CASE "Ｖ拡張経験"
			経験名 = Ｖ拡経験
		CASE "Ａ拡張経験"
			経験名 = Ａ拡経験
		CASE "ダンス経験"
			経験名 = 舞踊経験
		CASE "音ゲー経験"
			経験名 = 音ゲ経験
		CASEELSE
			経験名 = %EXPNAME:COUNT%
	ENDSELECT

	LOCALS:(LOCAL%6) = %SET_TOOLTIP(@"　%経験名, 8%:%表示用経験%", EXP_COMMENT(EXPNAME:COUNT, ARG))%
	RESETCOLOR
	LOCAL++
REND
SIF !LINEISEMPTY()
	PRINTL 

