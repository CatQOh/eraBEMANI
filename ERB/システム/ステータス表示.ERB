;パラメータ表示
@SHOW_INFO_PALAM
#DIMS DYNAMIC 記号
LOCAL = 0
VARSET LOCALS
REPEAT 15
	;LOCAL:0 = 表示されるパラメータの数値。kMGT等と一緒に表示される
	;LOCALS:0 = LOCAL:0に続くkMGT等の単位(小数点表記をする場合".N"も前に付く)

	;ヴァギナ無いなら快Ｖは灰色表示
	IF !VAGINA(TARGET) && PALAMNAME:COUNT == "快Ｖ"
		SETCOLORBYNAME gray
	ELSE
		SETCOLORBYNAME silver
	ENDIF

	SIF COUNT%3 == 0
		LOCALS = "　"
	;パラメータの名前・パラメータレベル
	IF !VAGINA(TARGET) && PALAMNAME:COUNT == "快Ｖ"
		LOCALS = %PALAMNAME:COUNT%Lv-
	ELSE
		LOCALS = %PALAMNAME:COUNT%Lv{GETPALAMLV(PALAM:COUNT, 9)}
	ENDIF

	;ゲージ処理 パラメータレベルが9以上なら余計な処理は飛ばす
	IF GETPALAMLV(PALAM:COUNT, 9) >= 9
		LOCALS += "[**********] "
	ELSE
		LOCALS += @"%BARSTR(PALAM:COUNT, PALAMLV:(GETPALAMLV(PALAM:COUNT, 9)+1), 10)% "
	ENDIF

	;これだけ特殊
	IF PALAMNAME:COUNT == "潤滑" && PALAM:COUNT >= 20000
		LOCALS += "濡                "
		GOTO SET
	ENDIF

;	STRLENFORM {PALAM:COUNT}

	; 1,000,000,000,000,000,000
	;  E   P   T   G   M   k
	;ｴｸｻ ﾍﾟﾀ  ﾃﾗｷﾞｶﾞ ﾒｶﾞ  ｷﾛ
	;表示領域は半角6文字分なので……
	; 1桁- 6桁:そのまま
	; 7桁- 8桁:k表記
	; 9桁-11桁:M表記
	;12桁-14桁:G表記
	;15桁-17桁:T表記
	;18桁-19桁:P表記
	;2^63≒9223P (9.223E)
	;9,12,15,18桁を直すと残りが3桁になって寂しいので小数点表記を追加
	記号 = 
	STRLENS TOSTR(PALAM:COUNT)
	SELECTCASE RESULT
		CASE IS >= 18
			LOCAL = PALAM:COUNT / POWER(10,15)
			IF RESULT == 18
				記号 = .%CHARATU(TOSTR(PALAM:COUNT),3)%P
			ELSE
				記号 = P
			ENDIF
		CASE IS >= 15
			LOCAL = PALAM:COUNT / POWER(10,12)
			IF RESULT == 15
				記号 = .%CHARATU(TOSTR(PALAM:COUNT),3)%T
			ELSE
				記号 = T
			ENDIF
		CASE IS >= 12
			LOCAL = PALAM:COUNT / POWER(10,9)
			IF RESULT == 12
				記号 = .%CHARATU(TOSTR(PALAM:COUNT),3)%G
			ELSE
				記号 = G
			ENDIF
		CASE IS >= 9
			LOCAL = PALAM:COUNT / POWER(10,6)
			IF RESULT == 9
				記号 = .%CHARATU(TOSTR(PALAM:COUNT),3)%M
			ELSE
				記号 = M
			ENDIF
		CASE IS >= 7
			LOCAL = PALAM:COUNT / POWER(10,3)
			記号 = k
		CASEELSE
			LOCAL = PALAM:COUNT
	ENDSELECT

	IF !VAGINA(TARGET) && PALAMNAME:COUNT == "快Ｖ"
		LOCALS += "------"
	ELSE
		IF PALAM:COUNT <= 999999
			LOCALS += @"{LOCAL, 6, LEFT}"
		ELSE
			LOCALS += @"%TOSTR(LOCAL)+記号, 6, LEFT%"
		ENDIF
	ENDIF

	;カンストしてたら特殊表示
	IF PALAM:COUNT == __INT_MAX__
		LOCALS += " CounterStop"
	ELSE
		LOCALS += " Next/"

		;ヴァギナ無ければ快Ｖは表示しない
		IF !VAGINA(TARGET) && PALAMNAME:COUNT == "快Ｖ"
			LOCALS += "------"
		ELSE
			IF GETPALAMLV(PALAM:COUNT, 9) >= 9
				LOCALS += "MAXLV!"
			ELSE
				LOCALS += @"{PALAMLV:(GETPALAMLV(PALAM:COUNT, 9)+1)-PALAM:COUNT, 6, LEFT}"
			ENDIF
		ENDIF
	ENDIF
	$SET
	LOCALS:(COUNT%3+1) = %SET_TOOLTIP(LOCALS, PALAM_COMMENT(PALAMNAME:COUNT))%
	RESETCOLOR

	;COUNT仕様の都合で2
	SIF COUNT%3 == 2
		HTML_PRINT LOCALS:1+LOCALS:2+LOCALS:3

REND
