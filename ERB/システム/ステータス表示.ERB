@STATUS, ARG
DRAWLINE
PRINTL □ 基本情報
PRINTFORM 　名前:%NAME:ARG%　
IF NO:ARG >= 10000 || CSTR:ARG:種族 == CSVCSTR(NO:ARG, 110)
	PRINTFORM 種族:%CSTR:ARG:種族%　
ELSEIF CSTR:ARG:種族 != CSVCSTR(NO:ARG, 110)
	PRINTFORM 種族:元%CSVCSTR(NO:ARG, 110)%の%CSTR:ARG:種族%　
ENDIF
IF ARG != MASTER && (TALENT:MASTER:孔雀の瞳 || (ASSI > 0 && TALENT:ASSI:孔雀の瞳))
	PRINTFORM (依存度:{CFLAG:ARG:依存度} ストレス値{CFLAG:ARG:ストレス値})　
ELSEIF ARG == MASTER
	PRINTFORM (堕した人数:{FLAG:累計恋慕+FLAG:累計淫乱+FLAG:累計服従}人)　
	PRINT 難易度:
	SELECTCASE FLAG:難易度
		CASE 1
			PRINT NORMAL
		CASE 2
			PRINT HYPER
		CASE 3
			PRINT ANOTHER
	ENDSELECT
ENDIF
PRINTL 
IF ARG != MASTER
	DRAWLINE
	PRINTL □ 珠
	CALL SHOW_INFO_JUEL, ARG
ENDIF
CALL SHOW_INFO, ARG

PRINTL □ 相性
SIF TALENT:ARG:服従
	PRINTL 　全キャラ×120%
SIF TALENT:ARG:隷属
	PRINTL 　全キャラ×150%
LOCAL = 0
REPEAT 20000
	IF EXISTCSV(COUNT) || COUNT > 10000
		SIF COUNT > 10000 && CHILDNAME:(COUNT-10000) == ""
			BREAK
		LOCALS = 
		IF RELATION:ARG:COUNT != 100
			IF !TALENT:ARG:服従 && !TALENT:ARG:隷属
				IF NO:MASTER == COUNT
					LOCALS = 　%NAME:MASTER%×{RELATION:ARG:COUNT}\%
				ELSE
					LOCALS = 　%CSVNAMEF(COUNT)%×{RELATION:ARG:COUNT}\%
				ENDIF
			ELSEIF RELATION:ARG:COUNT > 120 && TALENT:ARG:服従
				IF NO:MASTER == COUNT
					LOCALS = 　%NAME:MASTER%×{RELATION:ARG:COUNT}\%
				ELSE
					LOCALS = 　%CSVNAMEF(COUNT)%×{RELATION:ARG:COUNT}\%
				ENDIF
			ELSEIF RELATION:ARG:COUNT > 150 && TALENT:ARG:隷属
				IF NO:MASTER == COUNT
					LOCALS = 　%NAME:MASTER%×{RELATION:ARG:COUNT}\%
				ELSE
					LOCALS = 　%CSVNAMEF(COUNT)%×{RELATION:ARG:COUNT}\%
				ENDIF
			ENDIF
		ENDIF
		IF LOCALS != ""
			PRINTFORMLC %LOCALS%
			LOCAL++
		ENDIF
		IF LOCAL == 4 && !LINEISEMPTY()
			PRINTL 
			LOCAL = 0
		ENDIF
	ENDIF
REND
SIF !LINEISEMPTY()
	PRINTL 
DRAWLINE
PRINTPLAIN 　BASIC　
PRINT [0] 戻る 
PRINT [5] その他情報 
IF ARG > 0
	PRINT [6] 前のキャラ 
ELSE
	PRINTPLAIN 　　　　　　　 
ENDIF
SIF ARG < CHARANUM-1
	PRINT [7] 次のキャラ 

IF !CHECK_CHILD_CARE(ARG) && !CFLAG:ARG:使用不可
	IF TARGET != ARG && ARG != 0
		PRINT [8] 対象に指定 
	ELSE
		PRINTPLAIN 　　　　　　　 
	ENDIF
	IF ASSI != ARG && ARG != 0 && CFLAG:ARG:状態 >= 2
		PRINT [9] 助手に指定 
	ELSE
		PRINTPLAIN 　　　　　　　 
	ENDIF
ENDIF
PRINTL 

DO
	INPUT
	SELECTCASE RESULT
		CASE 0
			RETURN 0
		CASE 5
			;以下フラグリセットのためにJUMPで処理
			JUMP STATUS_ETC, ARG
		CASE 6
			SIF ARG > 0
				JUMP STATUS, ARG-1
		CASE 7
			SIF ARG < CHARANUM-1
				JUMP STATUS, ARG+1
		CASE 8, 9
			IF !CHECK_CHILD_CARE(ARG) && !CFLAG:ARG:使用不可
				IF RESULT == 8
					SIF TARGET != ARG && ARG != 0
						CALL ASIGN_TARGET, ARG
					RETURN 0
				ENDIF
				IF RESULT == 9
					SIF ASSI != ARG && ARG != 0 && CFLAG:ARG:状態 >= 2
						CALL ASIGN_ASSI, ARG
					RETURN 0
				ENDIF
			ENDIF
	ENDSELECT
LOOP 1

@STATUS_ETC, ARG
#DIM PAGE
#DIM DYNAMIC ドラゴンに処女, 2
#DIM DYNAMIC LCOUNT
#DIM DYNAMIC 家族人数
#DIM DYNAMIC 兄数
#DIM DYNAMIC 弟数
#DIM DYNAMIC 姉数
#DIM DYNAMIC 妹数
#DIM DYNAMIC 息子娘数
#DIM DYNAMIC 孫数
#DIM DYNAMIC 父親
#DIM DYNAMIC 母親
#DIM DYNAMIC 尊属
#DIM DYNAMIC 家族番号簡易版
#DIM DYNAMIC 兄弟姉妹番号簡易版

;処女が主人のドラゴンとなっている場合は、現在のドラゴンの名前を入れて後で戻す
IF VIRGIN:ARG:1 == "主人のドラゴン"
	VIRGIN:ARG:1 = %CSTR:MASTER:ドラゴン%
	ドラゴンに処女:0 = 1
ENDIF
IF VIRGIN:ARG:11 == "主人のドラゴン"
	VIRGIN:ARG:11 = %CSTR:MASTER:ドラゴン%
	ドラゴンに処女:1 = 1
ENDIF



DRAWLINE
PRINTL □ 基本情報
PRINTFORM 　名前:%NAME:ARG%　
PRINT 性別:
SELECTCASE SEX(ARG)
	CASE 1
		PRINT 男　
	CASE 2
		PRINT 女　
ENDSELECT
IF NO:ARG >= 10000 || CSTR:ARG:種族 == CSVCSTR(NO:ARG, 110)
	PRINTFORM 種族:%CSTR:ARG:種族%　
ELSEIF CSTR:ARG:種族 != CSVCSTR(NO:ARG, 110)
	PRINTFORM 種族:元%CSVCSTR(NO:ARG, 110)%の%CSTR:ARG:種族%　
ENDIF
IF ARG != MASTER && (TALENT:MASTER:孔雀の瞳 || (ASSI > 0 && TALENT:ASSI:孔雀の瞳))
	PRINTFORM (依存度:{CFLAG:ARG:依存度} ストレス値{CFLAG:ARG:ストレス値})　
ELSEIF ARG == MASTER && FLAG:ゲームモード != 7
	PRINTFORM 堕した人数:{FLAG:累計恋慕+FLAG:累計淫乱+FLAG:累計服従}人　
	PRINT 難易度:
	SELECTCASE FLAG:難易度
		CASE 1
			PRINT NORMAL
		CASE 2
			PRINT HYPER
		CASE 3
			PRINT ANOTHER
	ENDSELECT
ENDIF
PRINTL 
PRINTL □ その他情報
IF FLAG:ゲームモード == 1 && FLAG:パートナー == NO:ARG
		PRINTFORML 　{REALTIME:0}年{REALTIME:1}月{REALTIME:2}日に%CALLNAME:MASTER%のパートナーになった
ELSEIF FLAG:ゲームモード == 7
	IF NO:ARG >= 10000
		PRINTFORML 　%CSVNAMEF(CFLAG:ARG:血縁コード/100000)%と%CSVNAMEF(CFLAG:ARG:血縁コード%100000)%の\@ CFLAG:ARG:近親交配 ? 近親交配で # 間に \@産まれた\@ SEX(ARG) == 1 ? 息子 # 娘 \@だ
	ELSEIF ARG == MASTER
		REPEAT CHARANUM
			IF CFLAG:COUNT:嫁
				LOCAL = COUNT
				BREAK
			ENDIF
		REND
		PRINTFORML 　{REALTIME:0}年{REALTIME:1}月{REALTIME:2}日に%CALLNAME:LOCAL%と結婚した
	ELSE
		PRINTFORML 　{REALTIME:0}年{REALTIME:1}月{REALTIME:2}日に%CALLNAME:MASTER%と結婚した
	ENDIF
ELSE
	IF ARG == MASTER
		PRINTFORML 　{REALTIME:0}年{REALTIME:1}月{REALTIME:2}日にこの世界へと迷いこむ
		PRINTFORML 　現在は{CHARANUM-1}人の奴隷を扱う調教師
		SIF FLAG:周回数 >= 2
			PRINTFORML 　{FLAG:周回数}周目のゲームであり、この世界を積極的に楽しんでいる
		SIF FLAG:累計恋慕
			PRINTFORML 　累計{FLAG:累計恋慕}人の奴隷の心を奪った
		SIF FLAG:累計淫乱
			PRINTFORML 　累計{FLAG:累計淫乱}人の奴隷を肉欲に溺れさせた
		SIF FLAG:累計服従
			PRINTFORML 　累計{FLAG:累計服従}人の奴隷が%CALLNAME:MASTER%に服従した
	ELSE
		IF NO:ARG == 0
			PRINTFORML 　%CALLNAME:MASTER%が調教師を始めたと聞いて、自ら奴隷になりたいと志願してきた変人である
		ELSEIF NO:ARG >= 10000
			PRINTFORML 　%CSVNAMEF(CFLAG:ARG:血縁コード/100000)%と%CSVNAMEF(CFLAG:ARG:血縁コード%100000)%の\@ CFLAG:ARG:近親交配 ? 近親交配で # 間に \@産まれた\@ SEX(ARG) == 1 ? 息子 # 娘 \@だ
		ELSE
			PRINTFORML 　{BUYY:ARG:0}年{BUYM:ARG:0}月{BUYD:ARG:0}日に購入
		ENDIF
	ENDIF
ENDIF
IF CFLAG:ARG:嫁
	PRINTFORML 　現在は%CALLNAME:MASTER%の嫁である
ELSEIF CFLAG:ARG:専属娼婦
	PRINTFORML 　現在は%CALLNAME:MASTER%専属の娼婦である
ELSEIF CFLAG:ARG:傀儡
	PRINTFORML 　現在は%CALLNAME:MASTER%の支配下に置ける傀儡である
ENDIF

IF CFLAG:ARG:血縁コード && NO:ARG < 10000
	LOCAL = 0
	LOCAL:1 = 0
	FOR LCOUNT, 0, 10000
		SIF LCOUNT == NO:ARG
			CONTINUE
		IF EXISTCSV(LCOUNT) && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "血縁コード")) == CFLAG:ARG:血縁コード
			;その家族で一番高齢（？）に位置する続柄を入れておく
			SIF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) > 尊属
				尊属 = CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄"))
			;「家族番号簡易版」はCSVある家族が一人だけの場合に使う
			家族番号簡易版 = LCOUNT
			IF CFLAG:ARG:続柄 < 1000
				;「兄弟姉妹番号簡易版」はCSVある兄弟姉妹が一人だけの場合に使う
				SELECTCASE CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄"))
					CASE IS < 1000
						兄弟姉妹番号簡易版 = LCOUNT
						SELECTCASE CSVTALENT(LCOUNT, GETNUM(TALENT, "性別"))
							CASE 1
								IF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < CFLAG:ARG:続柄
									兄数++
								ELSE
									弟数++
								ENDIF
							CASE 2
								IF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < CFLAG:ARG:続柄
									姉数++
								ELSE
									妹数++
								ENDIF
						ENDSELECT
					CASE 1000
						父親 = LCOUNT
					CASE 1001
						母親 = LCOUNT
				ENDSELECT
			ELSEIF CFLAG:ARG:続柄 < 2000
				;さすがに息子娘とかくらいしか表示するつもりがない
				SIF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < 1000
					息子娘数++
			ELSEIF CFLAG:ARG:続柄 < 3000
				SIF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < 1000
					孫数++
			ENDIF
		ENDIF
		家族人数++
	NEXT
	SELECTCASE 家族人数
		CASE 1
			IF CFLAG:ARG:続柄 < 1000
				IF 尊属 < 1000
					IF CFLAG:ARG:続柄 == 1
						PRINTFORML 　%CSVNAME(家族番号簡易版)%の\@ SEX(ARG) == 1 ? 兄 # 姉 \@にあたる
					ELSE
						PRINTFORML 　%CSVNAME(家族番号簡易版)%の\@ SEX(ARG) == 1 ? 弟 # 妹 \@にあたる
					ENDIF
				ELSEIF 尊属 < 2000
					PRINTFORML 　%CSVNAME(家族番号簡易版)%の\@ SEX(ARG) == 1 ? 息子 # 娘 \@にあたる
				ELSEIF 尊属 < 3000
					PRINTFORML 　%CSVNAME(家族番号簡易版)%の孫にあたる
				ENDIF
			ELSEIF CFLAG:ARG:続柄 < 2000
				PRINTFORML 　%CSVNAME(家族番号簡易版)%の\@ SEX(ARG) == 1 ? 父親 # 母親 \@にあたる
			ELSEIF CFLAG:ARG:続柄 < 3000
				PRINTFORML 　%CSVNAME(家族番号簡易版)%の\@ SEX(ARG) == 1 ? 祖父 # 祖母 \@にあたる
			ENDIF
		CASE IS >= 2
			IF CFLAG:ARG:続柄 < 1000
				IF 尊属 < 1000
					;今のところ3兄弟、3姉妹までなのでそれにだけ対応
					;ちなみにCONV_NOは英数字→漢数字変換関数
					LOCALS = %CONV_NO(家族人数+1)%
					IF CFLAG:ARG:続柄 == 1
						SELECTCASE SEX(ARG)
							CASE 1
								LOCALS += "兄"
								IF 姉数 || 妹数
									LOCALS += "妹"
								ELSE
									LOCALS += "弟"
								ENDIF
							CASE 2
								LOCALS += "姉"
								IF 兄数 || 弟数
									LOCALS += "弟"
								ELSE
									LOCALS += "妹"
								ENDIF
						ENDSELECT
					ELSE
						IF 兄数
							LOCALS += "兄"
							IF 姉数 || 妹数 || SEX(ARG) == 2
								LOCALS += "妹"
							ELSE
								LOCALS += "弟"
							ENDIF
						ELSEIF 姉数
							LOCALS += "姉"
							IF 兄数 || 弟数 || SEX(ARG) == 1
								LOCALS += "弟"
							ELSE
								LOCALS += "妹"
							ENDIF
						ENDIF
					ENDIF
					PRINTFORM 　%LOCALS%の{CFLAG:ARG:続柄}番目であり、
					IF 兄数 || 姉数
						LOCALS = 
						FOR LCOUNT, 0, 10000
							SIF !EXISTCSV(LCOUNT)
								CONTINUE
							IF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < CFLAG:ARG:続柄 && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "血縁コード")) == CFLAG:ARG:血縁コード
								SIF LOCALS != ""
									LOCALS += "、"
								LOCALS += @"%CSVCALLNAME(LCOUNT)%"
							ENDIF
						NEXT
						PRINTFORM %LOCALS%の\@ SEX(ARG) == 1 ? 弟 # 妹 \@
						SIF (兄数 || 姉数) && (弟数 || 妹数)
							PRINT 、
					ENDIF
					IF 弟数 || 妹数
						LOCALS = 
						FOR LCOUNT, 0, 10000
							SIF !EXISTCSV(LCOUNT)
								CONTINUE
							IF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) > CFLAG:ARG:続柄 && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "血縁コード")) == CFLAG:ARG:血縁コード
								SIF LOCALS != ""
									LOCALS += "、"
								LOCALS += @"%CSVCALLNAME(LCOUNT)%"
							ENDIF
						NEXT
						PRINTFORM %LOCALS%の\@ SEX(ARG) == 1 ? 兄 # 姉 \@
					ENDIF
					PRINTL にあたる
				ELSEIF 尊属 < 2000
					PRINT 　
					IF 兄数+姉数+弟数+妹数 == 1
						IF CFLAG:ARG:続柄 == 1
							PRINTFORM %CSVNAME(兄弟姉妹番号簡易版)%の\@ SEX(ARG) == 1 ? 兄 # 姉 \@であり、
						ELSE
							PRINTFORM %CSVNAME(兄弟姉妹番号簡易版)%の\@ SEX(ARG) == 1 ? 弟 # 妹 \@であり、
						ENDIF
					ELSE
						IF 兄数+姉数+弟数+妹数 == 1
							LOCALS = %CONV_NO(兄数+姉数+弟数+妹数+1)%
							IF CFLAG:ARG:続柄 == 1
								SELECTCASE SEX(ARG)
									CASE 1
										LOCALS += "兄"
										IF 姉数 || 妹数
											LOCALS += "妹"
										ELSE
											LOCALS += "弟"
										ENDIF
									CASE 2
										LOCALS += "姉"
										IF 兄数 || 弟数
											LOCALS += "弟"
										ELSE
											LOCALS += "妹"
										ENDIF
								ENDSELECT
							ELSE
								IF 兄数
									LOCALS += "兄"
									IF 姉数 || 妹数 || SEX(ARG) == 2
										LOCALS += "妹"
									ELSE
										LOCALS += "弟"
									ENDIF
								ELSEIF 姉数
									LOCALS += "姉"
									IF 兄数 || 弟数 || SEX(ARG) == 1
										LOCALS += "弟"
									ELSE
										LOCALS += "妹"
									ENDIF
								ENDIF
							ENDIF
							PRINTFORM %LOCALS%の{CFLAG:ARG:続柄}番目であり、
							IF 兄数 || 姉数
								LOCALS = 
								FOR LCOUNT, 0, 10000
									SIF !EXISTCSV(LCOUNT)
										CONTINUE
									IF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < CFLAG:ARG:続柄 && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < 1000 && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "血縁コード")) == CFLAG:ARG:血縁コード
										SIF LOCALS != ""
											LOCALS += "、"
										LOCALS += @"%CSVCALLNAME(LCOUNT)%"
									ENDIF
								NEXT
								PRINTFORM %LOCALS%の\@ SEX(ARG) == 1 ? 弟 # 妹 \@
								SIF (兄数 || 姉数) && (弟数 || 妹数)
									PRINT 、
							ENDIF
							IF 弟数 || 妹数
								LOCALS = 
								FOR LCOUNT, 0, 10000
									SIF !EXISTCSV(LCOUNT)
										CONTINUE
									IF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) > CFLAG:ARG:続柄 && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < 1000 && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "血縁コード")) == CFLAG:ARG:血縁コード
										SIF LOCALS != ""
											LOCALS += "、"
										LOCALS += @"%CSVCALLNAME(LCOUNT)%"
									ENDIF
								NEXT
								PRINTFORM %LOCALS%の\@ SEX(ARG) == 1 ? 兄 # 姉 \@
							ENDIF
							PRINT で、
						ENDIF
					ENDIF
					SIF 父親
						PRINTFORM %CSVCALLNAME(父親)%
					SIF 父親 && 母親
						PRINT と
					SIF 母親
						PRINTFORM %CSVCALLNAME(母親)%
					SIF 父親 && 母親
						PRINT 夫妻
					PRINTFORML の\@ SEX(ARG) == 1 ? 息子 # 娘 \@にあたる
				ELSEIF 尊属 < 3000
					PRINT 　
					IF 兄数+姉数+弟数+妹数 == 1
						IF CFLAG:ARG:続柄 == 1
							PRINTFORM %CSVNAME(兄弟姉妹番号簡易版)%の\@ SEX(ARG) == 1 ? 兄 # 姉 \@であり、
						ELSE
							PRINTFORM %CSVNAME(兄弟姉妹番号簡易版)%の\@ SEX(ARG) == 1 ? 弟 # 妹 \@であり、
						ENDIF
					ELSE
						IF 兄数+姉数+弟数+妹数 == 1
							LOCALS = %CONV_NO(兄数+姉数+弟数+妹数+1)%
							IF CFLAG:ARG:続柄 == 1
								SELECTCASE SEX(ARG)
									CASE 1
										LOCALS += "兄"
										IF 姉数 || 妹数
											LOCALS += "妹"
										ELSE
											LOCALS += "弟"
										ENDIF
									CASE 2
										LOCALS += "姉"
										IF 兄数 || 弟数
											LOCALS += "弟"
										ELSE
											LOCALS += "妹"
										ENDIF
								ENDSELECT
							ELSE
								IF 兄数
									LOCALS += "兄"
									IF 姉数 || 妹数 || SEX(ARG) == 2
										LOCALS += "妹"
									ELSE
										LOCALS += "弟"
									ENDIF
								ELSEIF 姉数
									LOCALS += "姉"
									IF 兄数 || 弟数 || SEX(ARG) == 1
										LOCALS += "弟"
									ELSE
										LOCALS += "妹"
									ENDIF
								ENDIF
							ENDIF
							PRINTFORM %LOCALS%の{CFLAG:ARG:続柄}番目であり、
						ENDIF
						;さすがに親も居れば情報量を減らす
						IF 父親 || 母親
							SIF 父親
								PRINTFORM %CSVCALLNAME(父親)%
							SIF 父親 && 母親
								PRINT と
							SIF 母親
								PRINTFORM %CSVCALLNAME(母親)%
							SIF 父親 && 母親
								PRINT 夫妻
							PRINTFORM の\@ SEX(ARG) == 1 ? 息子 # 娘 \@で、
						ELSE
							IF 兄数 || 姉数
								LOCALS = 
								FOR LCOUNT, 0, 10000
									SIF !EXISTCSV(LCOUNT)
										CONTINUE
									IF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < CFLAG:ARG:続柄 && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < 1000 && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "血縁コード")) == CFLAG:ARG:血縁コード
										SIF LOCALS != ""
											LOCALS += "、"
										LOCALS += @"%CSVCALLNAME(LCOUNT)%"
									ENDIF
								NEXT
								PRINTFORM %LOCALS%の\@ SEX(ARG) == 1 ? 弟 # 妹 \@
								SIF (兄数 || 姉数) && (弟数 || 妹数)
									PRINT 、
							ENDIF
							IF 弟数 || 妹数
								LOCALS = 
								FOR LCOUNT, 0, 10000
									SIF !EXISTCSV(LCOUNT)
										CONTINUE
									IF CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) > CFLAG:ARG:続柄 && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "続柄")) < 1000 && CSVCFLAG(LCOUNT, GETNUM(CFLAG, "血縁コード")) == CFLAG:ARG:血縁コード
										SIF LOCALS != ""
