REPEAT VARSIZE("ABL")
	IF COUNT == VARSIZE("ABL")-1
		HTML_PRINT LOCALS+LOCALS:1+LOCALS:2+LOCALS:3+LOCALS:4+LOCALS:5
		BREAK
	ELSEIF ABLNAME:COUNT == ""
		CONTINUE
	ENDIF

	;改行するのか？ 続けて出力するのか？
	IF LOCAL%6 == 0 && LOCAL != 0
		HTML_PRINT LOCALS+LOCALS:1+LOCALS:2+LOCALS:3+LOCALS:4+LOCALS:5
		VARSET LOCALS
	ENDIF

	SELECTCASE SEX(ARG)
		CASE 1
			;男ならレズっ気、レズ中毒、噴乳中毒は灰色表示
			SELECTCASE ABLNAME:COUNT
				CASE "レズっ気", "レズ中毒", "噴乳中毒"
					SETCOLORBYNAME gray
					表示用レベル = -----
				CASE "Ｖ感覚"
					;ヴァギナ無ければＶ感覚も灰色
					IF VAGINA(ARG)
						SETCOLORBYNAME silver
						表示用レベル = {ABL:ARG:COUNT, 5, LEFT}
					ELSE
						SETCOLORBYNAME gray
						表示用レベル = -----
					ENDIF
				CASEELSE
					SETCOLORBYNAME silver
					表示用レベル = {ABL:ARG:COUNT, 5, LEFT}
			ENDSELECT
		CASE 2
			;女ならホモっ気、ゲイ中毒は灰色表示
			SELECTCASE ABLNAME:COUNT
				CASE "ホモっ気", "ゲイ中毒"
					SETCOLORBYNAME gray
					表示用レベル = -----
				CASEELSE
					SETCOLORBYNAME silver
					表示用レベル = {ABL:ARG:COUNT, 5, LEFT}
			ENDSELECT
	ENDSELECT

	LOCALS:(LOCAL%6) = %SET_TOOLTIP(@"　%ABLNAME:COUNT, 8%:LV%表示用レベル%", ABL_COMMENT(ABLNAME:COUNT, ARG))%
	RESETCOLOR
	LOCAL++
REND

SIF !LINEISEMPTY()
	PRINTL 


;=============================================================================
;素質表示
@SHOW_INFO_TALENT, ARG
#DIMS DYNAMIC 素質名
LOCAL = 0
VARSET LOCALS, 
REPEAT VARSIZE("TALENT")
	SELECTCASE TALENTNAME:COUNT
		CASE "性別"
			CONTINUE
		CASE "恋心", "既婚者"
			SIF CFLAG:ARG:陥落
				CONTINUE
	ENDSELECT
	IF COUNT == VARSIZE("TALENT")-1
		HTML_PRINT LOCALS+LOCALS:1+LOCALS:2+LOCALS:3+LOCALS:4+LOCALS:5
		BREAK
	ELSEIF TALENT:ARG:COUNT == 0
		CONTINUE
	ENDIF

	IF TALENTNAME:COUNT == "バストサイズ"
		SELECTCASE TALENT:ARG:バストサイズ
			CASE 1
				素質名 = [絶壁]
			CASE 2
				素質名 = [貧乳]
			CASE 3
				素質名 = [並乳]
			CASE 4
				素質名 = [巨乳]
			CASE 5
				素質名 = [爆乳]
		ENDSELECT
	ELSE
		素質名 = [%TALENTNAME:COUNT%]
	ENDIF

	SIF LOCAL%6 == 0
		LOCALS = 　

	LOCALS:(LOCAL%6) += @"%SET_TOOLTIP(素質名, TALENT_COMMENT(TALENTNAME:COUNT, ARG))%"

	LOCAL++

	;改行するのか？ 続けて出力するのか？
	IF LOCAL%6 == 0 && LOCAL != 0
		HTML_PRINT LOCALS+LOCALS:1+LOCALS:2+LOCALS:3+LOCALS:4+LOCALS:5
		VARSET LOCALS
	ENDIF
REND

SIF !LINEISEMPTY()
	PRINTL 

