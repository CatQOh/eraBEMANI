		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				LOCALS = 特に何も感じていないようです
			CASE 1 TO 3
				LOCALS = 少し恥ずかしい様子です
			CASE 4 TO 7
				LOCALS = 羞恥に快楽が伴ってきたようです
			CASEELSE
				LOCALS = 恥ずかしいことが気持ち良いようです
		ENDSELECT
	CASE "苦痛"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				LOCALS = 特に痛くはないようです
			CASE 1 TO 3
				LOCALS = 痛みが伴っているようです
			CASE 4 TO 7
				LOCALS = 必死に痛みに耐えています
			CASEELSE
				LOCALS = この上ない痛みを感じているようです
		ENDSELECT
	CASE "恐怖"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				LOCALS = 何も恐れてはいない様子です
			CASE 1 TO 3
				LOCALS = 少しばかり%CALLNAME:PLAYER%を恐れているようです
			CASE 4 TO 7
				LOCALS = 心底恐れおののいているようです
			CASEELSE
				LOCALS = 恐怖のあまり体が震えているようにも見えます
		ENDSELECT
	CASE "反感"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				LOCALS = まだ不満ではないようです
			CASE 1 TO 3
				LOCALS = 少々満足いかないようです
			CASE 4 TO 7
				LOCALS = 表情に苛つきが募り始めています
			CASEELSE
				LOCALS = 態度に怒りが満ちています
		ENDSELECT
	CASE "不快"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				LOCALS = 特に不快ではないようです
			CASE 1 TO 3
				LOCALS = あまり快くはない様子です
			CASE 4 TO 7
				LOCALS = 嫌がる素振りを見せています
			CASEELSE
				LOCALS = すっかり気分を害してしまったようです
		ENDSELECT
	CASE "抑鬱"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				LOCALS = 平常心を保っています
			CASE 1 TO 3
				LOCALS = 若干気分が沈み始めてきたようです
			CASE 4 TO 7
				LOCALS = モチベーションを失いかけています
			CASEELSE
				LOCALS = すっかりやる気を失ってしまったようです
		ENDSELECT
	CASEELSE
		;#;THROW パラメータ名「%ARGS%」のツールチップが設定されていません
ENDSELECT

RETURNF LOCALS

