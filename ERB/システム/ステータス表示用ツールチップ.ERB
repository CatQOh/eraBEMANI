			SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
				CASE 0
					LOCALS = 特に感じていないようです
				CASE 1
					LOCALS = 少し気持ち良くなってきたようです
				CASE 2
					LOCALS = 確実に感じてきているようです
				CASE 3
					LOCALS = もう少しで絶頂に達してしまいそうです
			ENDSELECT
		ENDIF
	CASE "潤滑"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				LOCALS = ほとんど濡れていません
			CASE 1
				LOCALS = 少し湿りを帯びています
			CASE 2
				LOCALS = だんだんと濡れてきたようです
			CASE 3
				IF VAGINA(TARGET)
					LOCALS = 愛液が目に見えて溢れてきました
				ELSE
					LOCALS = それなりの潤滑を得ているようです
				ENDIF
			CASE IS >= 4
				IF VAGINA(TARGET)
					LOCALS = 膣周りはすっかりと濡れそぼっています
				ELSE
					LOCALS = 行為に支障をきたさない程度には濡れています
				ENDIF
		ENDSELECT
	CASE "勃起"
		IF !PENIS(TARGET)
			LOCALS = 女性には無縁のステータスです
		ELSE
			SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
				CASE 0
					LOCALS = まだ勃っていません
				CASE 1
					LOCALS = 少し硬くなってきました
				CASE 2
					LOCALS = 半勃ち状態です
				CASE 3
					LOCALS = 十分に勃起しています
			ENDSELECT
		ENDIF
	CASE "恭順"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				IF CFLAG:陥落
					LOCALS = %CALLNAME:PLAYER%との行為を心待ちにしているようです
				ELSE
					LOCALS = あまり気乗りしていないようです
				ENDIF
			CASE 1 TO 3
				IF CFLAG:陥落
					LOCALS = %CALLNAME:PLAYER%ともっと深く交わりたいようです
				ELSE
					LOCALS = まだ%CALLNAME:PLAYER%に対してよそよそしさが目立ちます
				ENDIF
