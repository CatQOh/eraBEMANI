			CASE 4 TO 7
				LOCALS = 理性が働かなくなってきたようです
			CASEELSE
				LOCALS = すっかり快楽の虜になってしまったようです
		ENDSELECT
	CASE "屈服"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0
				LOCALS = 平常心を保っているようです
			CASE 1 TO 3
				IF CFLAG:陥落
					LOCALS = もっと激しくされたいようです
				ELSE
					LOCALS = まだ反抗心が伺えます
				ENDIF
			CASE 4 TO 7
				IF CFLAG:陥落
					LOCALS = %CALLNAME:PLAYER%に身を委ねています
				ELSE
					LOCALS = 素直に従うようになってきました
				ENDIF
			CASEELSE
				LOCALS = %CALLNAME:PLAYER%のなすがままになっています
		ENDSELECT
	CASE "習得"
		SELECTCASE GETPALAMLV(PALAM:ARGS, 9)
			CASE 0 TO 2
				LOCALS = まだ本調子ではないようです
			CASE 3 TO 5
				LOCALS = コツを掴んできたようです
			CASEELSE
				LOCALS = 互いの感じる部分を覚えてしまったようです
		ENDSELECT
