	RETURN 0

;朝奉仕イベント発生してない場合は発生
IF MAUD:7 && TIME == 0 && !PENIS(MASTER)
	DRAWLINE
	PRINTFORM %CALLNAME:MASTER%に添い寝するふたりのうち、先に目を覚ましたのは
	SELECTCASE RAND:2
		CASE 0
			PRINTFORMW リリーゼだった
			PRINTFORMW 「ふわ……おはよ、朝ごはんは何？」
		CASE 1
			PRINTFORMW モードだった
			PRINTFORMW 「おはようございます、ご主人様。朝食は何にしましょうか？」
	ENDSELECT
ENDIF

MAUD:7 = 0


