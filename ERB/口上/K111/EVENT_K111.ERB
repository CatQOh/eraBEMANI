;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		SELECTCASE FALLTYPE(TARGET)
			;恋慕、親愛
			CASE 1
				;授乳手コキっていいよね
				;ちゃんとロールプレイしてくれるファウラお姉さん
				IF PREVCOMS == "母乳飲み"
					SELECTCASE RAND:4
						CASE 0
							PRINTFORML 「くすっ%UNICODE(0x2665)%……ここをこんなに固くして…………そんなエッチな子には躾が必要ね♪」
							PRINTFORMW 「はいっ♪私のミルクを飲みながら、君のミルクもびゅっびゅしましょうね～%UNICODE(0x2665)%」
						CASE 1
							PRINTL 「私のおっぱいミルク、おいしいですか？」
							PRINTFORMW 「全身の力を抜きながら、いーっぱい出してくださいね～%UNICODE(0x2665)%」
						CASE 2
							PRINTFORML 「よしよし%UNICODE(0x2665)%いっぱいなでなでしてあげるからね♪」
							PRINTFORMW 「たくさん甘えてたくさん射精しましょうね～%UNICODE(0x2665)%」
						CASE 3
							PRINTFORML 「んっ%UNICODE(0x2665)%……そんなに一生懸命吸って…………私のミルクおいしいの？」
							PRINTFORMW 「正直な子にはご褒美をあげないといけないよねっ%UNICODE(0x2665)%」
					ENDSELECT
				ELSEIF BASE:PLAYER:射精 > MAXBASE:PLAYER:射精*3/4
					PRINTL 「あっ……びくびくして今にも出そう…………」
					PRINTDATAW
						DATAFORM 「我慢せずいっぱい出してね%UNICODE(0x2665)%」
						DATA 「このまま絞り出してあげるねっ♪」
						DATA 「このまま手を止めたら…………って冗談よ♪」
					ENDDATA
				ELSE
					SELECTCASE RAND:4
						CASE 0
							PRINTL 「あっ♪先走り汁がいっぱい出てぬるぬるしてきたね♪」
							PRINTFORMW 「我慢しなくていいからね%UNICODE(0x2665)%いっぱいびゅ～って出して♪」
						CASE 1
							PRINTL 「手を動かすたびに反応してる…………」
							PRINTFORMW 「いっぱい気持ちよくなってほしいなっ%UNICODE(0x2665)%」
						CASE 2
							PRINTL 「どう？もしかして物足りない？」
							PRINTFORMW 「強く動かすから痛かったら言ってね%UNICODE(0x2665)%」
						CASE 3
							PRINTL 「ペニスがすごく熱くなってる…………」
							PRINTFORMW 「出したら少しは熱さが収まるかなっ%UNICODE(0x2665)%」
					ENDSELECT
				ENDIF
			;淫乱、娼婦
			CASE 2
				PRINTDATAL
					DATAFORM 「おちんちん触っているだけで興奮してきたっ%UNICODE(0x2665)%」
					DATAFORM 「おちんちんがすごく熱い%UNICODE(0x2665)%」
					DATA 「もう我慢汁がいっぱい出てるよ♪」
				ENDDATA
				PRINTDATAW
					DATAFORM 「君のミルク、いっぱい搾り取ってあげるね%UNICODE(0x2665)%」
					DATAFORM 「君の好きなタイミングで、いつでも出してっ%UNICODE(0x2665)%」
					DATA 「私がもっと固くしてあげるからねっ♪」
				ENDDATA
			;服従、隷属
			CASE 3
				PRINTL 「少し強めに擦るわよ……」
				PRINTW 「痛かったら言ってよね」
		ENDSELECT
	;2種消滅
	ELSEIF (TALENT:抑圧 == 0 && TALENT:抵抗 == 0)
		PRINTL 「ほら、さっさと出して終わりにするわよ」
		PRINTW 「このペニスを見ているだけで腹が立つわ……」
	;1種消滅
	ELSEIF (TALENT:抑圧 == 0 || TALENT:抵抗 == 0)
		PRINTL 「出そうになったら言いなさいよね」
		PRINTW 「顔にかかると不愉快だから止めるからね」
	;消滅なし
	ELSE
		PRINTL 「うぇ……触るたびに反応して気持ち悪い……」
		PRINTDATAW
			DATA 「こんな汚いもの、触りたくないのに…………」
