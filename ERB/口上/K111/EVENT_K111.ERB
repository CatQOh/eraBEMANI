@COM_縦パイズリ_K111
;初めて
IF FCOM:SELECTCOM == 0
	PRINTL 「これを挟んで胸を動かせばいいの？」
	IF CFLAG:陥落
		PRINTFORMW 「いいよ♪…………来て%UNICODE(0x2665)%」
	ELSE
		PRINTW 「胸を犯しているみたいで気持ち悪いわね…………」
	ENDIF
	FCOM:SELECTCOM = 1
;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		SIF PALAM:潤滑 == 0
			PRINTL 「まずは唾液で濡らしてからね…………」
		SELECTCASE FALLTYPE(TARGET)
			;恋慕、親愛
			CASE 1
				IF PALAM:潤滑 >= PALAMLV:4
					PRINTDATA
						DATA 「あぅ……
						DATA 「んぅ…………
						DATA 「あぁっ……
						DATA 「あっ…………
					ENDDATA
					PRINTDATA
						DATA にゅるにゅるして
						DATA ぬるっとして
						DATA どろどろのペニスが……すごいよぉ…………
					ENDDATA
					PRINTDATAL
						DATAFORM ……あっ……乳首に擦れてるのに、ひぁっ%UNICODE(0x2665)%……気持ちいい」
						DATA いやらしい音出してる…………」
						DATA 私までイきそう……」
					ENDDATA
				ELSE
					PRINTDATAL
						DATA 「……どう？おっぱいに包まれて気持ちいい？」
						DATAFORM 「もっと腰を振っていいのよ%UNICODE(0x2665)%」
						DATA 「包んであげたときの顔、とても気持ちよさそう♪」
						DATA 「もうっ……こんなに脈打っているよ…………いやらしい♪」
					ENDDATA
				ENDIF
				PRINTDATAW
					DATAFORM 「おっぱいの中にたくさん出していいからね%UNICODE(0x2665)%」
					DATAFORM 「ほらっ%UNICODE(0x2665)%おっぱいの中にびゅくびゅくーって%UNICODE(0x2665)%」
					DATA 「君が満足するまで搾り取ってあげる♪」
					;同様にせっかく口が使えるのなら
					DATALIST 
						DATA 「せっかく口が空いてるんだし、乳首を舐めてあげるわ」
						DATAFORM 「…………くすっ、やっぱり\@ TALENT:PLAYER:性別 == 1 ?男の人も#\@乳首は敏感なのね♪」
					ENDLIST
					DATALIST
						DATAFORM 「…………ねえ、君の指……舐めさせてほしいな%UNICODE(0x2665)%」
						DATAFORM 「んっ%UNICODE(0x2665)%……ちゅ、ちゅぷ……くちゅっ」
						DATAFORM 「ふゎあぁ…………おいしい%UNICODE(0x2665)%……もっと欲しいよぉ%UNICODE(0x2665)%」
					ENDLIST
				ENDDATA
			;淫乱、娼婦
			CASE 2
				IF ABL:Ｂ感覚 >= 8 || TALENT:淫乳
					PRINTDATAW
						DATAFORM 「あっ……おっぱいが敏感なの%UNICODE(0x2665)%…………イっちゃう%UNICODE(0x2665)%」
						DATAFORM 「んっ……おちんちんが擦れるたびに…………あぁっ、気持ちいい%UNICODE(0x2665)%」
					ENDDATA
				ELSEIF PALAM:潤滑 >= PALAMLV:4
					PRINTDATA
						DATA 「あんっ……
						DATA 「あぁぁっ…………
					ENDDATA
					PRINTDATA
						DATA にゅるにゅるして
						DATA ぬるっとしていて
						DATA おちんちんがどろどろで……
					ENDDATA
					PRINTDATAL
						DATA ……おっぱいが犯されているよぉ♪」
						DATA 私までイきそう……」
						DATA 溶けちゃいそう…………」
					ENDDATA
				ELSE
					PRINTDATAL
						DATA 「……どう？おっぱいに包まれて気持ちいい？」
						DATAFORM 「おちんぽミルク、いっぱい中に出してね%UNICODE(0x2665)%」
					ENDDATA
				ENDIF
				PRINTDATAW
					DATAFORM 「犯してぇ%UNICODE(0x2665)%……おっぱいまんこもっと犯してほしいのぉ%UNICODE(0x2665)%」
					DATALIST
						DATAFORM 「んっ%UNICODE(0x2665)%……ひぅ、あっ%UNICODE(0x2665)%」
						DATAFORM 「…………おちんちんもっと欲しいよぉ%UNICODE(0x2665)%」
					ENDLIST
				ENDDATA
			;服従、隷属
			CASE 3
				PRINTDATAW
					DATA 「私の胸でいっぱい気持ちよくなって欲しいわ…………」
					DATA 「どう？気持ちいい？たくさん出してくださいね♪」
				ENDDATA
		ENDSELECT
	;2種消滅
	ELSEIF (TALENT:抑圧 == 0 && TALENT:抵抗 == 0)
		PRINTL 「こんな面倒なことをさせるなんて……」
		PRINTW 「ありがたく思いなさいよね…………」
	;1種消滅
	ELSEIF (TALENT:抑圧 == 0 || TALENT:抵抗 == 0)
		PRINTL 「こんなやつに胸を好きにさせられるなんて……」
		PRINTW 「ものすごく気分が悪いわ…………」
	;消滅なし
	ELSE
		PRINTL 「くっ……こんなの屈辱的だわ……」
		PRINTDATAW
			DATA 「魔法が使えるのなら消してしまいたいわ……こんなもの……」
			DATA 「こんな事をさせてさぞかし気分がいいでしょうね…………」
			DATA 「胸に出したら怒るわよ」
			DATA 「君は本当にこんなのが気持ちいいの？」
		ENDDATA
	ENDIF
ENDIF

RETURN 0

