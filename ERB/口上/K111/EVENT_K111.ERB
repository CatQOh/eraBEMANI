;-------------------------------------------------
;92,オナホ扱き　基本的には手淫と同じ
;-------------------------------------------------
@COM_オナホ扱き_K111
;初めて
IF FCOM:SELECTCOM == 0
	IF CFLAG:陥落
		PRINTL 「オナホールで君のペニスを扱けばいいのね」
		PRINTW 「いっぱい出してもらえるように頑張るからねっ♪」
	ELSE
		PRINTL 「はぁ……これで扱いてほしいの？」
		PRINTW 「……そんなの自分でやればいいのに…………」
	ENDIF
	FCOM:SELECTCOM = 1
;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		SELECTCASE FALLTYPE(TARGET)
			;恋慕、親愛
			CASE 1
				IF BASE:PLAYER:射精 > MAXBASE:PLAYER:射精*3/4
					PRINTL 「あっ……びくびくして今にも出そう…………」
					PRINTDATAW
						DATAFORM 「我慢せずいっぱい出してね%UNICODE(0x2665)%」
						DATA 「このまま絞り出してあげるねっ♪」
						DATA 「このまま手を止めたら…………って冗談よ♪」
					ENDDATA
				ELSE
					IF TALENT:処女
						LOCAL = RAND:5
					ELSE
						LOCAL = RAND:7
					ENDIF
					SELECTCASE LOCAL
						CASE 0
							PRINTL 「あっ♪先走り汁がいっぱい出てぬるぬるしてきたね♪」
							PRINTFORMW 「我慢しなくていいからね%UNICODE(0x2665)%いっぱいびゅ～って出して♪」
						CASE 1
							PRINTL 「手を動かすたびに反応してる…………」
							PRINTFORMW 「いっぱい気持ちよくなってほしいなっ%UNICODE(0x2665)%」
						CASE 2
							PRINTL 「どう？もしかして物足りない？」
							PRINTFORMW 「強く動かすから痛かったら言ってね%UNICODE(0x2665)%」
						CASE 3
							PRINTL 「ペニスがすごく熱くなってる…………」
							PRINTFORMW 「出したら少しは熱さが収まるかなっ%UNICODE(0x2665)%」
						CASE 4
							PRINTL 「まるで射精管理しているみたい♪」
							PRINTFORMW 「勝手に出したらお仕置きねっ%UNICODE(0x2665)%」
						;以下は処女ではないときに、追加でランダムの対象となる口上
						CASE 5
							PRINTL 「どう……？気持ちいい？」
							PRINTFORMW 「こっちの穴で満足できなかったら、次は…………ねっ%UNICODE(0x2665)%」
						CASE 6
							PRINTL （むぅっ…………なんか気持ちよさそうな顔してる……）
							PRINTW （もしかして私、こんなモノに嫉妬しちゃってるの……？）
					ENDSELECT
				ENDIF
			;淫乱、娼婦
			CASE 2
				PRINTDATAL
					DATAFORM 「くすっ……いやらしい音がいっぱい出てるよ%UNICODE(0x2665)%」
					DATAFORM 「おちんちんがすごく固くなってる%UNICODE(0x2665)%」
					DATA 「もう我慢汁がいっぱい出てるよ♪」
				ENDDATA
				PRINTDATAW
					DATAFORM 「君のミルク、いっぱい搾り取ってあげるね%UNICODE(0x2665)%」
					DATAFORM 「君の好きなタイミングで、いつでも出してっ%UNICODE(0x2665)%」
					DATA 「私がもっと固くしてあげるからねっ♪」
				ENDDATA
			;服従、隷属
			CASE 3
				PRINTL 「少し強めに擦るわよ……」
				PRINTW 「痛かったら言ってよね」
		ENDSELECT
	;2種消滅
	ELSEIF (TALENT:抑圧 == 0 && TALENT:抵抗 == 0)
		PRINTL 「ほら、さっさと出して終わりにするわよ」
		PRINTW 「このペニスを見ているだけで腹が立つわ……」
	;1種消滅
	ELSEIF (TALENT:抑圧 == 0 || TALENT:抵抗 == 0)
		PRINTL 「出そうになったら言いなさいよね」
		PRINTW 「顔にかかると不愉快だから止めるからね」
	;消滅なし
	ELSE
		PRINTL 「うぇ……動かすたびに反応して気持ち悪い……」
		PRINTDATAW
			DATA 「自分で扱けばいいのに…………」
			DATA 「オナホから出ている音のせいで気分が悪いわ……」
			DATA 「君は本当にこんなのが気持ちいいの？」
		ENDDATA
	ENDIF
ENDIF

RETURN 0

