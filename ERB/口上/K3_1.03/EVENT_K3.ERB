;-------------------------------------------------
;0,愛撫
;-------------------------------------------------
@COM_愛撫_K3
;初めて
IF FCOM:SELECTCOM == 0
	PRINTW 「そ、そんなところ触らないでよぉ……」
	PRINTW 「ん、にゃ……くすぐったい……」
	FCOM:SELECTCOM = 1
;二回目以降
ELSE
	;陥落済み
	IF CFLAG:陥落
		IF EXP:主人調教経験 > 1000
			LOCAL = RAND:3
		ELSE
			LOCAL = RAND:2
		ENDIF
		SELECTCASE LOCAL
			CASE 0
				PRINTW 「あ……ご、ごめん……もうちょっと優しく……」
				PRINTW 「ん…………あったかい……」
			CASE 1
				PRINTW 「ねぇ、あたしの鼓動、分かる……？」
				PRINTW 「とくん、とくん、って……あなたと一緒にいると……嬉しくて、幸せで、心拍数が上がっちゃうの……」
			CASE 2
				PRINTW 「あっ……そこ、気持ちいい……」
				PRINTW 「今までたくさん愛しあってきたから……どこが気持ちいいのか、あたしも分かるよ……」
				PRINTW 「……大好き…………一緒に、幸せになろう……？」
		ENDSELECT
	;それ以外
	ELSE
		SELECTCASE RAND:4
			CASE 0
				PRINTW 「変なところ触らないでよ！」
			CASE 1
				PRINTW 「あたしの体なんて触って……何がいいの？」
			CASE 2
				PRINTW 「んっ、くすぐったいよ……」
			CASE 3
				PRINTW 「ひゃっ！ だ、だめ……」
		ENDSELECT
	ENDIF
ENDIF

RETURN 0

