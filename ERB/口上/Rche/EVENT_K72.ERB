@EVENT18_K72
SELECTCASE FALLTYPE(TARGET)
	;恋慕、親愛
	CASE 1
		PRINTFORMW 「いやだ！やだやだやだやだやだやだ！！！！！」
		PRINTFORMW 「%CALLNAME:MASTER%と離ればなれになるなんていやだよぉ！」
