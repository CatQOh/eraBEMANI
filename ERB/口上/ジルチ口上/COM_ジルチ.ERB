			CASE 3
				PRINTFORMW 「はぁ…っ、あ…ぅ…」
				PRINTFORMW 「識…もっと…………」
		ENDSELECT
	ENDIF
;陥落済み
ELSEIF CFLAG:陥落
	SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「あぁ…いいぞ……しっかり咥えて偉いな…」
		CASE 1
			PRINTFORMW 「ふぅ…くっ、お前にフェラされるの最高、だな…っ」
	ENDSELECT
;未陥落で魔法掛かってる時
ELSEIF ハニーバニー
	PRINTFORMW 「っく…上手くなってるな、お前…」
;屈服刻印Lv2
ELSEIF MARK:屈服刻印 >= 2
	SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「くぁ…はっ…無理すんなって…！」
		CASE 1
			PRINTFORMW 「歯ぁ…立てんじゃねぇ、ぞ」
	ENDSELECT
;それ以外
ELSE
	SELECTCASE RAND:2
		CASE 0
			PRINTFORMW 「知らねー奴に咥えられて興奮するとか、あってたまるかよ…！｣
		CASE 1
			SELECTCASE SEX(PLAYER)
				CASE 1
					PRINTFORMW 「げぇっ、男にフェラされても嬉しくねぇよ！」
				CASE 2
					PRINTFORMW （俺的にはアリだが複雑な気分だぜ…）
			ENDSELECT
	ENDSELECT
ENDIF

RETURN 0

