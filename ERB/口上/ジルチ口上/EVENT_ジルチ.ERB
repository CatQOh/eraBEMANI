;-------------------------------------------------
;調教者の射精時のテキスト
;どこに射精した、という変数を削除したため、コマンドで分岐するのが確実で安心です
;分からない場合は既存の口上を参考にするとわかりやすいかもです
;SELECTCOMS = "実行したコマンド名"
;-------------------------------------------------
IF NOWEX:PLAYER:射精
	SELECTCASE SELECTCOMS
		CASE "フェラチオ", "オナフェラ", "シックスナイン"
			IF CFLAG:陥落
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「ん…っ、はぁ、結構出たな……溜め過ぎは良くないぜ？」
					CASE 1
						PRINTFORMW 「っく…ふ…！」
						PRINTFORMW 「わりぃ…ちょっと飲み切れないかと思ったが、なんとかな…」
				ENDSELECT
			ELSEIF MARK:屈服刻印 == 3
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「んんっ…ぐ…」
					CASE 1
						PRINTFORMW 「うぁ…どんだけ出せるんだよ、お前…」
					CASEELSE
						PRINTFORMW 「なんか嫌だぜ…これ」
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「うぶっ！？…がはぁ！」
						PRINTFORMW 「……出るんなら言えよ！」
					CASE 1
						PRINTFORMW 「げぇ…マジで口ん中イカ臭せぇ…」
				ENDSELECT
			ENDIF
		CASE "正常位アナル", "騎乗位アナル", "後背位アナル"
			IF CFLAG:陥落
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「うぁっ…ああ…はぁ…あああっ！」
					CASE 1
						PRINTFORMW 「い、いいんだ…そのまま出してくれっ…はぁあああ！」
					CASE 2
						PRINTFORMW 「んんっ！はぁ、あはぁ…！」
				ENDSELECT
			ELSEIF MARK:屈服刻印 == 3
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「はぁああ…い、や…じゃねぇけど…中が熱い…」
					CASE 1
						PRINTFORMW 「ひぃ…っあぁああ！」
					CASEELSE
						PRINTFORMW 「や、やめてくれ……この…！」
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「だ、だめだ…やめろやめろっ！中に出すな！早く抜けよ…っ！」
						PRINTFORMW ジルチの抵抗も空しく、そのまま直腸内に精を放った…
					CASE 1
						PRINTFORMW 「あぁっ…う、嘘だろ…ぉ…！？」
				ENDSELECT
			ENDIF
		CASE "正面座位アナル", "背面座位アナル"
			IF CFLAG:陥落
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「はぁっ、あぁ…うぅ…」
					CASE 1
						PRINTFORMW 「…なぁ…腰、辛くなかったか…？、俺は…多分平気だ」
				ENDSELECT
			ELSEIF MARK:屈服刻印 == 3
				SELECTCASE RAND:3
					CASE 0
						PRINTFORMW 「お前も、イッてんのかよ…」
					CASE 1
						PRINTFORMW 「ふぅ…んんっ…！」
					CASEELSE
						PRINTFORMW 「出来ればお前と俺の立場が変わってればよ…」
						PRINTFORMW 座位で精液を出されたジルチは恨めしそうに%CALLNAME:PLAYER%の前で吐き捨てた…
				ENDSELECT
			ELSE
				SELECTCASE RAND:2
					CASE 0
						PRINTFORMW 「クソっ、この体勢で出されちまうなんてよぉ…ついてないぜ…」
					CASE 1
						PRINTFORMW 「うぁあああっ！ホントに中出しすんじゃねぇ！くたばれ、ド畜生…ッ！」
				ENDSELECT
			ENDIF
	ENDSELECT
ENDIF

