	ENDIF
ENDIF

RETURN 0

;-------------------------------------------------
;バイブ
;-------------------------------------------------
@COM_バイブ_MAD†HOLIC
;装着時
IF TEQUIP:バイブ == 0
	;初めて
	IF FCOM:SELECTCOM == 0
		IF TALENT:処女
			PRINTFORM %CALLNAME:TARGET%の処女を食い破る無機物の姿を、
			PRINTFORM %CALLNAME:TARGET%は
			IF TALENT:淫乱 || TALENT:娼婦 || TALENT:服従 || TALENT:隷属 || ABL:マゾっ気 >= 5
				PRINTFORM 恍惚とした顔で
			ELSEIF TALENT:崩壊
				PRINTFORM 暗く濁った瞳で
			ELSEIF ABL:マゾっ気 >= 2
				PRINTFORM 複雑そうな顔で
			ELSE
				PRINTFORM 青ざめた顔で
			ENDIF
			PRINTFORMW 見つめている……
		ELSEIF CFLAG:陥落
			PRINTFORMW 膣に突如として入れられたバイブ
			PRINTFORM %CALLNAME:TARGET%はその快楽を
			IF TALENT:淫乱 || TALENT:娼婦 || ABL:Ｖ感覚 >= 5
				PRINTFORM めったに見せないような満面の笑みで
			ELSE
				PRINTFORM 恥ずかしげに赤くなった顔で
			ENDIF
			PRINTFORMW 受け入れた……
		ENDIF
		FCOM:SELECTCOM = 1
	;二回目以降
	ELSE
		;陥落済み
		IF CFLAG:陥落
			PRINTFORM バイブから与えられる機械的な快楽を、
			IF TALENT:淫乱 || TALENT:娼婦 || ABL:Ｖ感覚 >= 5
				PRINTFORM 喜色満面に
			ELSE
				PRINTFORM 恥ずかしげな顔で
			ENDIF
			PRINTFORM 受け入れる姿を眺める%CALLNAME:MASTER%に
			PRINTFORMW %CALLNAME:TARGET%は物欲しげな顔を投げかけている……
		;屈服刻印Lv3
		;ELSEIF MARK:屈服刻印 == 3
			;PRINTFORMW 
		;それ以外
		ELSEIF ABL:Ｖ感覚 >= 5
			PRINTFORMW バイブに責め立てられる快楽に%CALLNAME:TARGET%は頭が真っ白になり、ただ喘ぐことしかできない……
		ELSE
			PRINTFORMW 膣を刺激するバイブを見ないですむように、%CALLNAME:TARGET%は顔を背けている
			SIF ABL:露出癖 >= 3
				PRINTFORMW しかし性器からは愛液があふれ出ており、感じているのは明白だ……
		ENDIF
	ENDIF
;解除時
ELSEIF TEQUIP:バイブ
	;初めて（の解除）
	;解除時にしかフラグを処理しないため、装着時のまま調教終了すると解除経験は無いことになる
	IF FCOM:SELECTCOM == 1
		;PRINTFORMW 
		FCOM:SELECTCOM = 2
	;2回目以降
	ELSE
		;PRINTFORMW 
	ENDIF
ENDIF

RETURN 0

