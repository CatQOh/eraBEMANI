SIF !GROUPMATCH(SELECTCOMS, "正常位させる", "後背位させる", "正常位アナルさせる", "後背位アナルさせる")
	TRYCALL EX_MESSAGE

IF NOWEX:射精
	SELECTCASE SELECTCOMS
