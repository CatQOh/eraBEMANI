;名前表示用関数
;あくまで表示用 内部処理は今まで通り日本語を使う
@NAMEDISP, ARG
#FUNCTIONS

;旧データ等で定義されてない場合の情報更新
SIF CSTR:ARG:英語名 == "" && CSVENGNAME(NO:ARG) != ""
	CSTR:ARG:英語名 = %CSVENGNAME(NO:ARG)%

IF LANGUAGE == "ENG" && CSTR:ARG:英語名 != ""
	RETURNF CSTR:ARG:英語名
ELSE
	RETURNF NAME:ARG
ENDIF

;呼び名表示用関数 同上
@CALLNAMEDISP, ARG
#FUNCTIONS

;旧データ等で定義されてない場合の情報更新
SIF CSTR:ARG:英語呼び名 == "" && CSVENGCALLNAME(NO:ARG) != ""
	CSTR:ARG:英語呼び名 = %CSVENGCALLNAME(NO:ARG)%

IF LANGUAGE == "ENG" && CSTR:ARG:英語呼び名 != ""
	RETURNF CSTR:ARG:英語呼び名
ELSE
	RETURNF NAME:ARG
ENDIF

;CSVからARGの英語名を返す
@CSVENGNAME, ARG
#FUNCTIONS

RETURNF CSVCSTR(ARG, GETNUM(CSTR, "英語名"))

;CSVからARGの英語呼び名を返す
@CSVENGCALLNAME, ARG
#FUNCTIONS

RETURNF CSVCSTR(ARG, GETNUM(CSTR, "英語呼び名"))

