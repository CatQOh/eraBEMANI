;EDIT段位の入力用関数
@段位認定エディット
#DIM DYNAMIC EDIT, 5
#DIM DYNAMIC LRESULT
#DIM DYNAMIC 人数
#DIM DYNAMIC マニュアル表示
DRAWLINE

マニュアル表示 = 0

PRINTL □エディット内容
SIF 人数 >= 1
	PRINTFORML 　一人目 %CSVNAMEF(EDIT:1)% [-1] 外す
SIF 人数 >= 2
	PRINTFORML 　二人目 %CSVNAMEF(EDIT:2)% [-2] 外す
SIF 人数 >= 3
	PRINTFORML 　三人目 %CSVNAMEF(EDIT:3)% [-3] 外す
SIF 人数 >= 4
	PRINTFORML 　四人目 %CSVNAMEF(EDIT:4)% [-4] 外す

CALL CHARA_LIST, 4, 1

SELECTCASE RESULT
	CASE 10000
		VARSET CCB, 0
		VARSET EDIT, 0
		人数 = 0
		RESTART
	CASE -4 TO -1
		CCB:(EDIT:ABS(RESULT)) = 0
		ARRAYREMOVE EDIT, ABS(RESULT), 1
		人数--
	CASEELSE
		SIF !EXISTCSV(RESULT)
			RESTART
		LRESULT = RESULT
		CALL CHARA_MANUAL, LRESULT
		DO
			PRINTL 
			PRINTL このキャラにしますか？
			PRINTL [0] はい
			PRINTL [1] いいえ
			SIF マニュアル表示 == 0
				PRINTL [2] 素質を表示する
			INPUT
			SELECTCASE RESULT
				CASE 0
					EDIT:(人数+1) = LRESULT
					CCB:LRESULT = 1
					GOTO CHARA_DECIDE
				CASE 1
					RESTART
				CASE 2
					マニュアル表示 = 1
					PRINTL 
					PRINTFORML 種族:%CSVCSTR(LRESULT, 110)%
					PRINT 素質
					REPEAT 200
						SELECTCASE TALENTNAME:COUNT
							CASE "性別"
								CONTINUE
							CASE "バストサイズ"
								SELECTCASE CSVTALENT(LRESULT, COUNT)
									CASE 1
										PRINT 【絶壁】
									CASE 2
										PRINT 【貧乳】
									CASE 3
										PRINT 【並乳】
									CASE 4
										PRINT 【巨乳】
									CASE 5
										PRINT 【豊乳】
									CASE 6
										PRINT 【爆乳】
								ENDSELECT
								CONTINUE
						ENDSELECT
						SIF CSVTALENT(LRESULT, COUNT)
							PRINTFORM 【%TALENTNAME:COUNT%】
					REND
					SIF !LINEISEMPTY()
						PRINTL 
			ENDSELECT
		LOOP 1
ENDSELECT

RESTART

$CHARA_DECIDE
人数++
SIF 人数 < 4
	RESTART

VARSET LOCAL, 0

RETURN EDIT:1, EDIT:2, EDIT:3, EDIT:4

