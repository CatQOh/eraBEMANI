				INPUT
				SELECTCASE RESULT
					CASE 0
						PRINTFORMW %ITEMNAME:LOCAL%を導入しました
						ITEM:LOCAL = 1
						MONEY -= ITEMPRICE:LOCAL
						SIF FLAG:口上表示
							TRYCALLFORM BR_ITEM_K{FLAG:パートナー}, LOCAL
				ENDSELECT
			LOOP LOOPRES(0, 1)
			RESTART
