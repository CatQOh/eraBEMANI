HTML_PRINT LOCALS
LOCALS = <button value='3' title='娼館の運営方針を決定します'>　[3] - 運営方針</button>
HTML_PRINT LOCALS
LOCALS = <button value='4' title='娼館で働かせるキャラを配属します'>　[4] - 人員配置</button>
HTML_PRINT LOCALS
