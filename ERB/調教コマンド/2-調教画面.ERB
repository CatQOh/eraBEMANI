;TRAIN内の構成に呼び出される
@SHOW_STATUS
DRAWLINE
CALL 日付表示
IF LANGUAGE == "ENG"
	CALL PASELI, @"Money:{MONEY}＄ "
	PRINTFORML Weather:%WEATHERDISP()% Temparature:%DIV10(FLAG:気温)%℃

	PRINTFORML Target:%NAMEDISP(TARGET)% Trainer:\@ ASSIPLAY ? %NAMEDISP(ASSI)%(Assistant) # %NAMEDISP(MASTER)% \@

	SIF TALENT:PLAYER:孔雀の瞳
		PRINTFORM %CALLNAMEDISP(TARGET)%'s Depend:{CFLAG:依存度} 

	PRINTFORM %CALLNAMEDISP(TARGET)%'s Stress:
	CALL ストレス表示, TARGET
	PRINTL 

	IF 上級売春中
		PRINTFORML □%CALLNAMEDISP(TARGET)%'s Status
		IF PENIS(TARGET) || TALENT:去勢済み
			PRINT 　 Ejacu
			SIF CFLAG:残数 == 0
				PRINT ※NO MORE※
			PRINTFORML %BARBASE("射精", TARGET)%
		ENDIF

		CALL SHOW_INFO_PALAM

		PRINTFORML □%CALLNAMEDISP(MASTER)%'s Status
		CALL SHOW_INFO_HEALTH, MASTER
		IF PENIS(MASTER) || TALENT:MASTER:去勢済み
			PRINT 　 Ejacu(Mast)
			SIF CFLAG:MASTER:残数 == 0
				PRINT ※NO MORE※
			PRINTFORML %BARBASE("射精", MASTER)%
		ENDIF

		IF ASSI >= 0 && (PENIS(ASSI) || TALENT:ASSI:去勢済み)
			PRINT 　 Ejacu(Assi)
			SIF CFLAG:ASSI:残数 == 0
				PRINT ※NO MORE※
			PRINTFORML %BARBASE("射精", ASSI)%
		ENDIF
		SIF TALENT:MASTER:魔術師
			PRINTFORML 　MagicP(Mast)%BARBASE("魔力", MASTER)%
		SIF ASSI > 0 && TALENT:ASSI:魔術師
			PRINTFORML 　MagicP(Assi)%BARBASE("魔力", ASSI)%
	ELSE
		PRINTFORML □%CALLNAMEDISP(TARGET)%'s Status
		CALL SHOW_INFO_HEALTH
		IF PENIS(TARGET) || TALENT:去勢済み
			PRINT 　 Ejacu
			SIF CFLAG:残数 == 0
				PRINT ※NO MORE※
			PRINTFORML %BARBASE("射精", TARGET)%
		ENDIF

		SIF TALENT:母乳体質
			PRINTFORML 　　Milk%BARBASE("母乳", TARGET)%

		;服装
		CALL PLAYER_CLOTHES, TARGET

		CALL SHOW_INFO_PALAM

		IF ゲームモード() == "新婚モード"
			PRINTFORML □%CALLNAMEDISP(MASTER)%'s Status
		ELSE
			PRINTL □Master's Status and Assi's Status
		ENDIF
		IF PENIS(MASTER) || TALENT:MASTER:去勢済み
			PRINT 　 Ejacu(Mast)
			SIF CFLAG:MASTER:残数 == 0
				PRINT ※NO MORE※
			PRINTFORML %BARBASE("射精", MASTER)%
		ENDIF

		IF ASSI >= 0 && (PENIS(ASSI) || TALENT:ASSI:去勢済み)
			PRINT 　 Ejacu(Assi)
			SIF CFLAG:ASSI:残数 == 0
				PRINT ※NO MORE※
			PRINTFORML %BARBASE("射精", ASSI)%
		ENDIF
		SIF TALENT:MASTER:魔術師
			PRINTFORML 　MagicP(Mast)%BARBASE("魔力", MASTER)%
		SIF ASSI > 0 && TALENT:ASSI:魔術師
			PRINTFORML 　MagicP(Assi)%BARBASE("魔力", ASSI)%
	ENDIF

	SIF TEQUIP:触手
		PRINTFORML 　Ejacu(Tent)%BARBASE("触手射精", MASTER)%

	;服装の表示
	CALL PLAYER_CLOTHES, MASTER
	SIF ASSI >= 0
		CALL PLAYER_CLOTHES, ASSI

	IF TALENT:MASTER:ドラゴン使い
		PRINTFORML □%CSTR:MASTER:ドラゴン%'s Status
		PRINTFORML 　 Ejacu　　　%BARBASE("ドラゴン射精", MASTER)%
	ENDIF
	IF ASSI > 0 && TALENT:ASSI:ドラゴン使い
		PRINTFORML □%CSTR:ASSI:ドラゴン%のステータス
		PRINTFORML 　 Ejacu　　　%BARBASE("ドラゴン射精", ASSI)%
	ENDIF
ELSE
	CALL PASELI, @"所持金:{MONEY}＄ "
	PRINTFORML 天気:%天気()% 気温:%DIV10(FLAG:気温)%℃ %月齢()%(月齢:%DIV100(月齢)%)

	IF 上級売春中
		PRINTFORML 客:%NAME:TARGET% 娼婦:\@ ASSIPLAY ? %NAME:ASSI%(助手) # %NAME:MASTER% \@
	ELSEIF ゲームモード() == "新婚モード"
		PRINTFORML 受け:%NAME:TARGET% 攻め:\@ ASSIPLAY ? %NAME:ASSI%(助手) # %NAME:MASTER% \@
	ELSE
		PRINTFORML 調教中:%NAME:TARGET% 調教者:\@ ASSIPLAY ? %NAME:ASSI%(助手) # %NAME:MASTER% \@
	ENDIF
	SIF TALENT:PLAYER:孔雀の瞳
		PRINTFORM %CALLNAME:TARGET%の依存度:{CFLAG:依存度} 

	PRINTFORM %CALLNAME:TARGET%のストレス値:
	CALL ストレス表示, TARGET
	PRINTL 

	IF 上級売春中
		PRINTFORML □%CALLNAME:TARGET%のステータス
		IF PENIS(TARGET) || TALENT:去勢済み
			PRINT 　射精
			SIF CFLAG:残数 == 0
				PRINT ※打ち止め※
			PRINTFORML %BARBASE("射精", TARGET)%
		ENDIF

		CALL SHOW_INFO_PALAM

		PRINTFORML □%CALLNAME:MASTER%のステータス
		CALL SHOW_INFO_HEALTH, MASTER
		IF PENIS(MASTER) || TALENT:MASTER:去勢済み
			PRINT 　射精(主人)
			SIF CFLAG:MASTER:残数 == 0
				PRINT ※打ち止め※
			PRINTFORML %BARBASE("射精", MASTER)%
		ENDIF

		IF ASSI >= 0 && (PENIS(ASSI) || TALENT:ASSI:去勢済み)
			PRINT 　射精(助手)
			SIF CFLAG:ASSI:残数 == 0
				PRINT ※打ち止め※
			PRINTFORML %BARBASE("射精", ASSI)%
		ENDIF
		SIF TALENT:MASTER:魔術師
			PRINTFORML 　魔力(主人)%BARBASE("魔力", MASTER)%
		SIF ASSI > 0 && TALENT:ASSI:魔術師
			PRINTFORML 　魔力(助手)%BARBASE("魔力", ASSI)%
	ELSE
		PRINTFORML □%CALLNAME:TARGET%のステータス
		CALL SHOW_INFO_HEALTH
		IF PENIS(TARGET) || TALENT:去勢済み
			PRINT 　射精
			SIF CFLAG:残数 == 0
				PRINT ※打ち止め※
			PRINTFORML %BARBASE("射精", TARGET)%
		ENDIF

		SIF TALENT:母乳体質
			PRINTFORML 　母乳%BARBASE("母乳", TARGET)%

		;服装
		CALL PLAYER_CLOTHES, TARGET

		CALL SHOW_INFO_PALAM

		IF ゲームモード() == "新婚モード"
			PRINTFORML □%CALLNAME:MASTER%のステータス
		ELSE
			PRINTL □主人と助手のステータス
		ENDIF
		IF PENIS(MASTER) || TALENT:MASTER:去勢済み
			PRINT 　射精(主人)
			SIF CFLAG:MASTER:残数 == 0
				PRINT ※打ち止め※
			PRINTFORML %BARBASE("射精", MASTER)%
		ENDIF

		IF ASSI >= 0 && (PENIS(ASSI) || TALENT:ASSI:去勢済み)
			PRINT 　射精(助手)
			SIF CFLAG:ASSI:残数 == 0
				PRINT ※打ち止め※
			PRINTFORML %BARBASE("射精", ASSI)%
		ENDIF
		SIF TALENT:MASTER:魔術師
			PRINTFORML 　魔力(主人)%BARBASE("魔力", MASTER)%
		SIF ASSI > 0 && TALENT:ASSI:魔術師
			PRINTFORML 　魔力(助手)%BARBASE("魔力", ASSI)%
	ENDIF

	SIF TEQUIP:触手
		PRINTFORML 　射精(触手)%BARBASE("触手射精", MASTER)%

	;服装の表示
	CALL PLAYER_CLOTHES, MASTER
	SIF ASSI >= 0
		CALL PLAYER_CLOTHES, ASSI

	IF TALENT:MASTER:ドラゴン使い
		PRINTFORML □%CSTR:MASTER:ドラゴン%のステータス
		PRINTFORML 　射精　　　%BARBASE("ドラゴン射精", MASTER)%
	ENDIF
	IF ASSI > 0 && TALENT:ASSI:ドラゴン使い
		PRINTFORML □%CSTR:ASSI:ドラゴン%のステータス
		PRINTFORML 　射精　　　%BARBASE("ドラゴン射精", ASSI)%
	ENDIF
ENDIF

