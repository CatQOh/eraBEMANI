@STAIN_INFO
;汚れ表示
CALL SHOW_STAIN, MASTER
CALL SHOW_STAIN, TARGET
SIF ASSI > 0
	CALL SHOW_STAIN, ASSI
WAIT

@SHOW_STAIN, ARG
#DIM DYNAMIC LCOUNT
PRINTFORML 【%CALLNAME:ARG%の汚れ】
FOR LCOUNT, 0, 8
	SELECTCASE STAINNAME:LCOUNT
		CASE "ペニス"
			SIF !PENIS(ARG)
				CONTINUE
		CASE "ヴァギナ", "膣内"
			SIF !VAGINA(ARG)
				CONTINUE
		CASE "下着"
			SIF CSTR:ARG:下半身下着 == ""
				CONTINUE
	ENDSELECT

	;漢字2文字で統一
	SELECTCASE STAINNAME:LCOUNT
		CASE "口"
			PRINT 　口唇：
		CASE "手"
			PRINT 　手指：
		CASE "ペニス"
			PRINT 　陰茎：
		CASE "ヴァギナ"
			PRINT 　膣口：
		CASE "アナル"
			PRINT 　肛門：
		CASE "胸"
			PRINT 　乳房：
		CASEELSE
			PRINTFORM 　%STAINNAME:LCOUNT%：
	ENDSELECT
	SIF GET_STAIN(STAIN:ARG:LCOUNT, "愛液")
		PRINT <愛液>
	SIF GET_STAIN(STAIN:ARG:LCOUNT, "先走り")
		PRINT <先走り>
	SIF GET_STAIN(STAIN:ARG:LCOUNT, "精液")
		PRINT <精液>
	SIF GET_STAIN(STAIN:ARG:LCOUNT, "アナル")
		PRINT <アナル>
	SIF GET_STAIN(STAIN:ARG:LCOUNT, "母乳")
		PRINT <母乳>
	SIF GET_STAIN(STAIN:ARG:LCOUNT, "粘液")
		PRINT <粘液>
	SIF GET_STAIN(STAIN:ARG:LCOUNT, "破瓜血")
		PRINT <破瓜血>
	SIF GET_STAIN(STAIN:ARG:LCOUNT, "尿")
		PRINT <尿>
	PRINTL 
NEXT

