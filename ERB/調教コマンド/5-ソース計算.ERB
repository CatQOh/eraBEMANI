	CASE "ふたなり化", "バストサイズ変更"
	CASEELSE
		PREVCOMS = %SELECTCOMS%
ENDSELECT

;コマンドの実行者を保存
TFLAG:前コマンド実行者 = PLAYER

IF 上級売春中
	;気力０
