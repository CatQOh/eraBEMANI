SIF ASSI > 0 && CFLAG:ASSI:賢者タイム > 0
	CFLAG:ASSI:賢者タイム--

;-------------------------------------------------
;PALAM等での汚れ付与
;-------------------------------------------------
IF VAGINA(TARGET)
	;潤滑LV3以上で膣内が愛液に濡れる
	SIF PALAM:潤滑 >= PALAMLV:3
		SETBIT STAIN:膣内, 0
	;潤滑LV4以上で陰唇が愛液に濡れる
	SIF PALAM:潤滑 >= PALAMLV:4
		SETBIT STAIN:ヴァギナ, 0
	;下着に陰唇の汚れを付与する
	SIF TEQUIP:下半身下着
		STAIN:下着 |= STAIN:ヴァギナ
ENDIF
IF PENIS(TARGET)
	;勃起LV3以上でペニスが先走りに濡れる
	SIF PALAM:勃起 >= PALAMLV:3
		SETBIT STAIN:ペニス, 1
	;下着にペニスの汚れを付与する
	SIF TEQUIP:下半身下着
		STAIN:下着 |= STAIN:ペニス
ENDIF

;下着にアナルの汚れを付与する
SIF TEQUIP:下半身下着
	STAIN:下着 |= STAIN:アナル

;主人と助手には常に先走りと愛液汚れ
SIF PENIS(MASTER)
	SETBIT STAIN:MASTER:ペニス, 1
SIF VAGINA(MASTER)
	SETBIT STAIN:MASTER:膣内, 0
SIF PENIS(ASSI)
	SETBIT STAIN:ASSI:ペニス, 1
SIF VAGINA(ASSI)
	SETBIT STAIN:ASSI:膣内, 0

;主人の下着に汚れ付与
IF TEQUIP:MASTER:下半身下着
	SIF PENIS(MASTER)
		STAIN:MASTER:下着 |= STAIN:MASTER:ペニス
	SIF VAGINA(MASTER)
		STAIN:MASTER:下着 |= STAIN:MASTER:ヴァギナ
ENDIF
;助手の下着に汚れ付与
IF ASSI > 0 && TEQUIP:ASSI:下半身下着
	SIF PENIS(ASSI)
		STAIN:ASSI:下着 |= STAIN:ASSI:ペニス
	SIF VAGINA(ASSI)
		STAIN:ASSI:下着 |= STAIN:ASSI:ヴァギナ
ENDIF

