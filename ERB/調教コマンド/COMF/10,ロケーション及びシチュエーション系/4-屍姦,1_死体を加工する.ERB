@USERCOM_ABLE_死体を加工する, ARG
#FUNCTION
;STS導入してないとそもそも表示されない
SIF !STSMOD
	RETURNF 0

コマンド実行条件 += "対象が死んでる場合に実行可能/"
SIF !CFLAG:死亡 || TEQUIP:仮死薬
	COMRESULT = 0

RETURNF COMRESULT

@COM_死体を加工する
#DIM DYNAMIC 増加量

PRINTFORML %CALLNAME:TARGET%の死体を剥製に加工します。よろしいですか？(調教は終了します)
PRINTL [0] - はい
PRINTL [1] - いいえ

CALL INPUTF, 0, 1
SIF RESULT
	RETURN 0

CALL KOJO_MESSAGE_COM

CALL 剥製加工, TARGET

SELECTCOMS = 剥製加工

FORCE_BEGIN AFTERTRAIN
