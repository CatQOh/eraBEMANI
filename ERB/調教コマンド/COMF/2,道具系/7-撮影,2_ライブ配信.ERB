;-------------------------------------------------
;ライブ配信
;-------------------------------------------------
@USERCOM_ABLE_ライブ配信, ARG
#FUNCTION
#DIM DYNAMIC 条件確定

;ゲーセンに居る場合はできない
コマンド実行条件 += "ゲーセンでは実行不可/"
SIF TEQUIP:ゲームセンター
	COMRESULT = 0
;上級売春中は不可
コマンド実行条件 += "上級売春中は実行不可/"
SIF 上級売春中
	COMRESULT = 0
;普通に撮影中も無理
コマンド実行条件 += "ビデオカメラによる撮影中は実行できない/"
SIF TEQUIP:ビデオカメラ
	COMRESULT = 0
;マジックミラー撮影中はダメ
コマンド実行条件 += "マジックミラーによる撮影中は実行できない/"
SIF TEQUIP:羞恥プレイ && SAVESTR:大鏡 == "マジックミラー"
	COMRESULT = 0
;解除はいつでも可能
SIF TEQUIP:ライブ配信 && COMRESULT
	条件確定 = 1

;ビデオカメラ持っていないとダメ
コマンド実行条件 += "アイテム[ビデオカメラ]が必要/"
SIF !ITEM:ビデオカメラ
	COMRESULT = 0
;主人か助手の撮影技能3以上必要
コマンド実行条件 += "主人もしくは助手の撮影技能Lv3以上が必要/"
IF ABL:MASTER:撮影技能 < 3
	IF ASSI > 0
		SIF ABL:ASSI:撮影技能 < 3
			COMRESULT = 0
	ELSE
		COMRESULT = 0
	ENDIF
ENDIF

IF 条件確定
	COMRESULT = 1
	RETURNF 1
ENDIF
RETURNF COMRESULT

@COM_ライブ配信
;撮影者の処理
IF !TEQUIP:ライブ配信
	IF ABL:MASTER:撮影技能 >= 3 && (ASSI > 0 && ABL:ASSI:撮影技能 >= 3)
		PRINTL 撮影者を選択してください　途中変更はできません
		PRINTL （撮影者を変更する場合、一度撮影を終了してください）
		PRINTFORML [0] %CALLNAME:MASTER%
		PRINTFORML [1] %CALLNAME:ASSI%
		PRINTL [2] やめる
		DO
			SIF ISSKIP()
				BREAK
			INPUT
			SELECTCASE RESULT
				CASE 0
					TFLAG:ビデオ撮影者 = 1
					PRINTFORML 撮影者は%CALLNAME:MASTER%です
					BREAK
				CASE 1
					TFLAG:ビデオ撮影者 = 2
					PRINTFORML 撮影者は%CALLNAME:ASSI%です
					BREAK
				CASE 2
					RETURN 0
			ENDSELECT
		LOOP 1
	ELSEIF ABL:MASTER:撮影技能 >= 3
		TFLAG:ビデオ撮影者 = 1
		PRINTFORML 撮影者は%CALLNAME:MASTER%です
	ELSE
		TFLAG:ビデオ撮影者 = 2
		PRINTFORML 撮影者は%CALLNAME:ASSI%です
	ENDIF
ENDIF

CALL KOJO_MESSAGE_COM

;着脱の処理
IF TEQUIP:ライブ配信
	PRINTW ★★★ライブ配信を終了します★★★
	TFLAG:ビデオ撮影回数 = 0
	TEQUIP:ライブ配信 = 0
ELSE
	PRINTW ★★★ライブ配信を開始します★★★
	;視聴者は1人はいる
	視聴者数 = 1
	;チャンネル登録者数に応じて増加
	視聴者数 += RAND(チャンネル登録者数/2+1)
	LIKE = 0
	DISLIKE = 0
	コメント数 = 0
	VARSET 視聴コメント
	TEQUIP:ライブ配信 = 2
ENDIF

;-------------------------------------------------
;絶頂チェック
;-------------------------------------------------
CALL SOURCE_CHECK_CVAB

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK_T, 0

CALL SAMEN_SHOOT_T, 0, NOWEX:射精

RETURN 1

;-------------------------------------------------
;装着継続の処理
;-------------------------------------------------
@EQUIP_COM_ライブ配信
#DIM DYNAMIC 増加量

;ビデオ撮影とは違って開始時もカウントする
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 0
LOSEBASE:気力 += 50

SOURCE:恐怖 += 1000
SOURCE:露出 += 5000
SOURCE:逸脱 += 2000
SOURCE:反感 += 1000

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:鬱屈 += 700

;苦痛を伴わないマゾっ気及びマゾの処理は先にしておく
;マゾっ気を見る
増加量 = 1000
SELECTCASE ABL:マゾっ気
	CASE 0
		TIMES 増加量 , 0.80
		SOURCE:液体 += 0
		SOURCE:欲情 += 0
	CASE 1
		TIMES 増加量 , 1.00
		SOURCE:液体 += 50
		SOURCE:欲情 += 100
	CASE 2
		TIMES 増加量 , 1.20
		SOURCE:液体 += 100
		SOURCE:欲情 += 200
	CASE 3
		TIMES 増加量 , 1.40
		SOURCE:液体 += 200
		SOURCE:欲情 += 400
	CASE 4
		TIMES 増加量 , 1.50
		SOURCE:液体 += 400
		SOURCE:欲情 += 600
	CASE 5
		TIMES 増加量 , 1.70
		SOURCE:液体 += 600
		SOURCE:欲情 += 800
	CASE 6
		TIMES 増加量 , 1.90
		SOURCE:液体 += 800
		SOURCE:欲情 += 1000
	CASE 7
		TIMES 増加量 , 2.10
		SOURCE:液体 += 1000
		SOURCE:欲情 += 1200
	CASE 8
		TIMES 増加量 , 2.30
		SOURCE:液体 += 1200
		SOURCE:欲情 += 1500
	CASE 9
		TIMES 増加量 , 2.50
		SOURCE:液体 += 1500
		SOURCE:欲情 += 2000
	CASE IS >= 10
		TIMES 増加量 , 3.00
		SOURCE:液体 += 2000
		SOURCE:欲情 += 3000
ENDSELECT

;ナルシスト
IF TALENT:ナルシスト
	SOURCE:欲情 += 200
	SOURCE:露出 += 1000
	SOURCE:屈従 += 100
ENDIF

SOURCE:露出 += 増加量

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
SELECTCASE TFLAG:ビデオ撮影者
	CASE 1
		CALL EXPUP, "撮影経験", 1, MASTER
	CASE 2
		CALL EXPUP, "撮影経験", 1, ASSI
ENDSELECT
CALL EXPUP, "被写経験", 1

@ライブ配信_FUNC
#DIM DYNAMIC ポイント
#DIM DYNAMIC 視聴者数補正

TFLAG:ビデオ撮影回数++

;視聴者数の補正
視聴者数補正 = 100
SIF TALENT:人気
	視聴者数補正 += 100
SIF TALENT:小柄体型
	視聴者数補正 += 50
SIF TALENT:バストサイズ >= 5
	視聴者数補正 += 30
SIF TALENT:ふたなり
	視聴者数補正 += 30
SIF FALLTYPE(TARGET) == 2
	視聴者数補正 += 30
SIF TALENT:無関心
	視聴者数補正 -= 30
SIF TALENT:動物耳 || TALENT:尻尾 || TALENT:羽 || TALENT:足使用不可
	視聴者数補正 += 30
SIF SEX(TARGET) == 1
	視聴者数補正 -= 30
;撮影技能*10
SELECTCASE TFLAG:ビデオ撮影者
	CASE 1
		視聴者数補正 += ABL:MASTER:撮影技能*10
	CASE 2
		視聴者数補正 += ABL:ASSI:撮影技能*10
ENDSELECT

;補正はターン数でも決まる最初は10%、1ターンで+10% 最高100%
視聴者数補正 *= 10*MIN(TFLAG:ビデオ撮影回数, 10)

;コマンドごとのポイント
ポイント = ビデオポイント(SELECTCOMS)
;イってるとポイント2倍
SIF NOWEX:Ｃ絶頂 || NOWEX:Ｖ絶頂 || NOWEX:Ａ絶頂 || NOWEX:Ｂ絶頂 || NOWEX:射精 || NOWEX:潮噴き || NOWEX:噴乳
	ポイント *= 2

CALL ライブコメント, SELECTCOMS

;ポイント高いコマンドを選ぶといいねが増える
;撮影が進んでるのにソフトなことばかりしてるとDISLIKEも増える
LIKE += ビデオポイント(SELECTCOMS)/10
SIF ビデオポイント(SELECTCOMS) < TFLAG:ビデオ撮影回数*10
	DISLIKE += ビデオポイント(SELECTCOMS)/10

;ポイント*補正(100.00%)/10が視聴者数に加算される
IF ポイント*視聴者数補正/10000/10
	視聴者数 += ポイント*視聴者数補正/10000/10
	PRINTFORML 視聴者数増加+{ポイント*視聴者数補正/10000/10}人(現在{視聴者数}人)
ENDIF

;視聴者数>チャンネル登録者数なら1割の超過分がチャンネル登録される
IF (視聴者数-チャンネル登録者数)/10 > 0
	チャンネル登録者数 += (視聴者数-チャンネル登録者数)/10
	PRINTFORML チャンネル登録者数増加+{(視聴者数-チャンネル登録者数)/10}(現在{チャンネル登録者数}人)
ENDIF

;実績:うさぬこの自慰を撮影する
SIF CHARANAMEF(TARGET) == "＊うさぬこ＊" && SELECTCOMS == "自慰"
	CALL 実績解除,  "「F◯2アダルトに投稿するんだぬ？」"

RETURN 1

