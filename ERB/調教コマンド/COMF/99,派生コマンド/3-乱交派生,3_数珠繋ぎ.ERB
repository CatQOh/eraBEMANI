			SIF ISSKIP()
				BREAK
			INPUT
			SELECTCASE RESULT
				CASE 0
					RETURN 0
				CASE 1
					SIF !VAGINA(ASSI)
						CONTINUE
					TFLAG:数珠繋ぎ = 123
					TFLAG:主人挿入箇所 = 1
					BREAK
				CASE 2
					TFLAG:数珠繋ぎ = 123
					TFLAG:主人挿入箇所 = 2
					BREAK
				CASE 3
					SIF !VAGINA(MASTER)
						CONTINUE
					TFLAG:数珠繋ぎ = 231
					TFLAG:奴隷挿入箇所 = 1
					BREAK
				CASE 4
					TFLAG:数珠繋ぎ = 231
					TFLAG:奴隷挿入箇所 = 2
					BREAK
			ENDSELECT
		LOOP 1
	ENDIF
ELSEIF TFLAG:前コマンド実行者 == PLAYER
	SELECTCASE PREVCOMS
		CASE "助手を犯させる"
			SELECTCASE TFLAG:奴隷挿入箇所
				CASE 1
					PRINTFORM %CALLNAME:TARGET%が%CALLNAME:ASSI%の膣に挿入しています。
					TFLAG:奴隷挿入箇所 = 1
				CASE 2
					PRINTFORM %CALLNAME:TARGET%が%CALLNAME:ASSI%のアナルに挿入しています。
					TFLAG:奴隷挿入箇所 = 2
			ENDSELECT
