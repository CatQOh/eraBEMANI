;-------------------------------------------------
;騎乗乳房自慰
;セックス・愛撫系派生コマンド
;-------------------------------------------------
@USERCOM_ABLE505
;騎乗乳房自慰
;胸が露出してないとダメ
SIF TEQUIP:上半身上着 || (TEQUIP:全身上着 && CFLAG:着衣併用 != 1) || TEQUIP:上半身下着
	RETURN 0

;下半身下着をつけているとダメ
SIF TEQUIP:下半身下着
	RETURN 0
;下の空いてない上着を着ているとダメ
IF TEQUIP:下半身上着 || (TEQUIP:全身上着 && CFLAG:着衣併用 != 1)
	SIF !CFLAG:挿入可
		RETURN 0
ENDIF

RETURN 1

@COM505
PRINTL 騎乗乳房自慰

;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

VARSET LOCAL, 0
LOCAL = RESULT


;ABL:欲望
IF ABL:欲望
	LOCAL += ABL:欲望*3
	PRINTFORM +欲望LV{ABL:欲望}({ABL:欲望*3})
ENDIF
SELECTCASE PREVCOMS
	CASE "騎乗位"
		;ABL:Ｖ感覚
		IF ABL:Ｖ感覚
			LOCAL += ABL:Ｖ感覚*2
			PRINTFORM +Ｖ感覚LV{ABL:Ｖ感覚}({ABL:Ｖ感覚*2})
		ENDIF
	CASE "騎乗位アナル"
		;ABL:Ａ感覚
		IF ABL:Ａ感覚
			LOCAL += ABL:Ａ感覚*2
			PRINTFORM +Ａ感覚LV{ABL:Ａ感覚}({ABL:Ａ感覚*2})
		ENDIF
ENDSELECT
;ABL:奉仕精神
IF ABL:奉仕精神
	LOCAL += ABL:奉仕精神*4
	PRINTFORM +奉仕精神LV{ABL:奉仕精神}({ABL:奉仕精神*4})
ENDIF

;ABL:性交中毒
IF ABL:性交中毒
	LOCAL += ABL:性交中毒
	PRINTFORM +性交中毒LV{ABL:性交中毒}({ABL:性交中毒})
ENDIF
;快楽刻印
IF MARK:快楽刻印
	LOCAL += MARK:快楽刻印*3
	PRINTFORM +快楽刻印LV{MARK:快楽刻印}({MARK:快楽刻印*3})
ENDIF
;ABL:露出癖
IF ABL:露出癖
	PRINTFORM +露出癖LV{ABL:露出癖}({ABL:露出癖*4})
	LOCAL += ABL:露出癖*4
ENDIF
;ABL:自慰中毒
IF ABL:自慰中毒
	PRINTFORM +自慰中毒LV{ABL:自慰中毒}({ABL:自慰中毒*3})
	LOCAL += ABL:自慰中毒*3
ENDIF
;快楽刻印
IF MARK:快楽刻印
	PRINTFORM +快楽刻印LV{MARK:快楽刻印}({MARK:快楽刻印*3})
	LOCAL += MARK:快楽刻印*3
ENDIF

;PALAM:欲情
IF PALAM:欲情 < PALAMLV:1
	LOCAL:1 = 0
ELSEIF PALAM:欲情 < PALAMLV:2
	LOCAL:1 = 1
ELSEIF PALAM:欲情 < PALAMLV:3
	LOCAL:1 = 2
ELSEIF PALAM:欲情 < PALAMLV:4
	LOCAL:1 = 3
ELSEIF PALAM:欲情 < PALAMLV:5
	LOCAL:1 = 4
ELSE
	LOCAL:1 = 5
ENDIF
LOCAL:2 = LOCAL:1
SIF TALENT:衝動的
	TIMES LOCAL:2 , 1.50
IF LOCAL:2
	LOCAL += LOCAL:2*3
	PRINTFORM +欲情LV{LOCAL:1}({LOCAL:2*3})
ENDIF
;自制心
IF TALENT:自制心
	LOCAL -= 5
	PRINTFORM -自制心(5)
ENDIF
;恥じらい
IF TALENT:恥じらい
	LOCAL -= 5
	PRINT -恥じらい(5)
ENDIF
;恥薄い
IF TALENT:恥薄い
	LOCAL += 3
	PRINT +恥薄い(2)
ENDIF
;自慰しやすい
IF TALENT:自慰しやすい
	LOCAL += 5
	PRINT +自慰しやすい(5)
ENDIF
;快感に素直
IF TALENT:快感に素直
	LOCAL += 5
	PRINT +快感に素直(5)
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感の否定 && FALLTYPE(TARGET) != 2
	LOCAL -= 8
	PRINT -快感の否定(8)
ENDIF
;男嫌い（恋慕、親愛で無効）
IF TALENT:男嫌い && SEX(PLAYER) == 1 && (FALLTYPE(TARGET) != 1 || PLAYER != MASTER)
	LOCAL -= 12
	PRINT -男嫌い(12)
ENDIF
;恋慕
IF TALENT:恋慕 && PLAYER == MASTER
	LOCAL += 15
	PRINT +恋慕(5)
ENDIF
;親愛
IF TALENT:親愛 && PLAYER == MASTER
	LOCAL += 20
	PRINT +親愛(10)
ENDIF
;媚薬
IF TEQUIP:媚薬
	LOCAL += 8
	PRINT +媚薬(8)
ENDIF

;合計を表示(50以上で実行)
PRINTFORM  = {LOCAL}

LOCAL:2 = 50

;難易度上昇
;公開で+10、搾乳器で+5
SIF TEQUIP:ビデオカメラ
	LOCAL:2 += 10
SIF TEQUIP:搾乳器
	LOCAL:2 += 5

SIF LOCAL < LOCAL:2
	PRINT  < 
SIF LOCAL == LOCAL:2
	PRINT  = 
SIF LOCAL > LOCAL:2
	PRINT  > 
PRINTFORM 実行値{LOCAL:2}
WAIT
;実行できない
IF LOCAL < LOCAL:2
	CALL KOJO_MESSAGE_EVENT, 100
	RETURN 0
ENDIF

