;-------------------------------------------------
;吸精
;調教対象が[吸血鬼]持ちに限る
;-------------------------------------------------
@COM520
SIF TALENT:吸血鬼 == 0
	RETURN 0
PRINTL 吸精

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 0
LOSEBASE:気力 += 0

SOURCE:性行動 = 800
SOURCE:達成 = 1000
SOURCE:液体 = 350
SOURCE:屈従 = 150
SOURCE:逸脱 = 300
SOURCE:反感 = 700


IF TALENT:親愛 || TALENT:娼婦 || TALENT:隷属
	PRINTFORML %CALLNAME:TARGET%が%CALLNAME:MASTER%から吸精を試みた結果なんと%CALLNAME:TARGET%の体力・気力が大きく回復した！
	PRINTFORML 体力+{MAXBASE:体力 / 4} 気力+{MAXBASE:気力 / 5}
	BASE:体力 += MAXBASE:体力 / 4
	BASE:気力 += MAXBASE:気力 / 5
ELSEIF TALENT:恋慕 || TALENT:淫乱 || TALENT:服従
	PRINTFORML %CALLNAME:TARGET%が%CALLNAME:MASTER%から吸精を試みた結果なんと%CALLNAME:TARGET%の体力・気力が回復した！
	PRINTFORML 体力+{MAXBASE:体力 / 20} 気力+{MAXBASE:気力 / 20}
	BASE:体力 += MAXBASE:体力 / 20
	BASE:気力 += MAXBASE:気力 / 20
ELSE
	PRINTFORML %CALLNAME:TARGET%が%CALLNAME:MASTER%から吸精を試みた結果%CALLNAME:TARGET%の体力・気力がわずかに回復した
	PRINTFORML 体力+{MAXBASE:体力 / 100} 気力+{MAXBASE:気力 / 100}
	BASE:体力 += MAXBASE:体力 / 100
	BASE:気力 += MAXBASE:気力 / 100
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
PRINTL 精液経験+1
EXP:精液経験 += 1

RETURN 1

