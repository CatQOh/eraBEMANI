	SELECTCASE FLAG:天気
		CASE 2
			TIMES LOCAL , 0.50
		CASE 3 TO 7
			TIMES LOCAL , 0.20
	ENDSELECT
	;一部PALAMが1.{LOCAL}倍になる 気温100℃でなんと2倍！(人類滅亡)
	REPEAT VARSIZE("PALAM")
		SELECTCASE PALAMNAME:COUNT
			CASE "潤滑", "恭順", "欲情", "屈服", "習得"
				UP:COUNT += (UP:COUNT*LOCAL)/1000
