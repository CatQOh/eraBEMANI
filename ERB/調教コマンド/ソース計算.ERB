;相性
LOCAL = RELATION:(NO:PLAYER)
SIF LOCAL != 100
	PRINTFORML ＜相性{LOCAL/100}.{(LOCAL%100/10)}倍＞

;同じコマンドの連続実行
SIF SELECTCOM == PREVCOM
	PRINTL ＜同一コマンド連続実行＞

;今回のコマンドを「前回のコマンド」とする
PREVCOM = SELECTCOM

;今回のコマンド名を「前回のコマンド名」に保存
PREVCOMS = %SELECTCOMS%

;コマンドの実行者を保存
TFLAG:前コマンド実行者 = PLAYER

;気力０
SIF BASE:気力 <= 0
	PRINTL ★気力０★

;体力・気力の減少

IF LOSEBASE:体力 >= 0
	PRINTFORM 体力-{LOSEBASE:体力} 
ELSE
	PRINTFORM 体力+{ABS(LOSEBASE:体力)} 
ENDIF

IF LOSEBASE:気力 >= 0
	PRINTFORML 気力-{LOSEBASE:気力}
ELSE
	PRINTFORML 気力+{ABS(LOSEBASE:気力)}
ENDIF

;-------------------------------------------------
;射精後の再装填処理
;-------------------------------------------------
;対象
SIF CFLAG:賢者タイム > 0
	CFLAG:賢者タイム--
;主人
SIF CFLAG:MASTER:賢者タイム > 0
	CFLAG:MASTER:賢者タイム--
;助手
SIF ASSI > 0 && CFLAG:ASSI:賢者タイム > 0
	CFLAG:ASSI:賢者タイム--

;-------------------------------------------------
;PALAM等での汚れ付与
;-------------------------------------------------
IF VAGINA(TARGET)
	;潤滑LV3以上で膣内が愛液に濡れる
	SIF PALAM:潤滑 >= PALAMLV:3
		SETBIT STAIN:膣内, 0
	;潤滑LV4以上で陰唇が愛液に濡れる
	SIF PALAM:潤滑 >= PALAMLV:4
		SETBIT STAIN:ヴァギナ, 0
	;下着に陰唇の汚れを付与する
	SIF TEQUIP:下半身下着
		STAIN:下着 |= STAIN:ヴァギナ
ENDIF
IF PENIS(TARGET)
	;欲情LV4以上でペニスが先走りに濡れる
	SIF PALAM:欲情 >= PALAMLV:4
		SETBIT STAIN:ペニス, 1
	;下着にペニスの汚れを付与する
	SIF TEQUIP:下半身下着
		STAIN:下着 |= STAIN:ペニス
ENDIF

;下着にアナルの汚れを付与する
SIF TEQUIP:下半身下着
	STAIN:下着 |= STAIN:アナル

;主人と助手には常に先走りと愛液汚れ
SIF PENIS(MASTER)
	SETBIT STAIN:MASTER:ペニス, 1
SIF VAGINA(MASTER)
	SETBIT STAIN:MASTER:膣内, 0
SIF PENIS(ASSI)
	SETBIT STAIN:ASSI:ペニス, 1
SIF VAGINA(ASSI)
	SETBIT STAIN:ASSI:膣内, 0

;主人の下着に汚れ付与
IF TEQUIP:MASTER:下半身下着
	SIF PENIS(MASTER)
		STAIN:MASTER:下着 |= STAIN:MASTER:ペニス
	SIF VAGINA(MASTER)
		STAIN:MASTER:下着 |= STAIN:MASTER:ヴァギナ
ENDIF
;助手の下着に汚れ付与
IF ASSI > 0 && TEQUIP:ASSI:下半身下着
	SIF PENIS(ASSI)
		STAIN:ASSI:下着 |= STAIN:ASSI:ペニス
	SIF VAGINA(ASSI)
		STAIN:ASSI:下着 |= STAIN:ASSI:ヴァギナ
ENDIF

