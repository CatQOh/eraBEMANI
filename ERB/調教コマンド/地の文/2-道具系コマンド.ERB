;-------------------------------------------------
;バイブ
;-------------------------------------------------
@MESSAGE_COM_バイブ
;服着てる
SIF OVER_GENITAL(TARGET) != "" && OVER_GENITAL(TARGET) != "貞操帯"
	PRINTFORM %OVER_GENITAL(TARGET)%を捲り、
;解除時
IF TEQUIP:バイブ
	PRINTFORML %CALLNAME:TARGET%から蠢くバイブをゆっくりと引き抜いた
	IF 上級売春中
		SELECTCASE 娼館方針
			CASE 1
				PRINTFORML %CALLNAME:TARGET%には少々乱暴な行為だったようだ。もう少しソフトなプレイを所望している……
			CASEELSE
				PRINTFORML %CALLNAME:TARGET%はまだまだ物足りず、早急な次の行為を望んでいる……
		ENDSELECT
	ELSE
		IF TALENT:無機物
			PRINTFORML 無機質な%CALLNAME:TARGET%の膣内はバイブの形に拡がっている……
		ELSE
			SELECTCASE EXP:Ｖ経験
				;200以上
				CASE IS >= 200
					PRINTFORML %CALLNAME:TARGET%は、膣内の喪失感に耐えかねて切なそうな顔をしている……
				;150以上
				CASE IS >= 150
					PRINTFORML %CALLNAME:TARGET%の膣内に溜まっていた粘液が掻き出されて滴り、水溜りを作っている……
				;120以上
				CASE IS >= 120
					PRINTFORML バイブを抜かれた%CALLNAME:TARGET%の肉襞からぼたぼたと愛液が滴っている……
				;70以上
				CASE IS >= 70
					PRINTFORML %CALLNAME:TARGET%はもじもじと股を擦り合わせて%CALLNAME:PLAYER%の方を見ている……
				;40以上
				CASE IS >= 40
					PRINTFORML %CALLNAME:TARGET%は熱のこもった息を吐いている……
				;20以上
				CASE IS >= 20
					PRINTFORML %CALLNAME:TARGET%はゆるんだ膣口をひくつかせている……
				;20未満
				CASE IS < 20
					PRINTFORML %CALLNAME:TARGET%はホッとしたような表情をしている……
			ENDSELECT
		ENDIF
	ENDIF
;装着時
ELSE
	PRINTFORM %CALLNAME:TARGET%の
	;ケモ
	IF TALENT:ファーリー
		;潤滑LV5以上
		SIF PALAM:潤滑 >= PALAMLV:5
			PRINT 溢れ出る
		;潤滑LV3以上
		SIF PALAM:潤滑 >= PALAMLV:3
			PRINT 愛液に濡れた
		PRINT 体毛を掻き分け、牝穴
	;無機物
	ELSEIF TALENT:無機物
		PRINT 硬く無機質な膣
	ELSE
		;潤滑LV5以上
		SIF PALAM:潤滑 >= PALAMLV:5
			PRINT たっぷりと
		;潤滑LV3以上
		SIF PALAM:潤滑 >= PALAMLV:3
			PRINT 粘液をたたえた
		;小柄体型
		IF TALENT:小柄体型
			SELECTCASE RAND:2
				CASE 0
					PRINT 幼い割れ目
				CASE 1
					PRINT 未成熟な秘裂
			ENDSELECT
		ELSE
			PRINT 秘裂
		ENDIF
	ENDIF
	SELECTCASE SAVESTR:バイブ
		CASE "強力なバイブ"
			PRINTL に改造した強力なバイブの先端をあてがい馴染ませながら、奥へと沈めていく……
		CASE "細いバイブ"
			PRINTL に細く改良したバイブを突き入れる……
		CASE "太いバイブ"
			PRINTL が怪我をしないように、時間をかけて太いバイブを飲み込ませていく……
		CASEELSE
			SELECTCASE RAND:2
				CASE 0
					PRINTL にバイブの先端をあてがい馴染ませながら、奥へと沈めていく……
				CASE 1
					PRINTL を震える張り型でゆっくりと貫いていく……
			ENDSELECT
	ENDSELECT

	IF TALENT:アニマル
		PRINTFORML %CALLNAME:TARGET%はバイブの挿入を生殖行為と錯覚してか、舌を出して息を荒げて大人しくしている……
	ELSEIF 上級売春中
		IF TALENT:処女
			PRINTFORML %CALLNAME:TARGET%は処女を散らす覚悟で%娼館店名%に訪れたようだ。その望みは叶った……
		ELSE
			SELECTCASE 娼館方針
				CASE 1
					PRINTFORML バイブを手に取る%CALLNAME:PLAYER%を、%CALLNAME:TARGET%は少し驚いた表情で見ている。膣を使うのは予定外らしい……
				CASEELSE
					IF TALENT:マゾ
						PRINTFORML %CALLNAME:TARGET%は求めていた快感を得られて満足気だ……
					ELSEIF TALENT:サド
						PRINTFORML %CALLNAME:TARGET%は何故自分が責められなくてはいけないのかと悪態をついている……
					ELSE
						PRINTFORML %CALLNAME:TARGET%は自分が責められるだけでは満足いかないようだ……
					ENDIF
			ENDSELECT
		ENDIF
	ELSE
		PRINTFORM %CALLNAME:TARGET%は
		;処女
		IF TALENT:処女
			;娼婦 処女ビッチ
			IF TALENT:娼婦
				PRINTL 女性器に入り込んでくるバイブを見つめては、自分が処女である事を忘れているかのような笑みを浮かべている……
			;恋慕or親愛
			ELSEIF TALENT:恋慕 || TALENT:親愛
				PRINTFORML 初めてを散らすのがその淫具であることを悟ると、少し寂しそうな目で%CALLNAME:PLAYER%を見つめた……
			;淫乱
			ELSEIF TALENT:淫乱
				PRINTL 自分が処女である事もお構いなしに、股を開いては挿入をせがんでいる……
			;服従or隷属
			ELSEIF TALENT:服従 || TALENT:隷属
				PRINTFORML 処女を捨てる覚悟はとうに出来ていたかのように、力を抜いて%CALLNAME:PLAYER%に体を委ねた……
			;一線越えない 貞操観念
			ELSEIF TALENT:一線越えない || TALENT:貞操観念
				PRINTL 何かを諦めた表情で目にうっすら涙を浮かべ、振動するバイブを受け入れた……
			;その他
			ELSE
				PRINTL それで処女を散らすことを察すると、喪失の恐怖に怯えながら震えている……
			ENDIF
		ELSE
			IF SAVESTR:バイブ == "太いバイブ"
				SELECTCASE GETEXPLV(EXP:Ｖ拡張経験, 10)
					CASE IS <= 1
						PRINTL 自らの膣には大きすぎるバイブに痛みを訴えている……
					CASE IS <= 3
						PRINTL 膣の拡張にも慣れてきたようだが、未だ痛みを感じるのかうっすらと涙を浮かべている……
					CASE IS <= 5
						PRINTL 膣内を拡げられることにも慣れてきたのか、申し分ない快感を得ているようだ……
					CASEELSE
						PRINTL すっかり拡張しきった膣にめり込む太いバイブの快楽に溺れている……
				ENDSELECT
			ELSE
				;服従or隷属 Ｖ経験70以上
				IF FALLTYPE(TARGET) == 3 && EXP:Ｖ経験 >= 70
					;Ｖ経験
					SELECTCASE EXP:Ｖ経験
						;200以上
						CASE IS >= 200
							PRINTL 挿入への期待だけでガクガクと腰を震わせてイきそうになっている……
						;150以上
						CASE IS >= 150
							PRINTFORML とろけきった目で%CALLNAME:PLAYER%を見つめ、自分から陰唇を擦りつけて受け入れた……
						;120以上
						CASE IS >= 120
							PRINTL 恍惚とした表情で待ちきれないように腰をくねらせている……
						;70以上
						CASE IS >= 70
							PRINTFORML もの欲しそうに目を潤ませ%CALLNAME:PLAYER%を見つめながら、膣内を掻き回される快感に悦んでいる……
					ENDSELECT
				;服従隷属無し もしくはＶ経験70未満
				ELSE
					;Ｖ経験
					SELECTCASE EXP:Ｖ経験
						;40以上
						CASE IS >= 40
							PRINTL 膣内を抉るバイブの感触に陶酔しきった吐息をついて、揺さぶられる膣穴から愛液を止め処なく垂れ流している……
						;20以上
						CASE IS >= 20
							PRINTL 敏感な膣壁をバイブでくすぐられる感触に身をよじらせ愛液を溢れさせている……
						;20未満
						CASE IS < 20
							PRINTL バイブの擦れる感触に陰唇をひくつかせている……
					ENDSELECT
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
