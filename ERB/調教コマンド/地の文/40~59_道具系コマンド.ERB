				PRINTFORML %CALLNAME:TARGET%は少しがっかりした様子だ……
		ELSE
			PRINTFORML %CALLNAME:MASTER%はコンドームを外した
			SIF FALLTYPE(TARGET) == 2 || (上級売春中 && TALENT:男好き)
				PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%の生の射精を求めてうずうずしている……
		ENDIF
	CASE ASSI
		;解除時
		IF TEQUIP:ASSI:コンドーム
			PRINTFORML %CALLNAME:ASSI%は自分のペニスにコンドームを被せた
		ELSE
			PRINTFORML %CALLNAME:ASSI%はコンドームを外した
		ENDIF
	CASE TARGET
		;服着てる
		SIF OVER_GENITAL(TARGET) != ""
			PRINTFORM %OVER_GENITAL(TARGET)%を捲り、
		;解除時
		IF TEQUIP:コンドーム
			PRINTFORML %CALLNAME:TARGET%にコンドームを取り付けた
			IF 上級売春中
				IF TALENT:マゾ
					PRINTFORML %CALLNAME:TARGET%は射精管理への期待にペニスをびくびくと震わせている……
				ELSE
					PRINTFORML %CALLNAME:TARGET%は生でやりたい様子だが、店の方針としてなだめた……
				ENDIF
			ENDIF
		ELSE
			PRINTFORML %CALLNAME:TARGET%からコンドームを取り外した
		ENDIF
ENDSELECT
WAIT

