;-------------------------------------------------
;スパンキング
;-------------------------------------------------
@MESSAGE_COM_スパンキング
IF OVER_GENITAL(TARGET) != ""
	PRINTFORML 服の上から%CALLNAME:TARGET%の尻を平手で強く叩いた
	IF TALENT:無機物
		PRINTFORML しかし%CALLNAME:TARGET%はあまり痛がってる様子ではなく、無機質な尻を叩く%CALLNAME:PLAYER%の手が痛くなってきた……
	ELSE
		PRINTFORM 何度も何度も叩かれている内に、%CALLNAME:TARGET%の
		;ペニスあり+マゾっ気LV3以上
		IF PENIS(TARGET) && ABL:マゾっ気 >= 3
			PRINTL 服の中の物が膨れ上がっていった……
		;ヴァギナあり+マゾっ気LV3以上
		ELSEIF VAGINA(TARGET) && ABL:マゾっ気 >= 3
			PRINTL 服が愛液で濡れはじめていた……
		ELSE
			PRINTL 顔はスパンキングの羞恥に赤くなっていた……
		ENDIF
	ENDIF
ELSE
	PRINTFORML %CALLNAME:TARGET%の尻を平手で強く叩いた
	IF TALENT:無機物
		PRINTFORML しかし%CALLNAME:TARGET%はあまり痛がってる様子ではなく、無機質な尻を叩く%CALLNAME:PLAYER%の手が痛くなってきた……
	ELSEIF TALENT:アニマル
		PRINTFORML 乱暴な調教に%CALLNAME:TARGET%は低い唸り声を上げている……
	ELSE
		IF 上級売春中
			PRINTFORML %CALLNAME:TARGET%は悦びを含んだ悲鳴を上げ、ねだるように尻を突き上げている……
		ELSE
			PRINTFORM 何度も何度も叩かれている内に、%CALLNAME:TARGET%の
			;尻尾あり+マゾっ気LV3以上
			IF TALENT:尻尾 && ABL:マゾっ気 >= 3
				PRINTL 尻尾がゆらゆらと揺れていた……
			;ペニスあり+マゾっ気LV3以上
			ELSEIF PENIS(TARGET) && ABL:マゾっ気 >= 3
				PRINTL 股間は硬くなっていった……
			;ヴァギナあり+マゾっ気LV3以上
			ELSEIF VAGINA(TARGET) && ABL:マゾっ気 >= 3
				PRINTL 股間は自身の愛液で濡れはじめていた……
			ELSE
				IF TALENT:尻尾
					PRINTL 尻尾が驚きを表してぴんと立っていった……
				ELSE
					PRINTL 尻は真っ赤に腫れていった……
				ENDIF
			ENDIF
		ENDIF
	ENDIF
ENDIF
WAIT

;-------------------------------------------------
;乳スパンキング
;-------------------------------------------------
@MESSAGE_COM_乳スパンキング
SIF OVER_BUST(TARGET) != ""
	PRINT 服の上から
PRINTFORM %CALLNAME:TARGET%の
SELECTCASE TALENT:バストサイズ
	;貧乳か絶壁か男
	CASE 0, 1, 2
		SELECTCASE TALENT:バストサイズ
			;絶壁
			CASE 1
				PRINT 膨らみがほぼ皆無な平らな
			;貧乳
			CASE 2
				PRINT 辛うじて膨らみの確認出来る
		ENDSELECT
		PRINTL 胸の頂点を強く爪弾いた……
	;普通～爆乳
	CASE 3 TO 6
		SELECTCASE TALENT:バストサイズ
			;普通
			CASE 3
				PRINT 乳房を
			;巨乳、豊乳
			CASE 4, 5
				PRINT 豊満な乳房を揺らし、
			;爆乳
			CASE 6
				PRINT 豊か過ぎる乳房を跳ね回らせ
		ENDSELECT
		PRINTL 弾くように平手で叩いた……
ENDSELECT
SIF 上級売春中
	PRINTFORML %CALLNAME:TARGET%は快感からか羞恥からか、艶めかしい息を漏らしている……
WAIT

;-------------------------------------------------
;144,放尿
;-------------------------------------------------
@MESSAGE_COM_放尿
;小柄体型+PLAYERがNOT小柄体型+ペニス無し
;IF TALENT:小柄体型 && !TALENT:PLAYER:小柄体型 && !PENIS(TARGET) && !TEQUIP:ゲームセンター && !TEQUIP:羞恥プレイ
;	PRINTFORML 幼い子供のように%CALLNAME:TARGET%の両足を抱え上げ、放尿するように命じた
;ELSE
	PRINTFORML %CALLNAME:TARGET%に放尿するように命じた
;ENDIF
SIF TALENT:アニマル
	GOTO ANIMAL

IF (OVER_GENITAL(TARGET) != "" && !CFLAG:挿入可) || TEQUIP:下半身下着
	PRINTFORM 服を着たまま放尿するように命じられた%CALLNAME:TARGET%は
	SIF TEQUIP:トイレ
		PRINT 便座に座ると
	SELECTCASE ABL:露出癖
		CASE 0 TO 2
			PRINTFORML 不自然にいきみながら%OVER_GENITAL(TARGET)%に染みを広げた……
		CASE 3 TO 5
			PRINTFORML 何の抵抗も無しに%OVER_GENITAL(TARGET)%を尿で汚した……
		CASE IS >= 6
			PRINTFORML 躊躇なく漏らし始め、%OVER_GENITAL(TARGET)%からぽたぽたと垂れる生温かい尿に表情を蕩けさせている……
	ENDSELECT
;野外プレイ中
ELSEIF TEQUIP:野外プレイ
	SELECTCASE ABL:露出癖
		CASE 0 TO 2
			SELECTCASE FALLTYPE(TARGET)
				;未陥落
				CASE 0
					PRINTFORML %CALLNAME:TARGET%は羞恥と屈辱感に涙を浮かべながら地面に向けて放尿した……
				;恋慕or親愛
				CASE 1
					PRINTFORML %CALLNAME:TARGET%は周囲に誰も居ないことを確かめると、%CALLNAME:PLAYER%の目の前で放尿して見せた……
				;淫乱or娼婦
				CASE 2
					PRINTFORML %CALLNAME:TARGET%は頬を染めて息を荒げ、身震いしながら地面に尿を零した……
				;服従or隷属
				CASE 3
					PRINTFORML %CALLNAME:TARGET%はちらちらと辺りを確認しながら、%CALLNAME:PLAYER%の目の前で尿を漏らしている……
			ENDSELECT
		CASE 3 TO 5
			SELECTCASE FALLTYPE(TARGET)
				;未陥落
				CASE 0
					PRINTFORML %CALLNAME:TARGET%は%CALLNAME:PLAYER%を目を合わせないようにして放尿するが、深く吐いた息と震える体には明らかに快感が混じっている……
				;恋慕or親愛
				CASE 1
					PRINTFORML 言われるままに放尿する%CALLNAME:TARGET%は、周囲を気にすることなくただ%CALLNAME:MASTER%だけをじっと見つめている……
				;淫乱or娼婦
				CASE 2
					PRINTFORML 口元を緩ませながら漏らす%CALLNAME:TARGET%は、地面に溜まっていく自分の尿を楽しそうに見ている……
				;服従or隷属
				CASE 3
					IF PENIS(TARGET)
						PRINTFORML %CALLNAME:TARGET%は立ったまま\@ PALAM:欲情 >= PALAMLV:3 ? 勃起した #  \@ペニスから尿を飛ばし、%CALLNAME:PLAYER%の視線を受けて膝を震わせている……
					ELSE
						PRINTFORML %CALLNAME:TARGET%は自らの陰唇を指で拡げ、尿道を見せつけながら地面に尿を漏らしている……
					ENDIF
			ENDSELECT
		CASE IS >= 6
			SELECTCASE FALLTYPE(TARGET)
				CASE 0
					PRINTFORM %CALLNAME:TARGET%は屋外での放尿がすっかり癖になってしまい、
					IF PENIS(TARGET)
						PRINTFORML 膨らんだ肉棒から尿を噴き出しながらさらにそれを硬く勃起させている……
					ELSE
						PRINTFORML 自分で尿道を弄りながら黄色い液体を垂れ流している……
					ENDIF
				CASE 1
					PRINTFORML %CALLNAME:TARGET%はもはや人の目など気にも留めない様子で、大声で%CALLNAME:MASTER%に視姦を求めながら尿を漏らしている……
				CASE 2
					PRINTFORML %CALLNAME:TARGET%は露出しながらの放尿で軽い絶頂に達してしまっているらしく、びくびくと体を震わせながら口元から涎を零している……
				CASE 3
					PRINTFORML %CALLNAME:TARGET%は%CALLNAME:PLAYER%に見せつけるように尿を漏らしては、放尿を見られて興奮していることを告げてきた……
			ENDSELECT
	ENDSELECT
;羞恥プレイ中
ELSEIF TEQUIP:羞恥プレイ
	IF TALENT:ナルシスト
		PRINTFORML %CALLNAME:TARGET%は鏡に映る自分をじっと見つめながら放尿している……
	ELSE
		PRINTFORML %CALLNAME:TARGET%は鏡に映る自分を見ないようにしながらしゃがみこんで放尿し始めた……
	ENDIF
ELSE
	PRINTFORM %CALLNAME:TARGET%は
	SIF TEQUIP:トイレ
		PRINT 便座に座ると、
	SELECTCASE ABL:露出癖
		CASE 0 TO 2
			PRINTFORML 羞恥に頬を染めながら、\@ TEQUIP:トイレ ? 音を立てないように少しづつ # その場にしゃがみこんで \@放尿し始めた……
		CASE 3 TO 5
			$ANIMAL
			PRINTFORML 躊躇うことなく\@ TEQUIP:トイレ ? 音を立てて # 床に \@尿を漏らしている……
		CASE IS >= 6
			PRINTFORML は放尿の快感に息を荒げながら尿を漏らし続けている……
	ENDSELECT
ENDIF

IF TEQUIP:利尿剤
	SELECTCASE ABL:露出癖
		CASE 0 TO 2
			PRINTFORML 薬が効いているせいか、なかなか切れることのない排尿に焦りを見せているようだ
		CASE 3 TO 5
			PRINTFORML 薬が作用しての長い排尿に%CALLNAME:TARGET%は夢中になっている
		CASE IS >= 6
			PRINTFORML だらしなく尿を漏らし続ける%CALLNAME:TARGET%は、利尿剤を気に入ってしまったようだ
	ENDSELECT
ENDIF
WAIT

