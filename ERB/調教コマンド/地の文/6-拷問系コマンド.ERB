;-------------------------------------------------
;腹パン
;-------------------------------------------------
@MESSAGE_COM_腹パン
PRINTFORML %CALLNAME:TARGET%の腹部を思い切り殴った
SIF 上級売春中
	PRINTFORML %CALLNAME:TARGET%は座り込んで苦痛に悶えるも、その表情は妖しい笑みを含んでいる……

;-------------------------------------------------
;踏む
;-------------------------------------------------
@MESSAGE_COM_踏む
PRINTFORM %CALLNAME:TARGET%の
SELECTCASE TFLAG:踏む部位
	CASE 0
		PRINT 腹
	CASE 1
		PRINT 胸
	CASE 2
		IF TEQUIP:Ｖ異物
			SELECTCASE TEQUIP:Ｖ異物
				CASE 1
					PRINT 缶が挿入された
				CASE 2
					PRINT ペットボトルが挿入された
				CASE 3
					PRINT 一升瓶が挿入された
			ENDSELECT
		ENDIF
		PRINT 股間
	CASE 3
		PRINT 首
	CASE 4
		PRINT 顔
ENDSELECT
PRINTW を思い切り踏みつけた……

;-------------------------------------------------
;裸締め
;-------------------------------------------------
@MESSAGE_COM_裸締め
PRINTFORMW %CALLNAME:MASTER%は%CALLNAME:TARGET%の首を後ろから思い切り絞め上げた……

;-------------------------------------------------
;首を絞める
;-------------------------------------------------
@MESSAGE_COM_首を絞める
PRINTFORMW %CALLNAME:TARGET%の首を強く絞めた……

;-------------------------------------------------
;ビンタ
;-------------------------------------------------
@MESSAGE_COM_ビンタ
PRINTFORMW %CALLNAME:TARGET%の頬を手の平で弾いた……

;-------------------------------------------------
;フィストファック
;-------------------------------------------------
@MESSAGE_COM_フィストファック
IF 上級売春中
	PRINTFORML %CALLNAME:TARGET%の願うままに、%CALLNAME:PLAYER%は拳で膣を犯した
ELSE
	PRINTFORML %CALLNAME:TARGET%の膣は%CALLNAME:PLAYER%の拳でかき回された
ENDIF
;Ｖ感覚
SELECTCASE ABL:Ｖ感覚
	CASE IS <= 2
		PRINTFORMW %CALLNAME:TARGET%の膣はぽっかり口を開いている
	CASE 3
		PRINTFORMW %CALLNAME:TARGET%の膣は拡がったままなかなか戻らない
	CASE 4
		PRINTFORMW %CALLNAME:TARGET%の性器からは肉ビラがはみ出し、ぽっかり開いた膣口の奥には子宮口が見えている
	CASE IS >= 5
		IF TALENT:アニマル
			PRINTW 動物の生殖器は人間のそれよりも柔軟性があるのか、拳ひとつを簡単に飲み込んでしまった
		ELSE
			PRINTFORMW %CALLNAME:TARGET%は、自分の膣口を指で拡げ、恍惚としている
		ENDIF
ENDSELECT

;-------------------------------------------------
;アナルフィスト
;-------------------------------------------------
@MESSAGE_COM_アナルフィスト
IF 上級売春中
	PRINTFORML %CALLNAME:TARGET%の願うままに、%CALLNAME:PLAYER%は拳でアナルを犯した
ELSE
	PRINTFORML %CALLNAME:TARGET%の肛門は%CALLNAME:PLAYER%の拳でかき回された
ENDIF
;Ａ感覚
SELECTCASE ABL:Ａ感覚
	CASE IS <= 2
		PRINTFORMW %CALLNAME:TARGET%の肛門はぽっかり口を開いている
	CASE 3
		PRINTFORMW %CALLNAME:TARGET%の肛門は拡がったままなかなか戻らない
	CASE 4
		PRINTFORMW %CALLNAME:TARGET%の肛門からは肉ビラがはみ出し、腸の中が丸見えになっている
	CASE IS >= 5
		IF TALENT:アニマル
			PRINTW 動物の体は人間のそれよりも柔軟性があるのか、拳ひとつを簡単に飲み込んでしまった
		ELSE
			PRINTFORMW %CALLNAME:TARGET%は、自分の肛門を指で拡げ、恍惚としている
		ENDIF
ENDSELECT

;-------------------------------------------------
;両穴フィスト
;-------------------------------------------------
@MESSAGE_COM_両穴フィスト
IF 上級売春中
	PRINTFORML より激しい刺激を求める%CALLNAME:TARGET%の両穴を拳でかき回した
ELSE
	PRINTFORML %CALLNAME:TARGET%の膣と肛門は%CALLNAME:PLAYER%の拳でかき回された
ENDIF
;Ｖ感覚かＡ感覚がLV4以下
IF ABL:Ｖ感覚 <= 4 || ABL:Ａ感覚 <= 4
	PRINTFORMW %CALLNAME:TARGET%は\@ !TALENT:足使用不可 ? 両足 #\@を広げぐったりしており、拡がりきった膣口と肛門から粘液が垂れ流されていた……
;Ｖ感覚かＡ感覚がLV5以上
ELSEIF ABL:Ｖ感覚 >= 5 && ABL:Ａ感覚 >= 5
	IF TALENT:アニマル
		PRINTFORMW %CALLNAME:TARGET%は両穴の快楽に足をがくがくと震わせ、四つん這いで立つのもやっとな様子だ……
	ELSE
		PRINTFORMW %CALLNAME:TARGET%は、すっかり拡がった膣口と肛門の周りを両手の指で撫で回している
	ENDIF
ENDIF

;-------------------------------------------------
;異物挿入
;-------------------------------------------------
@MESSAGE_COM_異物挿入
PRINTFORM %CALLNAME:TARGET%の
SELECTCASE TFLAG:異物挿入
	CASE GETNUM(ABL, "Ｖ感覚")
		PRINT 膣に
		SELECTCASE TEQUIP:Ｖ異物
			CASE 1
				PRINT 缶を
			CASE 2
				PRINT ペットボトルを
			CASE 3
				PRINT 瓶を
		ENDSELECT
	CASE GETNUM(ABL, "Ａ感覚")
		PRINT アナルに
		SELECTCASE TEQUIP:Ａ異物
			CASE 1
				PRINT 缶を
			CASE 2
				PRINT ペットボトルを
			CASE 3
				PRINT 瓶を
		ENDSELECT
ENDSELECT
PRINTW ねじ込んだ……

SELECTCASE TFLAG:異物挿入
	CASE GETNUM(ABL, "Ｖ感覚")
		SELECTCASE TEQUIP:Ｖ異物
			CASE 1
				IF ABL:Ｖ感覚 <= 4
					PRINTFORMW %CALLNAME:TARGET%は非常識なほど太い異物の苦痛に悶えている……
				ELSE
					PRINTFORMW %CALLNAME:TARGET%の膣は水音を立てながら缶を飲み込み、ひんやりとした感触に震えている……
				ENDIF
			CASE 2
				IF ABL:Ｖ感覚 <= 4
					PRINTFORMW 薄汚れた不潔なペットボトルを挿入され、%CALLNAME:TARGET%は痛みのあまりに悲鳴を上げている……
				ELSE
					PRINTFORMW 透明なボトルからぽっかりと開いた膣内が露わになり、ボトルの中には少しずつ液が溜まっていった……
				ENDIF
			CASE 3
				IF ABL:Ｖ感覚 <= 4
					PRINTFORMW %CALLNAME:TARGET%の中に半ば無理やり一升瓶をねじ込むと、あまりの苦痛に%CALLNAME:TARGET%は声にならない悲鳴を上げた……
				ELSE
					PRINTFORMW 一升瓶すらも膣で飲み込んでしまった%CALLNAME:TARGET%は、苦痛と不安が混じったような表情をしている……
				ENDIF
		ENDSELECT
	CASE GETNUM(ABL, "Ａ感覚")
		SELECTCASE TEQUIP:Ａ異物
			CASE 1
				IF ABL:Ａ感覚 <= 4
					PRINTFORMW %CALLNAME:TARGET%は非常識なほど太い異物の苦痛に悶えている……
				ELSE
					PRINTFORMW %CALLNAME:TARGET%の膣は水音を立てながら缶を飲み込み、ひんやりとした感触に震えている……
				ENDIF
			CASE 2
				IF ABL:Ａ感覚 <= 4
					PRINTFORMW 薄汚れた不潔なペットボトルを挿入され、%CALLNAME:TARGET%は痛みのあまりに悲鳴を上げている……
				ELSE
					PRINTFORMW 透明なボトルからぽっかりと開いた腸内が露わになり、ボトルの中には少しずつ腸液が溜まっていった……
				ENDIF
			CASE 3
				IF ABL:Ａ感覚 <= 4
					PRINTFORMW %CALLNAME:TARGET%の中に半ば無理やり一升瓶をねじ込むと、あまりの苦痛に%CALLNAME:TARGET%は声にならない悲鳴を上げた……
				ELSE
					PRINTFORMW 一升瓶すらも肛門で飲み込んでしまった%CALLNAME:TARGET%は、苦痛と不安が混じったような表情をしている……
				ENDIF
		ENDSELECT
ENDSELECT

