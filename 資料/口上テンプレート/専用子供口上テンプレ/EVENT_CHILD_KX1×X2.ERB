@CHILD_START_KX1×X2
;-------------------------------------------------
;調教開始時 FEVE:0
;-------------------------------------------------
DRAWLINE
;初調教時
IF FEVE:0 == 0
	PRINTFORMW 
	FEVE:0 = 1
;陥落済
ELSEIF CFLAG:陥落
	;陥落後、初の調教
	IF FEVE:0 <= 1
		PRINTFORMW 
		FEVE:0 = 2
	;2回目以降
	ELSE
		PRINTFORMW 
	ENDIF
;未陥落
ELSE
	PRINTFORMW 
ENDIF

RETURN 0


@CHILD_TURNEND_KX1×X2
DRAWLINE
;通常終了時
IF BASE:体力 >= 500
	;陥落済み
	IF CFLAG:陥落
		PRINTFORMW 
	;その他
	ELSE
		PRINTFORMW 
	ENDIF
;体力500未満での強制終了時
ELSEIF BASE:体力 < 500
	;陥落済み
	IF CFLAG:陥落
		PRINTFORMW 
	;その他
	ELSE
		PRINTFORMW 
	ENDIF
ENDIF

RETURN 0


