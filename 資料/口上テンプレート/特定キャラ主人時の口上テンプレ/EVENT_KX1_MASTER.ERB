		;脱がした時
		ELSE
			PRINTFORMW 
		ENDIF
	;下半身上着を
	CASE 2
		;着せた時
		IF TEQUIP:特殊
			PRINTFORMW 
		;脱がした時
		ELSE
			PRINTFORMW 
		ENDIF
	;全身上着を
	CASE 3
		;着せた時
		IF TEQUIP:特殊
			PRINTFORMW 
		;脱がした時
		ELSE
			PRINTFORMW 
		ENDIF
	;上半身下着を
	CASE 4
		;着せた時
		IF TEQUIP:特殊
			PRINTFORMW 
		;脱がした時
		ELSE
			PRINTFORMW 
		ENDIF
	;下半身下着を
	CASE 5
		;着せた時
		IF TEQUIP:特殊
			PRINTFORMW 
		;脱がした時
		ELSE
			PRINTFORMW 
		ENDIF
	;足元枠を
